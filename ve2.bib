@article{ ISI:000298515700003,
Author = {Zaraca, F. and Ponzoni, A. and Stringari, C. and Ebner, J. A. and
   Giovannetti, R. and Ebner, H.},
Title = {{Lower extremity traumatic vascular injury at a level II trauma center:
   an analysis of limb loss risk factors and outcomes}},
Journal = {{MINERVA CHIRURGICA}},
Year = {{2011}},
Volume = {{66}},
Number = {{5}},
Pages = {{397-407}},
Month = {{OCT}},
Abstract = {{Aim. The objectives were to review in our series the risk factors,
   management and outcomes of patients who sustained vascular injuries in
   the lower limbs and to determine the effect of risk factors and
   treatment on the outcome of the injured extremity.
   Methods. Fifty-six patients submitted to surgical treatment were
   retrospectively reviewed. Results were analysed in terms of type of
   operation and reconstruction, intraoperative and 30 day complications,
   reconstruction occlusion, major amputation and mortality.
   Results. The mechanism of trauma was blunt in 30.4\% and penetrating in
   69.6\%. The overall primary amputation rate was 5.4\%, the overall
   secondary amputation rate was 1.8\%. The overall intraoperative and
   postoperative mortality were 1.8\% and 5.4\% respectively. At univariate
   analys is, the presence of compartment syndrome and ischemia time >6
   hours were associated with a significantly higher risk of early
   reconstruction thrombosis (both P=0.03). It showed also that the number
   of patent vessels (P=0.0000) and the presence of a MESS score >7
   (P=0.0000) significantly affected primary amputation, and that the
   occurrence of postoperative deep wound infection or sepsis (P=0.0000),
   of tibio-peroneal trunk injury (P=0.003) and of a MESS score >7
   (P=0.004) significantly affected secondary amputation.
   Conclusion. The number of patent arteries (0-1), the presence of a MESS
   score >7, the incidence of tibio-peroneal trunk injury and the
   occurrence of postoperative deep wound infection are significant
   independent factors for limb loss. The presence of compartment syndrome
   and of ischemia time >6 hours are associated with a significantly higher
   risk of early reconstruction thrombosis.}},
ISSN = {{0026-4733}},
ORCID-Numbers = {{Ponzoni, Andrea/0000-0001-8037-2557}},
Unique-ID = {{ISI:000298515700003}},
}

@article{ ISI:000295592900021,
Author = {Van Hoorebeke, S. and Van Immerseel, F. and Berge, A. C. and Persoons,
   D. and Schulz, J. and Hartung, J. and Harisberger, M. and Regula, G. and
   Barco, L. and Ricci, A. and De Vylder, J. and Ducatelle, R. and
   Haesebrouck, F. and Dewulf, J.},
Title = {{Antimicrobial resistance of Escherichia coli and Enterococcus faecalis
   in housed laying-hen flocks in Europe}},
Journal = {{EPIDEMIOLOGY AND INFECTION}},
Year = {{2011}},
Volume = {{139}},
Number = {{10, SI}},
Pages = {{1610-1620}},
Month = {{OCT}},
Abstract = {{The aim of this study was to determine the potential association between
   housing type and multiple drug resistance (MDR) in Escherichia coli and
   Enterococcus faecalis isolates recovered from 283 laying-hen flocks. In
   each flock, a cloacal swab from four hens was collected and produced
   1102 E. coli and 792 E. faecalis isolates. Broth microdilution was used
   to test susceptibility to antimicrobials. Country and housing type
   interacted differently with the MDR levels of both species. In the E.
   coli model, housing in a raised-floor system was associated with an
   increased risk of MDR compared to the conventional battery system {[}
   odds ratio (OR) 2.12, 95\% confidence interval (CI) 1.13-3.97)]. In the
   E. faecalis model the MDR levels were lower in free-range systems than
   in conventional battery cages (OR 0.51, 95\% CI 0.27-0.94). In Belgium,
   ceftiofur-resistant E. coli isolates were more numerous than in the
   other countries.}},
