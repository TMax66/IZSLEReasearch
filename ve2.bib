@article{ ISI:000266734200012,
Author = {Busani, Luca and Valsecchi, Maria Grazia and Rossi, Emanuela and Toson,
   Marica and Ferre, Nicola and Pozza, Manuela Dalla and Marangon, Stefano},
Title = {{Risk factors for highly pathogenic H7N1 avian influenza virus infection
   in poultry during the 1999-2000 epidemic in Italy}},
Journal = {{VETERINARY JOURNAL}},
Year = {{2009}},
Volume = {{181}},
Number = {{2}},
Pages = {{171-177}},
Month = {{AUG}},
Abstract = {{In 1999-2000, Italian poultry production was disrupted by an H7N1 virus
   subtype epidemic of highly pathogenic avian influenza (HPAI). The
   objectives of the present study were to identify risk factors for
   infection on poultry farms located in regions that had the highest
   number of outbreaks (Veneto and Lombardia) and the impact of pre-emptive
   culling as a complementary measure for eradicating infection. A Cox
   regression model that included spatial factors, such as the G index, was
   used. The results confirmed the relationship between risk of infection
   and poultry species, production type and size of farms. The
   effectiveness of pre-emptive culling was confirmed. An increased risk of
   infection was observed for poultry farms located near an infected farm
   and those at altitudes less than 150 in above sea level. The measures
   for the control and eradication of AI virus infection need to consider
   species differences in susceptibility, the types of production and the
   density of poultry farms in the affected areas. (c) 2008 Elsevier Ltd.
   All rights reserved.}},
DOI = {{10.1016/j.tvjl.2008.02.013}},
ISSN = {{1090-0233}},
