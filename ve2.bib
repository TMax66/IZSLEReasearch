@article{ ISI:000290106100002,
Author = {Busani, L. and Capelli, G. and Cecchinato, M. and Lorenzetto, M. and
   Savini, G. and Terregino, C. and Vio, P. and Bonfanti, L. and Dalla
   Pozza, M. and Marangon, S.},
Title = {{West Nile virus circulation in Veneto region in 2008-2009}},
Journal = {{EPIDEMIOLOGY AND INFECTION}},
Year = {{2011}},
Volume = {{139}},
Number = {{6}},
Pages = {{818-825}},
Month = {{JUN}},
Abstract = {{West Nile virus (WNV) was detected in Italy, in late summer 2008 in
   horses and birds in the Po valley. As a consequence, an intense WNV
   surveillance was implemented in that area involving Emilia-Romagna,
   Veneto and Lombardy. This paper presents the results of the September
   2008-November 2009 surveillance on equines, mosquitoes, wild birds, dogs
   and cattle in Veneto. WNV was detected in equines and dogs, and, to a
   lesser extent in cattle and wild birds. Simultaneous circulation of
   Usutu virus was detected by testing wild birds found dead. Usutu virus
   but not WNV was also found in mosquitoes monitored during 2009. Equine
   practices monitoring allowed the definition of an area of WNV
   circulation and the 2008-2009 westward and northward spread of the
   infection. Although a relatively low number of human cases and a low
   virus circulation in vectors and birds detected in Veneto region could
   be considered favourable conditions for a limited risk of human
   exposure, it remains difficult to predict the possible evolution of the
   epidemiological situation.}},
DOI = {{10.1017/S0950268810001871}},
