@article{ ISI:000290696500008,
Author = {Monne, Isabella and Fusaro, Alice and Valastro, Viviana and Citterio,
   Carlo and Pozza, Manuela Dalla and Obber, Federica and Trevisiol, Karin
   and Cova, Mariapia and De Benedictis, Paola and Bregoli, Marco and
   Capua, Ilaria and Cattoli, Giovanni},
Title = {{A distinct CDV genotype causing a major epidemic in Alpine wildlife}},
Journal = {{VETERINARY MICROBIOLOGY}},
Year = {{2011}},
Volume = {{150}},
Number = {{1-2}},
Pages = {{63-69}},
Month = {{MAY 12}},
Abstract = {{Canine distemper virus (CDV) infection represents an important
   conservation threat to many carnivore species and has contributed to the
   population decline of several wild terrestrial and aquatic mammalian
   species. Since 2006, the Alpine region of North-Eastern (NE) Italy has
   been experiencing a severe and widespread outbreak of CDV affecting the
   wild carnivore population. In this study we performed an extensive
   phylogenetic and molecular evolutionary analysis of CDV identified
   during the recent wildlife epidemic in the Alpine region. Our analysis
   yielded data on the evolutionary dynamics of the Alpine wildlife CDV
   epidemic and revealed the emergence and spread of a single genetic
   cluster of CDV. The wide distribution of the novel cluster combined with
   the identification of a specific amino acid mutation, which is believed
   to increase the ability of the virus to replicate in a wider host range,
   raises concerns over the possible implications of the spread of this
   virus on the conservation of endangered wildlife species. (C) 2011
   Elsevier B.V. All rights reserved.}},
DOI = {{10.1016/j.vetmic.2011.01.009}},
