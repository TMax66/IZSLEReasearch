@ARTICLE{Arena201772,
author={Arena, L. and Wemelsfelder, F. and Messori, S. and Ferri, N. and Barnard, S.},
title={Application of Free Choice Profiling to assess the emotional state of dogs housed in shelter environments},
journal={Applied Animal Behaviour Science},
year={2017},
volume={195},
