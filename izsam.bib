@ARTICLE{Monaco201973,
author={Monaco, F. and Purpari, G. and Di Gennaro, A. and Mira, F. and Di Marco, P. and Guercio, A. and Savini, G.},
title={Immunological response in horses following West Nile virus vaccination with inactivated or recombinant vaccine},
journal={Veterinaria italiana},
