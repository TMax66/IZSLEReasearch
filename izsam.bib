@ARTICLE{Mughini-Gras2014153,
author={Mughini-Gras, L. and Patregnani, T. and Nardelli, S. and Gagliazzo, L. and Comin, A. and Savini, G. and Marangon, S. and Bonfanti, L.},
title={Long-term persistence of vaccine-elicited bluetongue serotype 8 neutralizing antibodies in dairy cattle},
journal={Large Animal Review},
