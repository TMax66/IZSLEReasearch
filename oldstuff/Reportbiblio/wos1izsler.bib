@article{ ISI:000326891700004,
Author = {Foni, Emanuela and Garbarino, Chiara and Chiapponi, Chiara and Baioni,
   Laura and Zanni, Irene and Cordioli, Paolo},
Title = {{Epidemiological survey of swine influenza A virus in the wild boar
   population of two Italian provinces}},
