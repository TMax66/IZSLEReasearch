@article{ ISI:000418228100009,
Author = {Bertelloni, Fabrizio and Tosi, Giovanni and Massi, Paola and Fiorentini,
   Laura and Parigi, Maria and Cerri, Domenico and Ebani, Valentina
   Virginia},
Title = {{Some pathogenic characters of paratyphoid Salmonella enterica strains
   isolated from poultry}},
