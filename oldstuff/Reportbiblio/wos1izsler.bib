@article{ ISI:000410858900002,
Author = {Foni, Emanuela and Chiapponi, Chiara and Baioni, Laura and Zanni, Irene
   and Merenda, Marianna and Rosignoli, Carlo and Kyriakis, Constantinos S.
   and Luini, Mario Vittorio and Mandola, Maria Lucia and Bolzoni, Luca and
