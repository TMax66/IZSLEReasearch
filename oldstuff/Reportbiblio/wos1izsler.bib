@article{ ISI:000397897000001,
Author = {Marcoccia, Daniele and Pellegrini, Marco and Fiocchetti, Marco and
   Lorenzetti, Stefano and Marino, Maria},
Title = {{Food components and contaminants as (anti)androgenic molecules}},
Journal = {{GENES AND NUTRITION}},
