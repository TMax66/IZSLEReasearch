@ARTICLE{Maisano201980,
author={Maisano, A.M. and Luini, M. and Lorenzi, V. and Bolzoni, L. and Romanò, A. and Spelta, C. and Angelucci, A. and Fusi, F. and Ferrara, G. and Vezzoli, F. and Bertocchi, L.},
