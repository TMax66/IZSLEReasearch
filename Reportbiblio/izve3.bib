@article{ ISI:000074868700005,
Author = {Mioni, R and Grimaldi, M and Bordin, P and Miglioranzi, F and Ferrigno,
   R},
Title = {{Listeria monocytogenes detection in food: Evaluation of a selective agar
   medium species-specific and of a rapid identification kit}},
Journal = {{INDUSTRIE ALIMENTARI}},
Year = {{1998}},
Volume = {{37}},
Number = {{371}},
Pages = {{732-737}},
Month = {{JUN}},
Abstract = {{A comparative study between selective agar media now employed for
   Listeria monocytogenes detection and a new culture agar medium, ALOA,
   which is differential for the pathogenic species, has been conducted on
   164 samples of animal origin food.
   The high performance of this medium has been demonstrated by a +14.7\%
   of positive samples found, compared to the results obtained with Oxford,
   due to the characteristic morphology of Listeria monocytogenes colonies
   in ALOA. The presence of an opaque halo surrounding the colonies
   (phosphatidylinositol-specific phospholipase C production) allows to
   individuate presumed Listeria monocytogenes colonies also in a mixed
   population. No case of False positive identification for Listeria
   monocytogenes has been found by the biochemical identification of the
   colonies surrounded by an opaque halo; the only case of False positive
   reported is due by Enterococcus faecalis for Listeria spp. (colonies
   without halo). The use of the arilamidase test in paper disks
   (Monocytogenes ID Discs) instead of miniaturised multitest kits, has
   demonstrated to be valid enough to confirm Listeria monocytogenes
   presence from the other species, in a simple and quicker way.}},
ISSN = {{0019-901X}},
Unique-ID = {{ISI:000074868700005}},
}

