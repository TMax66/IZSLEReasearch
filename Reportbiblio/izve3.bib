@article{ ISI:000188168700015,
Author = {Montesissa, C and Capolongo, F and Santi, A and Biancotto, G and
   Dacasto, M},
Title = {{Metabolism of tilmicosin by rabbit liver microsomes and hepatocytes}},
Journal = {{VETERINARY JOURNAL}},
Year = {{2004}},
Volume = {{167}},
Number = {{1}},
Pages = {{87-94}},
Month = {{JAN}},
Abstract = {{We investigated tilmicosin (TIM) metabolism, at 25, 50 or 100 muM, in
   cultures of primary hepatocytes from rabbits bred commercially for food
   and in liver microsomes prepared from both untreated and rifampicin
   (RIF)-treated rabbits. RIF is a well-known cytochrome P4503A (CYP 3A)
   inducer in rabbits and most macrolides are known to be substrates of CYP
   3A.
   No peaks in addition to those of the cis and tracts forms of TIM were
   observed by high performance liquid chromatography (HPLC) in extracts of
   microsomes from untreated rabbits. When TIM was incubated with induced
   microsomes, at least two peaks were found by HPLC and an additional
   peak, eluting at shorter retention time was isolated from hepatocytes
   incubated for 24 h with the macrolide.
   The structures of the metabolites were then estimated by liquid
   chromatography-mass spectrometry (LC-MS) in concentrated extracts from
   induced microsomes. Five metabolites were separated and putatively
   identified: cis and trans demethylated tilmicosin, tilmicosin N-oxide
   and cis and trans tilmicosin epoxide. The overall amount of metabolites
   produced in vitro using livers of untreated and RIF treated rabbits was
   very low, has also been observed in vivo and in vitro in cattle,
   chickens and pigs. (C) 2003 Elsevier Ltd. All rights reserved.}},
DOI = {{10.1016/S1090-0233(03)00077-7}},
ISSN = {{1090-0233}},
ORCID-Numbers = {{Biancotto, Giancarlo/0000-0002-7679-6549}},
Unique-ID = {{ISI:000188168700015}},
}

