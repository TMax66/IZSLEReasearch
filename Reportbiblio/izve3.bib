@article{ ISI:000260835200018,
Author = {Doszpoly, Andor and Kovacs, Endre R. and Bovo, Giuseppe and LaPatra,
   Scott E. and Harrach, Balazs and Benko, Maria},
Title = {{Molecular confirmation of a new herpesvirus from catfish (Ameiurus
   melas) by testing the performance of a novel PCR method, designed to
   target the DNA polymerase gene of alloherpesviruses}},
Journal = {{ARCHIVES OF VIROLOGY}},
Year = {{2008}},
Volume = {{153}},
Number = {{11}},
Pages = {{2123-2127}},
Month = {{NOV}},
Abstract = {{A PCR method with consensus degenerate primers was developed for the
   detection of herpesviruses (HVs) of anamnia. Compared to previously
   published PCRs, targeting the DNA polymerase gene of fish HVs, the size
   of PCR products was more than tripled. Although broad applicability of
   the method could not be proven, approximately 1,600-bp fragments from
   HVs of white sturgeon ( Acipenser transmontanus) and black bullhead (
   Ameiurus melas) were obtained and sequenced. Phylogenetic tree
   reconstructions showed both HVs to be monophyletic with the single
   member (ictalurid HV-1) of the genus Ictalurivirus in the new family
   Alloherpesviridae.}},
DOI = {{10.1007/s00705-008-0230-7}},
ISSN = {{0304-8608}},
