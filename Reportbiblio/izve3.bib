}

@article{ ISI:A1996VB29900006,
Author = {Martini, M and Capelli, G and Poglayen, G and Bertotti, F and Turilli, C},
Title = {{The validity of some haematological and ELISA methods for the diagnosis
   of canine heartworm disease}},
Journal = {{VETERINARY RESEARCH COMMUNICATIONS}},
Year = {{1996}},
Volume = {{20}},
Number = {{4}},
Pages = {{331-339}},
Month = {{AUG}},
Abstract = {{Examinations for heartworm (Dirofilaria immitis) were performed on 175
   impounded dogs from a hyperendemic area of the Po Valley (Italy). Each
   blood sample was used with five haematological diagnostic methods
   (filtration, direct smear, modified Knott, clotted blood and capillary
   tube) and three commercial ELISA kits (PetChek, Diasystems, Uni-Tec).
   The results were compared with the true infection status obtained from
   post-mortem examination of the heart, pulmonary arteries, thoracic venae
   cavae and lungs. The prevalence of the infection by adult worms at
   necropsy was 63\%. The sensitivity of the rests ranged from 60\%
   (capillary tube) to 81\% (Diasystems) and the specificity from 88\%
   (filtration) to 98\% (PetChek). The results of all the tests differed
   significantly (p<0.01) from those obtained at necropsy. The sensitivity
   of the tests was also assessed with respect to the differing numbers of
   worms in the hosts. A positive correlation between the worm burden and
   the sensitivity was observed in all the tests. It is apparent that the
   ELISA methods were better able to detect cases with a low number of
   worms than the haematological tests.}},
DOI = {{10.1007/BF00366539}},
ISSN = {{0165-7380}},
ORCID-Numbers = {{martini, marco/0000-0002-9252-6389}},
Unique-ID = {{ISI:A1996VB29900006}},
}

@article{ ISI:A1996VE86200005,
Author = {Mioni, R and Grimaldi, M and Zorzi, P and Mani, A and Bordini, P},
Title = {{Comparison between selective media `'rapid E-coli'' and `'VRBA with
   sodium malonate'' for E-coli investigation}},
Journal = {{INDUSTRIE ALIMENTARI}},
Year = {{1996}},
Volume = {{35}},
Number = {{350}},
Pages = {{793-796}},
Month = {{JUL-AUG}},
Abstract = {{A comparative study between two different coltural media, `'Rapid' E.
   coli'' (Sanofi Diagnostics Pasteur) and `'V.R.B.A. (Oxoid) with sodium
   malonate'', was carried out in order to evaluate their selective power
   for E. coli isolation in animal foods.
   The study was performed on 144 samples and showed that `'Rapid' E.
   coli'' medium is much more efficient than `'V.R.B.A. with sodium
   malonate'' for the following reasons: E. coli are easier to identify and
   their count is simpler and more reliable; the enzymatic reaction
   (beta-glucuronidase activity) is highly specific for E. coli, so that
   the number of false positive is strongly reduced; `'Rapid' E. coli''
   medium strongly inhibits Gram positive and negative bacteria
   contaminating food.}},
ISSN = {{0019-901X}},
Unique-ID = {{ISI:A1996VE86200005}},
}

