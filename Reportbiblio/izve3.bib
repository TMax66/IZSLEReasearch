}

@article{ ISI:000248276300343,
Author = {Ricci, R. and Gottardo, F. and Ferlito, J. C. and Stefani, A. and
   Ravarotto, L. and Andrighetto, I.},
Title = {{Body condition score (BCS) and metabolic status of shelter dogs}},
Journal = {{ITALIAN JOURNAL OF ANIMAL SCIENCE}},
Year = {{2007}},
Volume = {{6}},
Number = {{1}},
Pages = {{859-861}},
Note = {{17th Congress of the Scientific-Association-of-Animal-Production,
   Alghero, ITALY, MAY 29-JUN 01, 2007}},
Organization = {{Univ Sassari, Dipartimento Sci Zootecn; Sci Assoc Anim Prod}},
Abstract = {{A group of 147 shelter dogs were weighted and assigned a body condition
   score (BCS) using a 9 point scale system, in order to evaluate the
   prevalence of obesity in the kennel. More than 60\% of the animals
   showed a BCS :6 (overweight and obese) and this condition was mainly
   attributed to an excess of carbohydrates and fat in the diet. In 67/147
   dogs, a blood sample was drawn and the effects of BCS, age and time
   spent in the shelter were evaluated on biochemical parameters. Obese
   dogs showed significantly higher levels of triglycerides (P<0.01), while
   increasing BCS determined only an increasing non significant trend on
   cholesterol values. Age influenced creatinine (P<0.05) and the oldest
   dogs scoring BC >= 6 registered significant higher NEFA (P<0.05) and CK
   (P=0.01) levels. Time spent in the shelter did not affect any parameter.
   The dogs' metabolic condition reflects the need of taking more care of
   the quality of feed administered in the shelters to avoid the negative
   health effects caused by chronic obesity.}},
DOI = {{10.4081/ijas.2007.1s.859}},
ISSN = {{1594-4077}},
Unique-ID = {{ISI:000248276300343}},
}

