@article{ ISI:000077984100013,
Author = {Cavirani, S and Donofrio, G and Chiocco, D and Foni, E and Martelli, P
   and Allegri, G and Cabassi, CS and De Iaco, B and Flammini, CF},
Title = {{Seroprevalence to bovine immunodeficiency virus and lack of association
   with leukocyte counts in Italian dairy cattle}},
Journal = {{PREVENTIVE VETERINARY MEDICINE}},
Year = {{1998}},
Volume = {{37}},
Number = {{1-4}},
Pages = {{147-157}},
Month = {{DEC 1}},
Abstract = {{We report herein on the first serological detection of antibodies to
   bovine immunodeficiency virus (BIV) in Italy. According to criteria of a
   stratified-random sampling of dairy cattle reared in the Parma area (a
   province in the Po Valley, Northern Italy), sera from 3166 cows
   belonging to 272 herds were collected. In addition, sera of 138 bulls
   from eight artificial-insemination (AI) centres were sampled.
   Seventy-eight cows (2.5\%) from 16 herds (5.8\%) and seven bulls (5.1\%)
   from two AI centres were positive for BIV-R29 antibodies in the
   IFA-test. IFA-positive, sera assayed by Western blot had reaction to
   different viral proteins: 81 out of 85 sera showed antibody to p26
   (considered the BIV major internal core protein); four sera reacted to
   other viral proteins but not to p26. Peripheral blood leukocytes of 60
   seropositive and 60 seronegative animals, belonging to eight
   BIV-infected herds, were enumerated to assess any effect of BIV
   infection on white-blood cells. No significant differences were detected
   between the two groups. These data indicate that BIV infection is
   present in Italian dairy cattle - but the role of BIV in inducing
   disease remains unclear. (C) 1998 Elsevier Science B.V. All rights
   reserved.}},
