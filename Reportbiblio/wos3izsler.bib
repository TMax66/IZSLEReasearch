@article{ ISI:000079027700009,
Author = {Niedbalski, W and Kesy, A and Brocchi, E and Borrego, B and Malicki, K},
Title = {{Antigenic characterization of Polish SVDV isolates}},
Journal = {{MEDYCYNA WETERYNARYJNA}},
Year = {{1999}},
Volume = {{55}},
Number = {{3}},
Pages = {{181-184}},
Abstract = {{The aim of this study was the antigenic analysis of Polish SVDV isolates
   from 1972-73. Experiments were performed with three panels of monoclonal
   antibodies (Mabs) raised against Italian SVDV It 73, It 91 and It 92 in
   a trapping ELISA. The results obtained were compared with the reactivity
   patterns of the four Italian viruses used as representatives of each of
   the four antigenic groups (Brocchi et al., Epidemiol. Infect. 118, 51,
   1997). It was found that the Polish SVDV isolates belong to antigenic
   group 2 (viruses present in Europe and Japan between 1972-1981)
   recognised by antibodies of the first panel (Mabs against It 72). It
   suggest the close relation and common origin of Polish isolates and the
   other European ones from the 1970s. The weak reactivity of Mab 1B3 (both
   of panel It 92) confirmed the antigenic differenties of Polish SVDV from
   viruses isolated from outbreaks of SVD occured in Italy in 1991-1992.
   These results can be useful for the epidemiological studies of SVD in
   Europe.}},
