@article{ ISI:000085427200003,
Author = {Bolzoni, G and Marcolini, A and Varisco, G},
Title = {{Evaluation of the Bactoscan FC. 1. Accuracy, comparison with the
   Bactoscan 8000 and somatic cells effect}},
Journal = {{MILCHWISSENSCHAFT-MILK SCIENCE INTERNATIONAL}},
Year = {{2000}},
Volume = {{55}},
Number = {{2}},
Pages = {{67-70}},
Abstract = {{In order to evaluate the use of the Bactoscan FC in comparison to the
   Bactoscan 8000 and its relationship to the standard plate count (FIL n
   degrees 100B/1991), some tests have been carried out. The Bactoscan FC
   and the reference method are related by a linear regression (y = 1,0317
   x - 0,9822) as is the BFC to the B8000 with a linear regression, similar
   to the one furnished by Fosselectric, allowing the results of the 2
   instruments to be equilibrated. In our experience it is possible to
   conclude that the BFC counts approximately 5\% more than the B8000.
   Considering the use of cytometric flow cell technology and the presence
   of a dye such as Etidium Bromide, the effect of somatic cells on
   bacterial count has been investigated. The results were statistically
   significant but with little relevance. In our experience, the BFC
   results could be conditioned only by very high cell values (2 million
   cell/ml) which are seldom observed in bulk milk samples. The relevance
   of this effect could be neglected if evaluated with respect to other
   typical variation factors involved in the bacterial contamination in raw
   milk.
   Where possible, data are evaluated in a similar way to other authors to
   allow confrontation with their results.}},
