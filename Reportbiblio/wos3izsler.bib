@article{ ISI:000076965400015,
Author = {Manganelli, R and Provvedi, R and Berneri, C and Oggioni, MR and Pozzi,
   G},
Title = {{Insertion vectors for construction of recombinant conjugative
   transposons in Bacillus subtilis and Enterococcus faecalis}},
Journal = {{FEMS MICROBIOLOGY LETTERS}},
Year = {{1998}},
Volume = {{168}},
Number = {{2}},
Pages = {{259-268}},
Month = {{NOV 15}},
Abstract = {{The broad-host range of conjugal transfer and the chromosomal location
   make conjugative transposons (CT) attractive candidates as tools for
   genetic manipulation of a large variety of bacteria. In this paper we
   describe insertion vectors capable of integrating into Tn916, the
   prototype of CT in Gram-positive bacteria. The integration of vectors
   into a single chromosomal copy of Tn916 was studied both after natural
   transformation of Bacillus subtilis, and after electroporation in
   Enterococcus faecalis. Integration occurred either by double or by
   single crossover, and the integrated DNA segment was shown to be highly
   stable. All recombinant CT (rCT) were still able to excise from the
   chromosome to form circular intermediates, the first step of both
   transposition and conjugal transfer. All classes of rCT generated by
   insertion vector pSMB47 were capable of conjugal transfer, while using
   pVMB11 it was possible to generate non-conjugative rCT. (C) 1998
   Federation of European Microbiological Societies. Published by Elsevier
   Science B.V. All rights reserved.}},
