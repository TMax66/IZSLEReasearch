@article{ ISI:000279535800025,
Author = {Colmegna, Silvia and Invernizzi, Anna and Mascher, Anna Lia and Corsale,
   Emanuela and Ferrazzi, Viviana and Grilli, Guido},
Title = {{Microbiological characteristics of poultry meats - Results of
   inspections carried out in the province of Milano, Italy}},
Journal = {{ITALIAN JOURNAL OF ANIMAL SCIENCE}},
Year = {{2009}},
Volume = {{8}},
Number = {{4}},
