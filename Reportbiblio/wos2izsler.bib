Title = {{Expression of a truncated hepatitis E virus capsid protein in the
   protozoan organism Leishmania tarentolae and its application in a
   serological assay}},
Journal = {{JOURNAL OF VIROLOGICAL METHODS}},
Year = {{2013}},
Volume = {{193}},
Number = {{1}},
