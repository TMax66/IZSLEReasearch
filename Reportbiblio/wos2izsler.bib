@article{ ISI:000245147700016,
Author = {Croci, Luciana and Losio, Marina Nadia and Suffredini, Elisabetta and
   Pavoni, Enrico and Di Pasquale, Simona and Fallacara, Francesca and
   Arcangeli, Giuseppe},
Title = {{Assessment of human enteric viruses in shellfish from the northern
   Adriatic sea}},
Journal = {{INTERNATIONAL JOURNAL OF FOOD MICROBIOLOGY}},
Year = {{2007}},
Volume = {{114}},
Number = {{2}},
Pages = {{252-257}},
Month = {{MAR 10}},
Abstract = {{Incidence and circulation of different strains of hepatitis A and
   Norovirus in shellfish were studied on 235 samples (Tapes philippinarum,
   Mytilus galloprovincialis, Ostrea spp. and Chlamys spp.) obtained from
   different sites, representing the shellfish production areas of the
   northern Adriatic sea. Shellfish were harvested in the period of one
   year and, after deputation, were examined for bacterial (Escherichia
   coli and Salmonella) and viral (HAV and NoV) contamination. Viral
   contamination was present on average in 22\% of samples: specifically,
   6\%, of samples tested positive for HAV, 14\% for NoV and 2\% for both
   viruses. None of the samples revealed the presence of Salmonella, and in
   most of them (93\%) the number of E. coli was below the European
   legislation limit of 230 MPN/100 g. T philippinarum was the species most
   often contaminated, as well as being the only species in which the legal
   limit for E. coli was, in some cases, exceeded. Both HAV and NoV
   contamination were detected throughout the year; NoV detection was
   slightly more frequent during winter months, but positive samples were
   also present in summer. The sequencing of the PCR products showed the
   circulation of only one HAV genotype (IA) and four different NoV
   genotypes (Hawaii, Melksham, Lordsdale and GGIIb) with a prevalence of
   the GGIIb genotype in the second period of the monitoring. Published by
   Elsevier B.V.}},
