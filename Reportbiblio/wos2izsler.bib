@article{ ISI:000264947100008,
Author = {Dusi, Guglielmo and Bozzoni, Eros and Assini, Walter and Tognoli, Nadia
   and Gasparini, Mara and Ferretti, Enrica},
Title = {{Confirmatory method for the determination of resorcylic acid lactones in
   urine sample using immunoaffinity cleanup and liquid
   chromatography-tandem mass spectrometry}},
