@article{ ISI:000315074900037,
Author = {Moreno, Ana and Lelli, Davide and Brocchi, Emiliana and Sozzi, Enrica
   and Vinco, Leonardo James and Grilli, Guido and Cordioli, Paolo},
Title = {{Monoclonal antibody-based ELISA for detection of antibodies against H5
   avian influenza viruses}},
