@article{ ISI:000287894200004,
Author = {Marenzoni, Maria Luisa and Lepri, Elvio and Coppola, Giacomo and Sforna,
   Monica and Sordini, Chiara and Tiburzi, Alessandra and Valentini, Silvia
   and Vitellozzi, Giovanni and Coletti, Mauro and Tagliabue, Silvia and
   Passamonti, Fabrizio},
Title = {{Two cases of equine pregnancy loss associated with Leptospira
   interrogans sensu lato}},
Journal = {{IPPOLOGIA}},
Year = {{2010}},
Volume = {{21}},
Number = {{4}},
Pages = {{23-29}},
Month = {{DEC}},
Abstract = {{Leptospira (Leptospira interrogans sensu lato) infection was diagnosed
   as the cause of an abortion at 8 months and a perinatal death in two
   horses coming from different stud farms. In this study the most
   consistent gross and microscopic lesions were reported. Diagnoses were
   based on polymerase chain reaction (PCR) on fetal/neonatal organs and
   placenta. Microscopic agglutination was useful to diagnose the infection
   only in a single case. The limits of every diagnostic test were
   discussed.}},
Publisher = {{EDITORE SCIVAC}},
Address = {{PALAZZO TRECCHI, 26100 CREMONA, ITALY}},
