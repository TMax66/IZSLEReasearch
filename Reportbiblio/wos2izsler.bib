@article{ ISI:000247994200031,
Author = {Donati, Manuela and Di Francesco, Antonietta and Gennaro, Renato and
   Benincasa, Monica and Magnino, Simone and Pignanelli, Salvatore and
   Shurdhi, Alisa and Moroni, Alessandra and Mazzoni, Claudio and Merialdi,
   Giuseppe and Baldelli, Raffaella and Cevenini, Roberto},
Title = {{Sensitivity of Chlamydia suis to cathelicidin peptides}},
Journal = {{VETERINARY MICROBIOLOGY}},
Year = {{2007}},
Volume = {{123}},
Number = {{1-3}},
Pages = {{269-273}},
Month = {{JUL 20}},
Abstract = {{Nine Chlamydia suis isolates, obtained from pigs with conjunctivitis,
   were molecularly characterized by ompA sequencing and their in vitro
   susceptibility to six cathelicidin peptides (SMAP-29, BAC-7, BMAP-27,
   BMAP-27, BMAP-28, PG-1, LL-37) determined in cell culture. SMAP-29 was
   the most active peptide, reducing the intracellular inclusion number by
   >= 50\% at a concentration of 10 mu g/ml (3 mu M) in six of the nine
   isolates tested. Three molecularly identical isolates were insensitive
   at a concentration as high as 80 mu g/ml (25 mu M). Of the remaining
   cathelicidin peptides tested, BAC-7 and BMAP-27 were active against six
   C. suis isolates at a concentration of 80 mu g/ml (25 and 26 mu M,
   respectively). Cathelicidins LL-37 and PG-1 did not show any
   anti-chlamydial activity at 80 mu g/ml. (C) 2007 Elsevier B.V. All
   rights reserved.}},
