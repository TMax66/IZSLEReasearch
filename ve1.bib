@article{ ISI:000378415900004,
Author = {Nunez-Ortiz, N. and Stocchi, V. and Toffan, A. and Pascoli, F. and Sood,
   N. and Buonocore, F. and Picchietti, S. and Papeschi, C. and Taddei, A.
   R. and Thompson, K. D. and Scapigliati, G.},
Title = {{Quantitative immunoenzymatic detection of viral encephalopathy and
   retinopathy virus (betanodavirus) in sea bass Dicentrarchus labrax}},
Journal = {{JOURNAL OF FISH DISEASES}},
Year = {{2016}},
Volume = {{39}},
Number = {{7}},
Pages = {{821-831}},
Month = {{JUL}},
Abstract = {{Viral encephalopathy and retinopathy disease caused by betanodavirus,
   genus of the family Nodaviridae, affects marine, wild and farmed species
   including sea bass, one of the most important farmed species in Europe.
   This work describes a reliable and sensitive indirect ELISA assay to
   detect betanodavirus in biological samples using a polyclonal antiserum
   (pAb 283) against the 283/I09 virus strain, the most common red-spotted
   grouper nervous necrosis virus (RGNNV) genotype in the Mediterranean
   area, and a capture-based ELISA using a monoclonal antibody (mAb 4C3)
   specific to a common epitope present on the capsid protein. Using
   adsorbed, purified VERv preparation, the detection limit of indirect
   ELISA was 2gmL(-1) (3x10(5) TCID50 per mL), whereas for capture-based
   ELISA, the sensitivity for the antigen in solution was 17gmL(-1)
   (35x10(5) TCID50 per mL). The capture-based ELISA was employed to detect
   VERv in brain homogenates of invivo infected sea bass and resulted
   positive in 22 of 32 samples, some of these with a high viral load
   estimates (about 1.1x10(8)TCID50 per mL). The ELISA system we propose
   may be helpful in investigations where coupling of viral content in fish
   tissues with the presence of circulating VERv-specific IgM is required,
   or for use in samples where PCR is difficult to perform.}},
DOI = {{10.1111/jfd.12415}},
ISSN = {{0140-7775}},
EISSN = {{1365-2761}},
