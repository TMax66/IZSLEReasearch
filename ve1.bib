}

@article{ ISI:000372688500005,
Author = {Petrovska, Liljana and Mather, Alison E. and AbuOun, Manal and Branchu,
   Priscilla and Harris, Simon R. and Connor, Thomas and Hopkins, K. L. and
   Underwood, A. and Lettini, Antonia A. and Page, Andrew and Bagnall, Mary
   and Wain, John and Parkhill, Julian and Dougan, Gordon and Davies,
   Robert and Kingsley, Robert A.},
Title = {{Microevolution of Monophasic Salmonella Typhimurium during Epidemic,
   United Kingdom, 2005-2010}},
Journal = {{EMERGING INFECTIOUS DISEASES}},
Year = {{2016}},
Volume = {{22}},
Number = {{4}},
Pages = {{617-624}},
Month = {{APR}},
Abstract = {{Microevolution associated with emergence and expansion of new epidemic
   clones of bacterial pathogens holds the key to epidemiologic success. To
   determine microevolution associated with monophasic Salmonella
   Typhimurium during an epidemic, we performed comparative whole-genome
   sequencing and phylogenomic analysis of isolates from the United Kingdom
   and Italy during 2005-2012. These isolates formed a single Glade
   distinct from recent monophasic epidemic clones previously described
   from North America and Spain. The UK monophasic epidemic clones showed a
   novel genomic island encoding resistance to heavy metals and a composite
   transposon encoding antimicrobial drug resistance genes not present in
   other Salmonella Typhimurium isolates, which may have contributed to
   epidemiologic success. A remarkable amount of genotypic variation
   accumulated during clonal expansion that occurred during the epidemic,
   including multiple independent acquisitions of a novel prophage carrying
   the sopE gene and multiple deletion events affecting the phase II
   flagellin locus. This high level of microevolution may affect
   antigenicity, pathogenicity, and transmission.}},
DOI = {{10.3201/eid2204.150531}},
