@article{ ISI:000363077300006,
Author = {Ruano, Francisco and Batista, Frederico M. and Arcangeli, Giuseppe},
Title = {{Perkinsosis in the clams Ruditapes decussatus and R. philippinarum in
   the Northeastern Atlantic and Mediterranean Sea: A review}},
Journal = {{JOURNAL OF INVERTEBRATE PATHOLOGY}},
Year = {{2015}},
Volume = {{131}},
Pages = {{58-67}},
Month = {{OCT}},
Abstract = {{Perkinsosis is a disease of gastropod and bivalve molluscs caused by
   protozoan parasites of the Perkinsus genus. These parasites have been
   responsible for mass mortalities worldwide, with a significant impact in
   both fisheries and aquaculture, and resulting in severe economic losses.
   This review focuses mainly on current knowledge of diagnostic methods
   and on the distribution of Perkinsus spp. in the Northeastern Atlantic
   and Mediterranean Sea, which infect the grooved carpet shell clam
   Ruditapes decussatus and the Japanese carpet shell clam Ruditapes
   philippinarum. The association between perkinsosis and high mortality
   rates of R. decussatus and R. philippinarum in southern European
   countries such as Portugal and Italy is discussed as is the role of
   environmental factors in those mortality outbreaks. The putative
   introduction of Perkinsus olseni into the Northeastern Atlantic and
   Mediterranean Sea is also discussed, as are management strategies that
   could be used to mitigate the impact of perkinsosis in production of R.
   decussatus and R. philippinarum. (C) 2015 Elsevier Inc. All rights
   reserved.}},
DOI = {{10.1016/j.jip.2015.07.015}},
ISSN = {{0022-2011}},
EISSN = {{1096-0805}},
ResearcherID-Numbers = {{Batista, Frederico/F-5651-2010}},
ORCID-Numbers = {{Batista, Frederico/0000-0002-2848-3011}},
Unique-ID = {{ISI:000363077300006}},
}

