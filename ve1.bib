@article{ ISI:000366078200030,
Author = {Schuhfried, Erna and del Pulgar, Jose Sanchez and Bobba, Marco and Piro,
   Roberto and Cappellin, Luca and Maerk, Tilmann D. and Biasioli, Franco},
Title = {{Classification of 7 monofloral honey varieties by PTR-ToF-MS direct
   headspace analysis and chemometrics}},
Journal = {{TALANTA}},
Year = {{2016}},
Volume = {{147}},
Pages = {{213-219}},
Month = {{JAN 15}},
Abstract = {{Honey, in particular monofloral varieties, is a valuable commodity.
   Here, we present proton transfer reaction-time of flight-mass
   spectrometry, PTR-ToF-MS, coupled to chemometrics as a successful tool
   in the classification of monofloral honeys, which should serve in fraud
   protection against mispresentation of the floral origin of honey. We
   analyzed 7 different honey varieties from citrus, chestnut, sunflower,
   honeydew, robinia, rhododendron and linden tree, in total 70 different
   honey samples and a total of 206 measurements. Only subtle differences
   in the profiles of the volatile organic compounds (VOCs) in the
   headspace of the different honeys could be found. Nevertheless, it was
   possible to successfully apply 6 different classification methods with a
   total correct assignment of 81-99\% in the internal validation sets. The
   most successful methods were stepwise linear discriminant analysis (LDA)
   and probabilistic neural network (PNN), giving total correct assignments
   in the external validation sets of 100 and 90\%, respectively. Clearly,
   PTR-ToF-MS/chemometrics is a powerful tool in honey classification. (C)
   2015 Elsevier B.V. All rights reserved.}},
DOI = {{10.1016/j.talanta.2015.09.062}},
ISSN = {{0039-9140}},
EISSN = {{1873-3573}},
