@article{ ISI:000440876900033,
Author = {Servillo, Luigi and D'Onofrio, Nunzia and Neglia, Gianluca and Casale,
   Rosario and Cautela, Domenico and Marrelli, Massimo and Limone, Antonio
   and Campanile, Giuseppe and Balestrieri, Maria Luisa},
Title = {{Carnitine Precursors and Short-Chain Acylcarnitines in Water Buffalo
   Milk}},
