@article{ ISI:000168102300007,
Author = {Esposito, M and Imparato, E and Castellano, V and Guadagnuolo, G and
   Serpe, L},
Title = {{Polychlorinated biphenyls (PCB) monitoring on food of animal origin}},
Journal = {{INDUSTRIE ALIMENTARI}},
Year = {{2001}},
Volume = {{40}},
Number = {{399}},
Pages = {{29-32}},
Month = {{JAN}},
Abstract = {{Recently, a large amount of polychlorinated biphenyls and dioxins was
   found in a wide range of animal feeds acid food of animal origin, made
   in Belgium (1999). This incident represents a serious problem for public
   health. For this rea;son the European Union has established a program to
   monitoring PCBs levels in various food products of animal origin, in
   order to evaluate contamination levels and potential human health
   hazards from consumption.
   In the present paper the A.A, report the results of PCBs content
   determination in food and animal tissues collected in southern Italy.
   The quantitation of PCBs was made from the sum of seven toxic congeners,
   obtained by high resolution gas chromatography with an electron capture
   detector. Confirmatory analysis was performed by gas chromatography-mass
   spectrometry.
   The results obtained confirm the presence of PCB residues in various
   tissues but in only a asample the analyses yield a result above the
   value established by the European Union for PCBs levels in food of
   animal origin produced in Belgium.}},
