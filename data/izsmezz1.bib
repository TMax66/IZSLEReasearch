@article{ ISI:000168701600001,
Author = {Scarano, G and Grasso, L and Arace, O and Oliviero, G and Serpe, L},
Title = {{Use of semiautomatic equipment for the determination of fumonisin B1 in
   milk}},
Journal = {{INDUSTRIE ALIMENTARI}},
Year = {{2001}},
Volume = {{40}},
Number = {{401}},
Pages = {{257-260}},
Month = {{MAR}},
Abstract = {{A HPLC method for the determination of fumonisin in milk was modified.
   In comparison with the original method, semiautomatic equipment have
   been used in the purification step by means of immunoaffinity columns,
   and in the derivatization and LC injection steps. The use of these
   equipments provided good repeatability, short process time, and in
   particular the analysis of several samples in the same time could be
   carried out.
   The accuracy of the modified method resulted to be acceptable within the
   considered concentration range.
   The detector response resulted to be linear in the range 0.001-0.010
   mug. The detection limit was 0.010 mug mL(-1). The recovery of fumonisin
   from milk samples at fortification levels of 0.025, 0.05 and 0.1 mug
   mL(-1) resulted to be 83.1, 79.7 and 73.5\% respectively.}},
