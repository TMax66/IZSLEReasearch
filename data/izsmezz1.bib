Author = {Santoro, Mario and D'Alessio, Nicola and Di Prisco, Francesca and Neola,
   Benedetto and Restucci, Brunella and Pagano, Teresa B. and Veneziano,
   Vincenzo},
Title = {{Angiostrongylus vasorum infection in red foxes (Vulpes vulpes) in
   southern Italy}},
Journal = {{ACTA PARASITOLOGICA}},
Year = {{2015}},
Volume = {{60}},
Number = {{2}},
Pages = {{356-359}},
Month = {{JUN}},
Abstract = {{Angiostrongylus vasorum (Nematoda: Angiostrongylidae) infection was
   detected at post-mortem examination in the pulmonary arteries and hearts
   of 34/102 (33,3\%) of red foxes (Vulpes vulpes) from the Campania Region
   in southern Italy. Pathological changes consisted of granulomatous
   interstitial pneumonia caused by larvae and intravascular pulmonary
   adult nematodes. These changes confirm that angiostrongylosis infection
   in red foxes has a mainly chronic course, in which the infected host may
   disperse parasite larvae in the environment over its lifetime. Results
   suggest that the life cycle of A. vasorum is well established in the red
   fox in the Campania Region representing a potential infection risk for
   dogs.}},
