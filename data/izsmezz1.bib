@article{ ISI:000074259900002,
Author = {Guarino, A and Fusco, G and Romano, M and De Marco, G and Bani, A},
Title = {{Epidemiologic investigation on Salmonella presence in animal origin food}},
Journal = {{INDUSTRIE ALIMENTARI}},
Year = {{1998}},
Volume = {{37}},
Number = {{370}},
Pages = {{604-608}},
Month = {{MAY}},
Abstract = {{The results of an investigation carried out over a period of two years
   (1996- 1997) in order to detect Salmonella bacteria in Food of animal
   origin are here reported. In all, 1,534 food samples were examined: 790
   fresh pork sausage samples, 252 samples of cattle, pork, sheep and
   chicken chopped meat, 145 minced cattle meat samples, 21 hamburger
   samples, 101 egg samples, and, finally, 225 mozzarella cheese samples.
   Salmonella bacteria were detected in 67 samples (4.3\%); the most
   frequently contaminated foods were the following: Fresh pork sausages
   (6.8\%), minced cattle meat (4.1\%), chopped meat (1.5\%), eggs (0.9\%),
   and, finally, mozzarella cheese (0.4\%).
   In the present study, 67 Salmonella strains have been isolated and 57 of
   them were also identified. In decreasing order, the most: frequently
   detected serotypes are: S. typhimurium (20.6\%), S. livingstone and S.
   derby(l2\%), S. london (6.8\%), S. bredeney and S. mensten (5.1\%), and,
   finally, S. infantis, anatum, virchow and bovis-morbificans (3.4\%). In
   general, over the whole period of investigation, there were no
   significant variations in the frequency of isolation, except for a
   considerable increase during the months of November and December.}},
