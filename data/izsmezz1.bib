@article{ ISI:000276390400009,
Author = {Esposito, M. and Serpe, F. P. and Cavallo, S. and Pellicano, R. and
   Gallo, P. and Colarusso, G. and D'ambrosio, R. and Baldi, L. and Iovane,
   G. and Serpe, L.},
Title = {{A survey of dioxins (PCDDs and PCDFs) and dioxin-like PCBs in sheep and
   goat milk from Campania, Italy}},
Journal = {{FOOD ADDITIVES \& CONTAMINANTS PART B-SURVEILLANCE}},
Year = {{2010}},
Volume = {{3}},
Number = {{1}},
Pages = {{58-63}},
Abstract = {{In 2008, after the crisis of buffalo dairy fields in Campania, Italy, an
   assessment of the contamination of polychlorinated dibenzo-p-dioxins and
   polychlorinated dibenzofurans (PCDD/Fs) and dioxin-like polychlorinated
   biphenyls (dl-PCBs) was also necessary for other animal species bred in
   the region. The contents of PCDDs, polychlorinated dibenzofurans
   (PCDFs), and dl-PCBs were determined by high-resolution gas
   chromatography/mass spectrometry (HR-GC/MS) (according to USEPA method
   1613) in 69 sheep and goat milk samples from 63 farms. In eleven samples
   from six sheep farms, the PCDD/Fs levels exceeded the maximum limit of
   3.0 pg g(-1) fat established by the European Commission, in particular
   the concentrations ranged between 3.89 and 12.90 pg g(-1) fat.
   Statistical treatment of the results for the congener profiles of the
   non-compliant and compliant samples has been used to identify the
   sources of contamination.}},
