@article{ ISI:000385142100040,
Author = {Santoro, Mario and Veneziano, Vincenzo and D'Alessio, Nicola and Di
   Prisco, Francesca and Lucibelli, Maria Gabriella and Borriello, Giorgia
   and Cerrone, Anna and Dantas-Torres, Filipe and Latrofa, Maria Stefania
   and Otranto, Domenico and Galiero, Giorgio},
Title = {{Molecular survey of Ehrlichia canis and Coxiella burnetii infections in
   wild mammals of southern Italy}},
Journal = {{PARASITOLOGY RESEARCH}},
Year = {{2016}},
Volume = {{115}},
Number = {{11}},
Pages = {{4427-4431}},
Month = {{NOV}},
Abstract = {{Ehrlichiosis and Q fever caused by the intracellular bacteria Ehrlichia
   canis and Coxiella burnetii, respectively, are tick-borne diseases with
   zoonotic potential and widespread geographical distribution. This study
   investigated the prevalence of both infections in wild mammals in
   southern Italy. Tissue samples obtained from the red fox (Vulpes
   vulpes), European badger (Meles meles), gray wolf (Canis lupus), beech
   marten (Martes foina), and crested porcupine (Hystrix cristata) were
   processed for molecular detection of both pathogens. E. canis was
   detected in 55 out of 105 (52 \%) red foxes and three out of six gray
   wolves. Four sequence types were identified, three of which were found
   in the spleen and liver samples of red foxes and wolves, and one in the
   kidney of a red fox. None of the examined mammals was positive to C.
   burnetii type. This represents the first report of E. canis in
   free-ranging wolves worldwide, as well as the first evidence of this
   pathogen in red foxes in the peninsular Italy. Our results suggest that
   E. canis infection is common in free-ranging canids in southern Italy
   and that a sylvatic life cycle of this pathogen may occur.}},
