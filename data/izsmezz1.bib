@article{ ISI:000077033100005,
Author = {Serpe, L and Guarino, A and Fusco, G and Scaramuzzo, A and Gallo, P},
Title = {{Detection of Salmonella spp. in meat products by PCR and specific probe
   hybridation: A comparative study with the microbiological method}},
Journal = {{INDUSTRIE ALIMENTARI}},
Year = {{1998}},
Volume = {{37}},
Number = {{374}},
Pages = {{1141+}},
Month = {{OCT}},
Abstract = {{Nowadays, salmonellosis outbreaks are a relevant public health problem;
   these infections are often caused by contaminated foods and beverages,
   therefore it is very important the control of foodstuffs in order to
   detect the presence of Salmonella spp. by means of quick, sensitive and
   reproducible analytical methods. The official microbiological method to
   analyze foods is based on colture isolation and identification of the
   microorganism.
   However, in the last years, some methods based on polymerase chain
   reaction (PCR) have been developed to amplify specific DNA Fragments
   from pathogenic microorganisms. Herein, we report a comparative study
   between a PCR based method and the microbiological official procedure
   For detection of Salmonella spp. in meat products; we carried out a
   comparative study by evaluating both the performances and the ratio
   benefit-to-cost of the two methods.}},
