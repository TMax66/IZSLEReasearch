@article{ ISI:000495293800001,
Author = {Twabela, Augustin T. and Okamatsu, Masatoshi and Tshilenge, Georges
   Mbuyi and Mpiana, Serge and Masumu, Justin and Lam Thanh Nguyen and
   Matsuno, Keita and Monne, Isabella and Zecchin, Bianca and Sakoda,
   Yoshihiro},
Title = {{Molecular, antigenic, and pathogenic characterization of H5N8 highly
   pathogenic avian influenza viruses isolated in the Democratic Republic
   of Congo in 2017}},
Journal = {{ARCHIVES OF VIROLOGY}},
Year = {{2020}},
Volume = {{165}},
Number = {{1}},
Pages = {{87-96}},
Month = {{JAN}},
Abstract = {{In May 2017, high mortality of chickens and Muscovy ducks due to the
   H5N8 highly pathogenic avian influenza virus (HPAIV) was reported in the
   Democratic Republic of Congo (DR Congo). In this study, we assessed the
   molecular, antigenic, and pathogenic features in poultry of the H5N8
   HPAIV from the 2017 Congolese outbreaks. Phylogenetic analysis of the
   eight viral gene segments revealed that all 12 DR Congo isolates
   clustered in clade 2.3.4.4B together with other H5N8 HPAIVs isolated in
   Africa and Eurasia, suggesting a possible common origin of these
   viruses. Antigenically, a slight difference was observed between the
   Congolese isolates and a representative virus from group C in the same
   clade. After intranasal inoculation with a representative DR Congo
   virus, high pathogenicity was observed in chickens and Muscovy ducks but
   not in Pekin ducks. Viral replication was higher in chickens than in
   Muscovy duck and Pekin duck organs; however, neurotropism was pronounced
   in Muscovy ducks. Our data confirmed the high pathogenicity of the DR
   Congo virus in chickens and Muscovy ducks, as observed in the field.
   National awareness and strengthening surveillance in the region are
   needed to better control HPAIVs.}},
