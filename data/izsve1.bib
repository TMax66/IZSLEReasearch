@article{ ISI:000491755200001,
Author = {Carminato, Antonio and Tecilla, Marco and Roccabianca, Paola and
   Zanardello, Claudia and Melchiotti, Erica and Capello, Katia and
   Vascellari, Marta},
Title = {{CD30 Cross-Reactivity and Expression in Feline Normal Tissues and
   Lymphomas}},
