@article{ ISI:000623291000001,
Author = {Twabela, Augustin T. and Lam Thanh Nguyen and Masumu, Justin and Mpoyo,
   Patrick and Mpiana, Serge and Sumbu, Julienne and Okamatsu, Masatoshi
   and Matsuno, Keita and Isoda, Norikazu and Zecchin, Bianca and Monne,
