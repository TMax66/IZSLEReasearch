Author = {Peggion, Caterina and Stella, Roberto and Lorenzon, Paolo and Spisni,
   Enzo and Bertoli, Alessandro and Massimino, Maria Lina},
Title = {{Microglia in Prion Diseases: Angels or Demons?}},
Journal = {{INTERNATIONAL JOURNAL OF MOLECULAR SCIENCES}},
