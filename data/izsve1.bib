@article{ WOS:000460823600002,
Author = {Primavilla, Sara and Farneti, Silvana and Petruzzelli, Annalisa and
   Drigo, Ilenia and Scuota, Stefania},
Title = {Contamination of hospital food with <i>Clostridium difficile</i> in
   Central Italy},
Journal = {ANAEROBE},
Year = {2019},
Volume = {55},
Pages = {8-10},
Month = {FEB},
Abstract = {This study investigates the contamination of foods with Clostridium
   difficile in 3 hospitals of Central Italy. We used real-time PCR for tpi
   gene to analyse 350 food samples, including 296 cooked meals and 54
   foods to be eaten raw. The molecular screening test was positive for 3
