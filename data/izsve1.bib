@article{ ISI:000425314100034,
Author = {Leopardi, Stefania and Holmes, Edward C. and Gastaldelli, Michele and
   Tassoni, Luca and Priori, Pamela and Scaravelli, Dino and Zamperin,
   Gianpiero and De Benedictis, Paola},
Title = {{Interplay between co-divergence and cross-species transmission in the
   evolutionary history of bat coronaviruses}},
Journal = {{INFECTION GENETICS AND EVOLUTION}},
Year = {{2018}},
Volume = {{58}},
Pages = {{279-289}},
Month = {{MAR}},
Abstract = {{Coronaviruses (CoVs) have been documented in almost every species of bat
   sampled. Bat CoVs exhibit both extensive genetic diversity and a broad
   geographic range, indicative of a long-standing host association.
   Despite this, the respective roles of long-term virus-host co-divergence
   and cross-species transmission (host-jumping) in the evolution of bat
   coronaviruses are unclear. Using a phylogenetic approach we provide
   evidence that CoV diversity in bats is shaped by both species richness
   and their geographical distribution, and that CoVs exhibit clustering at
   the level of bat genera, with these genus-specific clusters largely
   associated with distinct CoV species. Co-phylogenetic analyses revealed
   that cross-species transmission has been more common than co-divergence
   across coronavirus evolution as a whole, and that cross-species
   transmission events were more likely between sympatric bat hosts.
   Notably, however, an analysis of the CoV RNA polymerase phylogeny
   suggested that many such host-jumps likely resulted in short-term
   spill-over infections, with little evidence for sustained onward
   transmission in new co-roosting host species.}},
Publisher = {{ELSEVIER}},
Address = {{RADARWEG 29, 1043 NX AMSTERDAM, NETHERLANDS}},
Type = {{Article}},
