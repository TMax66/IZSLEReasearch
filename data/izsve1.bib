@article{ ISI:000467179800008,
Author = {Best, M. P. and Boyd, S. P. and Danesi, P.},
Title = {{Confirmed case of Pneumocystis pneumonia in a Maltese Terrier x Papillon
   dog being treated with toceranib phosphate}},
Journal = {{AUSTRALIAN VETERINARY JOURNAL}},
Year = {{2019}},
Volume = {{97}},
Number = {{5}},
Pages = {{162-165}},
Month = {{MAY}},
Abstract = {{Case report A 7-year-old female-neutered Maltese Terrier x Papillon dog
   was presented with tachypnoea and weight loss following 12 months of
   therapy with toceranib phosphate for a metastatic,
   histologically-low-grade mast cell tumour. The dog was diagnosed with
   Pneumocystis canis based on PCR with supportive clinical, radiographic
   and cytological findings. No other clinical evidence of immunocompromise
   was identified through assessment of haematology and immunoglobulin
   quantification. Clinical signs completely resolved with a short course
   of potentiated sulfonamides and discontinuation of the toceranib.
   Conclusion To the authors' knowledge this represents the first case of
   Pneumocystis in a dog secondary to immunomodulatory drug therapy. It is
   also the first case of opportunist infection secondary to a tyrosine
   kinase inhibitor in dogs.}},
Publisher = {{WILEY}},
Address = {{111 RIVER ST, HOBOKEN 07030-5774, NJ USA}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Best, MP (Corresponding Author), Brisbane Vet Specialist Ctr, Albany Creek, Qld, Australia.
