@article{ WOS:000648754100001,
Author = {Bottinelli, Marco and Stefani, Elisabetta and Matucci, Andrea and Dal
   Pra, Mauro and Capello, Katia and Zotti, Alessandro and Catania,
   Salvatore},
Title = {Isolation of <i>Mycoplasma iowae</i> in turkey flocks with skeletal
   abnormalities: a retrospective study},
Journal = {AVIAN PATHOLOGY},
