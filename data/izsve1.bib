@article{ ISI:000535657800001,
Author = {Boukedjouta, Rachid and Pretto, Tobia and Abbadi, Miriam and Biasini,
   Lorena and Toffan, Anna and Mezali, Karim},
Title = {{Viral encephalopathy and retinopathy is endemic in wild groupers (genus
   Epinephelus spp.) of the Algerian coast}},
Journal = {{JOURNAL OF FISH DISEASES}},
