@article{ WOS:000910760800001,
Author = {Inchingolo, Alessio Danilo and Gargiulo, Ciro Isacco and Malcangi,
   Giuseppina and Ciocia, Anna Maria and Patano, Assunta and Azzollini,
   Daniela and Piras, Fabio and Barile, Giuseppe and Settanni, Vito and
