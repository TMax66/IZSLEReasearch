@article{ WOS:001001594200001,
Author = {Zianni, Rosalia and Mentana, Annalisa and Tomaiuolo, Michele and
   Campaniello, Maria and Iammarino, Marco and Centonze, Diego and Palermo,
   Carmen},
Title = {Volatolomic approach by HS-SPME/GC-MS and chemometric evaluations for
   the discrimination of X-ray irradiated mozzarella cheese},
Journal = {FOOD CHEMISTRY},
Year = {2023},
Volume = {423},
Month = {OCT 15},
Abstract = {In this work, an untargeted screening of the volatile profile of X-ray
   irradiated mozzarella cheese was carried out to study the possible
   radio-induced modifications. A Central Composite Design (CCD) for
   Response Surface Methodology (RSM) was employed to optimise the HS-SPME
   analysis of volatile organic compounds (VOCs). The optimised HS-SPME
   conditions, in terms of sample amount (5.0 g), extraction temperature
   (50 degrees C) and extraction time (75 min), were used to analyse
   non-irradiated and irradiated samples at three dose levels, 1.0, 2.0,
   3.0 kGy. Partial Least Squares-Discriminant Analysis (PLS-DA) and Linear
   Discriminant Analysis (LDA) were applied to explore the variation of
   volatile profile with respect to the X-ray irradiation treatment. Both
   methods highlighted a high discriminant capability with excellent values
   of accuracy, specificity and sensitivity, demonstrating the
   effectiveness of the volatolomic approach to evaluate the variations
   induced by the treatment and allowing to select a total of 35 VOCs as
   potential irradiation markers.},
