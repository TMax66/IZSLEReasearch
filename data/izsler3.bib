@article{ ISI:A1995RR59500005,
Author = {AMADORI, M and ARCHETTI, IL and VERARDI, R and BERNERI, C},
Title = {{CHARACTERIZATION OF A DISTINCT SUBPOPULATION OF BOVINE GAMMA-DELTA
   T-CELLS}},
