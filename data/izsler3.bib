@article{ ISI:000229844000016,
Author = {Valtulini, S and Macchi, C and Ballanti, P and Cherel, Y and Laval, A
   and Theaker, JM and Bak, M and Ferretti, E and Morvan, H},
Title = {{Aluminium hydroxide-induced granulomas in pigs}},
Journal = {{VACCINE}},
Year = {{2005}},
Volume = {{23}},
Number = {{30}},
Pages = {{3999-4004}},
Month = {{JUN 10}},
Abstract = {{The effect of intramuscular injection of 40 mg/2 ml aluminium hydroxide
   in the neck of pigs was examined in a number of ways. The investigation
   followed repeated slaughterhouse reports, according to which 64.8\% of
   pigs from one particular farm were found at slaughter to have one or
   more nodules in the muscles of the neck (group slaughtered). The pigs
   had been injected with a vaccine containing 40 mg/2 ml dose of aluminium
   hydroxide as adjuvant. Research consisted of two phases: first, an
   epidemiological study was carried out, aimed at determining the risk
   factors for the granulomas. The results indicated that the vaccine was
   to be held responsible for the formation of granulomas. A clinical trial
   was then performed to further substantiate the initial hypothesis, by
   comparing pigs, which were aseptically inoculated twice with either the
   original vaccine or the adjuvant alone (groups vaccine and adjuvant) to
   pigs inoculated twice with apyrogenic bi-distilled water (group water)
   and to pigs inoculated once with the adjuvant and once with apyrogenic
   bi-distilled water (group adjuvant/water). Both studies agreed in their
   conclusions, which indicate that the high amount of aluminium hydroxide
   was the cause of the granulomas. (c) 2005 Elsevier Ltd. All rights
   reserved.}},
