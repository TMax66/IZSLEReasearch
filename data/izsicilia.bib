@article{ WOS:000723717800001,
Author = {Galluzzo, Paola and Migliore, Sergio and Cascio, Silvana and Barreca,
   Santino and Alfano, Marilena and Tagliarini, Antonina and Candela, Anna
   and Piraino, Chiara and Galuppo, Lucia and Condorelli, Lucia and
   Hussein, Hany A. and Tittarelli, Manuela and Chiarenza, Giuseppina},
Title = {Diagnostic Findings in a Confirmed Outbreak of <i>Brucella ovis</i>
   Infection in a Traditional Sheep Farm in Sicily (South-Italy)},
Journal = {PATHOGENS},
Year = {2021},
Volume = {10},
Number = {11},
Month = {NOV},
Abstract = {Aim of this study is to report a laboratory investigation performed
   following the isolation of Brucella ovis, causing ovine epididymitis, in
   a traditional sheep farm in Sicily (South Italy). This disease
   represents a newly emerging risk for Italian livestock and is listed
   among diseases of EU priority (EU Reg 2016/429). Blood samples from 56
   rams and 143 ewes were analyzed by both Enzyme-Linked Immunosorbent
   Assay (ELISA) and Complement Fixation Test (CFT). Genital swabs from all
   rams and 15 lactating ewes were collected to perform real-time PCR.
   Eighteen serologically positive rams were slaughtered and
   postmortem-inspected. Samples of testicle, epididymis, lymph nodes, and
   urine were also collected in order to perform microbiological,
   molecular, and histopathological analysis. Twelve slaughtered rams
   showed anatomo-pathological lesions. Real-time PCR for B. ovis
   BOV\_A0504 gene was positive for 13 testicles and epididymis and 11
   urine while B. ovis was isolated from epididymis and testicles of 7
   slaughtered rams. This is the first exhaustive laboratory report of a
   microbiological, molecular, and serological pattern of the disease in
   sheep in Italy. Despite the impact on health and animal welfare, the
   epidemiology of B. ovis infection is still unknown, particularly in our
   country where the disease is considered endemic.},
