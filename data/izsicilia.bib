@article{ WOS:000645049700007,
Author = {Costa, Antonella and Alio, Vincenzina and Sciortino, Sonia and Nicastro,
   Luisa and Cangini, Monica and Pino, Fiorella and Servadei, Irene and La
   Vignera, Angelo and Fortino, Gianluca and Monaco, Salvatore and
