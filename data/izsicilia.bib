@article{ WOS:000791825700014,
Author = {Giuseppe, Acri and Annastella, Falcone and Giannetto, Claudia and
   Elisabetta, Giudice and Giuseppe, Piccione and Barbara, Testagrossa and
   Cicero, Luca and Cassata, Giovanni and Simona, Di Pietro},
Title = {Preliminary study for the application of Raman spectroscopy for the
   identification of Leishmania infected dogs},
Journal = {SCIENTIFIC REPORTS},
Year = {2022},
Volume = {12},
Number = {1},
Month = {MAY 6},
Abstract = {Raman spectroscopy is a rapid qualitative and quantitative technique
   that allows the simultaneous determination of several components in
   biological fluids. This methodology concerns an alternative technique to
   distinguish between non-healthy and healthy subjects. Leishmaniasis is a
   zoonosis of world interest, the most important agent is L. infantum.
   Dogs are the principal reservoirs affected by a broad spectrum of
   clinical features. During a clinical exam, blood samples were collected
   in tubes without anticoagulants, from twenty two dogs. One aliquot was
   used for serological test for Leishmaniasis, one aliquot was subjected
   to the Raman spectroscopic analysis. Animals were divided into two
   groups of equal subjects, Leishmania group (LG) constituted by infected
   dogs, and control group (CG) constituted by healthy dogs. The acquired
   spectra were different in the region 1200-1370 cm(-1), in which it is
   possible to distinguish the amide III vibration (similar to 1300
   cm(-1)). In LG, an evident shift to the shortwave region is observed in
   spectral frequencies of the band centered at similar to 1250 cm(-1). Our
   results distinguished between LD group and CG. Further studies are
   necessary to exclude the effect of metabolic modification due to disease
   on the recorded spectra changes and to consolidate the achievability of
   Raman spectroscopy as rapid and less expensive diagnosis of
   Leishmaniasis.},
