@article{ WOS:000620367000019,
Author = {Ogbu, Kenneth Ikejiofor and Chukwudi, Ijeoma Chekwube and Mira,
   Francesco and Eze, Ukamaka Uchenna and Di Bella, Santina and Olaolu,
   Olushola Samuel and Tion, Matthew Terzungwe and Purpari, Giuseppa and
   Cannella, Vincenza and Nwosuh, Ignatius Chika and Guercio, Annalisa and
   Anene, Boniface Maduka},
Title = {Current status and risk factors of canine parvovirus type 2 in North
   Central Nigeria},
Journal = {COMPARATIVE IMMUNOLOGY MICROBIOLOGY AND INFECTIOUS DISEASES},
Year = {2021},
Volume = {74},
Month = {FEB},
Abstract = {Since its emergence in Nigeria, canine parvovirus type 2 (CPV-2)
   infection has posed problems to dog breeding and requires constant
   awareness and monitoring. In this study, the status, the assessment of
   extrinsic risk factors of parvoviral infection in dog kennels in North
   Central Nigeria, and isolation of the CPV-2 were carried out. Potential
   risk factors were considered during sampling: age, breed, sex, location,
   vaccination and health status, using well-structured questionnaires on
   dog owners with experience of CPV-2 infection. There was high prevalence
   which depended on age, breed, location, clinical status of the dog while
   vaccination status of the dogs did not influence the prevalence. CPV-2
   vaccination compliance by the breeders and management system of the
   kennels were also observed as risk factors. Isolation of CPV-2a and-2c
   strains from Nigeria for further study has been reported. The spread of
   CPV-2 in Nigeria is increasing, hence needs for continual
   epidemiological monitoring and review.},
