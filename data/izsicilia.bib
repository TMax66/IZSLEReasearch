@article{ WOS:000622016300001,
Author = {Chelli, Eleonora and Suffredini, Elisabetta and De Santis, Paola and De
   Medici, Dario and Di Bella, Santina and D'Amato, Stefania and Gucciardi,
   Francesca and Guercio, Annalisa and Ostanello, Fabio and Perrone,
