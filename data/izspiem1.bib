@article{ ISI:000489688800011,
Author = {Di Blasio, Alessia and Irico, Lara and Caruso, Claudio and Miceli,
   Ilaria and Robetto, Serena and Peletto, Simone and Varello, Katia and
   Giorda, Federica and Mignone, Walter and Rubinetti, Francesca and
