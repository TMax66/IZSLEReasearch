@inproceedings{ ISI:000377304000046,
Author = {Traversa, Amaranta and Bianchi, Daniela Manila and Gallina, Silvia and
   Macori, Guerrino and Fagioli, Franca and Barisone, Elena and Griglio,
   Bartolomeo and Squadrone, Stefania and Abete, Maria Cesarina and
   Decastelli, Lucia},
Editor = {{Chova, LG and Martinez, AL and Torres, IC}},
Title = {{EDUCATION AND HEALTH PROMOTION: A GUIDELINE BOOKLET FOR GOOD PRACTICE IN
   IMMUNOCOMPROMISED CHILDREN}},
Booktitle = {{ICERI2015: 8TH INTERNATIONAL CONFERENCE OF EDUCATION, RESEARCH AND
   INNOVATION}},
