@article{ ISI:000478756900010,
Author = {Melegari, Irene and Di Profio, Federica and Palombieri, Andrea and
   Sarchese, Vittorio and Diakoudi, Georgia and Robetto, Serena and Orusa,
   Riccardo and Marsilio, Fulvio and Banyai, Kristian and Martella, Vito
   and Di Martino, Barbara},
Title = {{Molecular detection of canine bufaviruses in wild canids}},
Journal = {{ARCHIVES OF VIROLOGY}},
Year = {{2019}},
Volume = {{164}},
Number = {{9}},
Pages = {{2315-2320}},
Month = {{SEP}},
Abstract = {{Novel protoparvoviruses genetically related to human and non-human
   primate bufaviruses (BuVs) have been detected recently in respiratory
   and enteric specimens collected from dogs and cats. In this study, by
   molecular screening of archival collections of faecal samples from
   wolves and foxes, we detected BuVs with a rate of 17.1\% (7/41) and
   10.5\% (9/86), respectively. Sequence analysis of a portion of the ORF2
   gene region of nine positive samples showed that the viruses in these
   samples were closely related to BuVs (97.5-99.0\% nucleotide sequence
   identity) found in domestic carnivores.}},
