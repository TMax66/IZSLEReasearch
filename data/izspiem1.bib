@article{ ISI:000636446400003,
Author = {Lecrenier, Marie-Caroline and Marien, Aline and Veys, Pascal and
   Belghit, Ikram and Dieu, Marc and Gillard, Nathalie and Henrottin, Jean
   and Herfurth, Uta M. and Marchis, Daniela and Morello, Sara and Oveland,
   Eystein and Poetz, Oliver and Rasinger, Josef Daniel and Steinhilber,
   Andreas and Baeten, Vincent and Berben, Gilbert and Fumiere, Olivier},
Title = {{Inter-laboratory study on the detection of bovine processed animal
   protein in feed by LC-MS/MS-based proteomics}},
Journal = {{FOOD CONTROL}},
Year = {{2021}},
Volume = {{125}},
Month = {{JUL}},
Abstract = {{Since the beginning of the feed ban regarding the use of processed
   animal protein (PAP) in feedstuffs, one of the main challenges has been
   to develop methods to detect and identify the type and origin of PAP or
   animal products. By now, in the context of a controlled relaxation of
   the ban, complementary methods are sought to characterize the product
   when positive responses are delivered by current official methods, with
   special attention on feed materials (whether or not authorized)
   originating from ruminants. Liquid chromatography-tandem mass
   spectrometry (LC-MS/MS)-based proteomics was identified as the most
   promising method to meet this challenge. This study presents the first
   inter-laboratory test constructed to evaluate and compare the LC-MS/MS
   methods currently developed in six different laboratories from four
   countries. Nine samples consisting of blank feed matrix or feed
   adulterated with bovine processed animal proteins and/or milk products
   were analyzed using inhouse protocols and the results obtained were
   discussed. This study confirms the potential of LC-MS/MS-based
   proteomics to resolve the current analytical gaps in the detection and
   differentiation of PAP and highlights the technical challenges that need
   to be addressed in future.}},
