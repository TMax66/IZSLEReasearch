@article{ ISI:000519242900044,
Author = {Mugetti, Davide and Pastorino, Paolo and Menconi, Vasco and Pedron,
   Claudio and Prearo, Marino},
Title = {{The Old and the New on Viral Diseases in Sturgeon}},
Journal = {{PATHOGENS}},
Year = {{2020}},
Volume = {{9}},
Number = {{2}},
Month = {{FEB}},
Abstract = {{Although sturgeon production by aquaculture has increased worldwide, a
   major factor limiting its expansion are infectious diseases, although
   few data about viral diseases are available however. This review
   provides a rapid overview of viral agents detected and described to
   date. Following a general introduction on viral diseases are four
   sections arranged by virus classification: sturgeon nucleocytoplasmic
   large DNA viruses, herpesviruses, white sturgeon adenovirus 1, and other
   viruses. Molecular diagnosis is currently the best tool to detect viral
   diseases, since cell culture isolation is not yet applicable for the
   detection of most sturgeon viruses.}},
