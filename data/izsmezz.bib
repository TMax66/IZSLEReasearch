@article{ WOS:000848288300002,
Author = {Qu, Chengkai and Albanese, Stefano and Cicchella, Domenico and Fortelli,
   Alberto and Hope, Dave and Esposito, Mauro and Cerino, Pellegrino and
   Pizzolante, Antonio and Qi, Shihua and De Vivo, Benedetto and Lima,
