@article{ WOS:000658754800012,
Author = {la Gatta, Barbara and Rutigliano, Mariacinzia and Rusco, Giusy and
   Gagliardi, Rosa and Zicarelli, Luigi and Di Luccia, Aldo},
Title = {Biochemical evidence for a quantitative polymorphism at the
   α<sub>S1</sub>- and κ-CN loci in Italian Mediterranean buffalo milk},
Journal = {INTERNATIONAL DAIRY JOURNAL},
Year = {2021},
Volume = {119},
Month = {AUG},
Abstract = {The application of reversed phase high performance liquid chromatography
   for the analysis of 116 Italian Mediterranean buffalo milk samples
   allowed us to find a non-allelic polymorphism at the level of
   alpha(S1)-CN and kappa-CN loci. Duplicated and triplicate haplotypes
   were revealed by different proportions in the heterozygous forms of the
   A and B alpha(S1)-CN alleles and of kappa-CN T (X1) and I (X2) alleles.
   Chromatographic profiles revealed that approximately 97.0\% and 91.3\%
   of the individuals exhibited the expected polymorphisms, as well as
   different aberrant patterns for alpha(S1)-CN and kappa-CN (4 and 5
   respectively). The current study contributes to the assessment of the
   genes involved in protein secretion and for inferring nonallelic
   polymorphisms through the characterisation of phenotypic expression. (C)
   2021 Elsevier Ltd. All rights reserved.},
