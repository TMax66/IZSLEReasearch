@article{ WOS:000911235800001,
Author = {Fusco, Giovanna and Cardillo, Lorena and Levante, Martina and Brandi,
   Sergio and Picazio, Gerardo and Napoletano, Michele and Martucciello,
   Alessandra and Fiorito, Filomena and De Carlo, Esterina and de Martinis,
   Claudio},
Title = {First serological evidence of SARS-CoV-2 natural infection in small
   ruminants Brief report},
Journal = {VETERINARY RESEARCH COMMUNICATIONS},
Year = {2023},
Volume = {47},
Number = {3},
Pages = {1741-1748},
Month = {SEP},
Abstract = {Severe Acute Respiratory Syndrome Coronavirus 2 (SARS-CoV-2) emerged in
   late December 2019 and spread worldwide, quickly becoming a pandemic.
   This zoonotic coronavirus shows a broad host range, including wildlife
   and domestic animals. Small ruminants are shown to be susceptible to
   SARS-CoV-2 but, to date, no natural infection has been reported. Herein,
   we performed a survey for SARS-CoV-2 among sheep and goats in the
   Campania region of Italy using an indirect multispecies ELISA. Next,
   positive sera were submitted to virus serum neutralization for the
   quantification of specific neutralizing antibodies. Out of 612 sheep and
   goats, 23 were found ELISA positive (3.75\%) and 1 of them showed 1:20
   neutralizing antibodies titer. No significant difference was found
   between the two species, as well as between male and female,
   geographical location and age. Our findings demonstrate that natural
   infection can occur in flocks in a field situation. Moreover, low
   susceptibility to SARS-CoV-2 is reported for sheep and goats,
   nevertheless, the continuous mutations of this virus open new scenarios
   on viral host range and tropism, highlighting the importance of
   investigating animal species that could represent ongoing or future
   possible hosts.},
