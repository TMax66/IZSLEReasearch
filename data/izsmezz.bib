@article{ WOS:000759084600067,
Author = {Diakoudi, Georgia and Capozza, Paolo and Lanave, Gianvito and
   Pellegrini, Francesco and Di Martino, Barbara and Elia, Gabriella and
   Decaro, Nicola and Camero, Michele and Ghergo, Paola and Stasi, Fabio
