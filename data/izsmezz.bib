@article{ WOS:000661464800001,
Author = {Cerino, Pellegrino and Coppola, Annachiara and Pierri, Biancamaria and
   Volzone, Palmiero and Bruzzese, Dario and Pierri, Andrea and Ferro,
   Amedeo and Schiavi, Daniela and Coppola, Antonio and Miniero, Stefano
