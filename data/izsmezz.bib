@article{ WOS:000612821900001,
Author = {Cutarelli, Anna and De Falco, Francesca and Uleri, Valeria and
   Buonavoglia, Canio and Roperto, Sante},
Title = {The diagnostic value of the droplet digital PCR for the detection of
   bovine <i>deltapapillomavirus</i> in goats by liquid biopsy},
Journal = {TRANSBOUNDARY AND EMERGING DISEASES},
Year = {2021},
Volume = {68},
Number = {6},
Pages = {3624-3630},
Month = {NOV},
Abstract = {In the present study, the highly pathogenic bovine deltapapillomavirus
   (delta PV) was investigated by liquid biopsy in blood samples of 168
   clinically normal goats using both droplet digital PCR (ddPCR) and
   quantitative real-time PCR (qPCR). Overall, ddPCR discovered BPV E5 DNA
   in similar to 61.3\% of the blood samples examined, while real-time qPCR
   revealed the virus in similar to 10.7\% of the same samples. Moreover,
   ddPCR showed BPV E5 DNA in similar to 78.8\% of blood samples from goats
   that were in close contact with cattle and in 20\% of blood samples from
   goats living in closed pens without any contact with cattle. In
   addition, ddPCR unmasked a single BPV genotype in similar to 59.2\% and
   multiple genotypes in similar to 40.8\% of goats harbouring BPV DNA,
   while real-time qPCR detected single genotypes in similar to 17\% and
   multiple genotypes in similar to 1\%. Of the BPV co-infections detected
   by ddPCR, 28 (similar to 67\%) involved two genotypes, eight (similar to
   19\%) three genotypes and six (similar to 14\%) four genotypes. In
   contrast, real-time qPCR revealed BPV co-infection by two genotypes in
   only one sample and failed to detect co-infection by three or four
   genotypes. BPV2 and BPV13 were the most prevalent viruses responsible
   for single and multiple co-infections, respectively. The present study
   showed that ddPCR is promising method for circulating bovine
   papillomavirus DNA detection and quantification and suggested that
   animal husbandry practices contribute to cross-species transmission of
   BPVs.},
