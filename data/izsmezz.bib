@article{ WOS:000632631400019,
Author = {Sgroi, Giovanni and Iatta, Roberta and Veneziano, Vincenzo and
   Bezerra-Santos, Marcos Antonio and Lesiczka, Paulina and Hrazdilova,
   Kristyna and Annoscia, Giada and D'Alessio, Nicola and Golovchenko,
   Maryna and Rudenko, Natalie and Modry, David and Otranto, Domenico},
Title = {Molecular survey on tick-borne pathogens and<i> Leishmania</i><i>
   infantum</i> in red foxes<i> (Vulpes</i><i> vulpes)</i> from southern
   Italy},
Journal = {TICKS AND TICK-BORNE DISEASES},
Year = {2021},
Volume = {12},
Number = {3},
Month = {MAY},
Abstract = {Red foxes (Vulpes vulpes) have been recognised to harbour and transmit a
   wide range of tick-borne pathogens (TBPs) including those of zoonotic
   concern. To investigate the prevalence and the distribution of TBPs and
   of Leishmania infantum in foxes (n = 244), spleen samples were collected
   within the frame of a multi-regional wildlife health surveillance
   program in Italy. A combined PCR/sequencing approach was performed for
   the detection of Anaplasma spp., Babesia spp., Borrelia spp., Ehrlichia
   spp., Hepatozoon spp. and L. infantum DNA. Overall, 146 foxes (59.8 \%,
   95 \% CI: 53.6?65.8) tested positive for at least one pathogen with
   Hepatozoon canis being the most prevalent (i.e., n = 124; 50.8 \%, 95 \%
   CI: 44.6?57.0), followed by Babesia vulpes (n = 20; 8.2 \%, 95 \% CI:
