@article{ WOS:000734876300001,
Author = {Cuomo, Paola and Medaglia, Chiara and Allocca, Ivana and Montone, Angela
   Michela Immacolata and Guerra, Fabrizia and Cabaro, Serena and Mollo,
   Ernesto and Eletto, Daniela and Papaianni, Marina and Capparelli,
