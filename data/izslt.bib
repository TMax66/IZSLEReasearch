Author = {Condoleo, Roberto and Rinaldi, Laura and Sette, Stefania and Mezher,
   Ziad},
Title = {Risk Assessment of Human Toxoplasmosis Associated with the Consumption
   of Pork Meat in Italy},
Journal = {RISK ANALYSIS},
