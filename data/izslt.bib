@article{ WOS:000588287600009,
Author = {Condoleo, Roberto and Giangolini, Gilberto and Chiaverini, Alexandra and
   Patriarca, Daniela and Scaramozzino, Paola and Mezher, Ziad},
Title = {Occurrence of <i>Listeria monocytogenes</i> and <i>Escherichia coli</i>
   in Raw Sheep's Milk from Farm Bulk Tanks in Central Italy},
Journal = {JOURNAL OF FOOD PROTECTION},
Year = {2020},
Volume = {83},
