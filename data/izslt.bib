@article{ WOS:000676467200001,
Author = {Cocumelli, Cristiano and Iurescia, Manuela and Diaconu, Elena Lavinia
   and Galietta, Valentina and Raso, Caterina and Buccella, Carmela and
   Stravino, Fiorentino and Grande, Francesco and Fiorucci, Letizia and De
   Liberato, Claudio and Caprioli, Andrea and Battisti, Antonio},
Title = {Plasmodium matutinum Causing Avian Malaria in Lovebirds (Agapornis
   roseicollis) Hosted in an Italian Zoo},
Journal = {MICROORGANISMS},
Year = {2021},
Volume = {9},
Number = {7},
Month = {JUL},
Abstract = {Avian malaria is a worldwide distributed, vector-born disease of birds
   caused by parasites of the order Haemosporida. There is a lack of
   knowledge about the presence and pathogenetic role of Haemosporida in
   Psittacidae. Here we report a case of avian malaria infection in
   lovebirds (Agapornis roseicollis), with the genetic characterization of
   the Plasmodium species involved. The birds were hosted in a zoo located
   in Italy, where avian malaria cases in African penguins (Spheniscus
   demersus) were already reported. Animals (n = 11) were submitted for
   necropsy after sudden death and were subjected to further analyses
   including histopathology, bacteriology, and PCR for the research of
   haemosporidians. Clinical history, gross lesions and histopathological
   observation of schizonts, together with positive PCR results for
