@article{ WOS:000432128400036,
Author = {Pintus, Davide and Varcasia, Antonio and Dessi, Giorgia and Tamponi,
   Claudia and Manunta, Maria Lucia and Carboni, Giovanni Antonio and
   Cancedda, Maria Giovanna and Ligios, Ciriaco and Scala, Antonio},
Title = {Atypical outbreak of acute coenurosis by Taenia multiceps in a sheep
   flock},
Journal = {PARASITOLOGY RESEARCH},
Year = {2018},
Volume = {117},
Number = {6},
Pages = {1985-1988},
Month = {JUN},
Abstract = {Herein, we examined the brain of adult ewes and lambs less than 30 days
   old which were found affected by neurological signs in a flock located
   in Sardinia (Italy). Gross anatomo-pathological examination of all
   brains of the animals revealed multiple linear reddish-yellow foci of
   necrotic purulent inflammation due to oncosphere migration.
   Histologically, we confirmed a multifocal pyo-granulomatous
   meningo-encephalitis both in ewes and in lambs, confirming acute
   coenurosis. Morphological examination and DNA sequencing identified the
   Taenia multiceps we isolated as Tm1 strain. This report describes for
   the first time a natural acute coenurosis infection in suckling lambs
   under 30 days of age.},
