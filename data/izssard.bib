@article{ ISI:000244823500028,
Author = {Boatto, Gianpiero and Culeddu, Nicola and Testa, Cecilia and Neri, Bruno
   and Brambilla, Gianfranco and Barbosa, Jorge and Cruz, Clara},
Title = {{Identification of a probable new adrenergic agonist by nuclear magnetic
   resonance and mass spectrometry}},
Journal = {{ANALYTICA CHIMICA ACTA}},
Year = {{2007}},
Volume = {{586}},
Number = {{1-2}},
Pages = {{223-227}},
