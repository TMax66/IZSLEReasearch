@article{ ISI:000401375500003,
Author = {Cabizza, Roberto and Rubattu, Nicolino and Salis, Severyn and Pes,
   Massimo and Comunian, Roberta and Paba, Antonio and Addis, Margherita
   and Testa, M. Cecilia and Urgeghe, P. Paolo},
Title = {{Transfer of oxytetracycline from ovine spiked milk to whey and cheese}},
