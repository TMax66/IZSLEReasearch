@article{ ISI:000398099100011,
Author = {Pulina, Silvia and Satta, Cecilia Teodora and Padedda, Bachisio Mario
   and Bazzoni, Anna Maria and Sechi, Nicola and Luglie, Antonella},
Title = {{Picophytoplankton Seasonal Dynamics and Interactions with Environmental
   Variables in Three Mediterranean Coastal Lagoons}},
