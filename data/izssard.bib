@article{ WOS:000897170300009,
Author = {Mudadu, Alessandro G. and Piras, Gabriella and Melillo, Rita and Salza,
   Sara and Cau, Simona and Virgilio, Sebastiano and Meloni, Domenico and
   Mele, Piera},
Title = {Survival of Naturally Contaminating<i> Listeria</i><i> monocytogenes</i>
   in Commercial Mediterranean-Style Dry Fermented Sausages during Storage},
Journal = {JOURNAL OF FOOD PROTECTION},
Year = {2022},
Volume = {85},
Number = {11},
Pages = {1576-1583},
Month = {NOV},
Abstract = {The aim of the present study was the determination of Listeria
   monocytogenes, competitive microbiota, microbial hygiene indicators, and
   physicochemical parameters in the typical Mediterranean-style fermented
   sausages Salsiccia Sarda. A batch of Salsiccia Sarda (25 samples)
   naturally contaminated by L. monocytogenes and vacuum packaged after 24
   days of ripening was included in the study. Fifteen samples stored at 8
   degrees C were analyzed after 13 days, after 90 days, and at the end of
   shelf life (after 180 days from vacuum packaging). Ten vacuum-packaged
   samples were stored at 12 degrees C in a domestic fridge simulating
   temperature abuse and were evaluated at the end of the shelf life.
   Samples were subjected to physicochemical analysis (pH and water
   activity) and investigated for the presence and enumeration of L.
   monocytogenes. Competitive microbiota, lactic acid bacteria,
   coagulase-negative staphylococci, and microbial hygiene indicators
   (total mesophilic bacterial counts, Enterobacte-riaceae, Enterococcuss
   spp., and Staphylococcus aureus) were determined in all the samples.
   Although a decreasing trend in L. monocytogenes prevalence was observed
   through the shelf life, the detection of this pathogen in fermented
   sausages confirms its ability to overcome hurdles of the manufacturing
   process. The results highlight the importance of the careful evaluation
   of the Salsiccia Sarda production process by food business operators to
   maintain unfavorable conditions for the growth of L. monocytogenes.},
