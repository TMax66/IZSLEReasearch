@article{ ISI:000226915100010,
Author = {Civitareale, A and Ambrosio, C and Sbraccia, M and Fiori, M and
   Brambilla, G and Testa, C},
Title = {{Pharmacological characterization of new beta-agonists using Hu beta(1)-
   and Hu beta(2)-adrenergic receptor binding assay in transfected HEK-293
   cells}},
