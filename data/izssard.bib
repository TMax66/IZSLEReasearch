@article{ WOS:000875011400001,
Author = {Audino, Tania and Berrone, Elena and Grattarola, Carla and Giorda,
   Federica and Mattioda, Virginia and Martelli, Walter and Pintore,
   Antonio and Terracciano, Giuliana and Cocumelli, Cristiano and Lucifora,
