@article{ WOS:000963545800001,
Author = {Hamze, Rim and Foxi, Cipriano and Ledda, Salvatore and Satta, Giuseppe
   and Ruiu, Luca},
Title = {<i>Pseudomonas protegens</i> Affects Mosquito Survival and Development},
Journal = {CURRENT MICROBIOLOGY},
Year = {2023},
Volume = {80},
Number = {5},
Month = {MAY},
Abstract = {This study investigated the pathogenic potential of Pseudomonas
   protegens on mosquito larvae of the two species Culex pipiens and Aedes
   albopictus, representing major threats for disease transmission in the
   Mediterranean area and worldwide. The bacterium achieved to kill over
   90\% of the mosquito larvae within 72 h after exposition to a bacterial
   concentration of 100 million CFU/ml. These lethal effects were
   concentration dependent and a significantly higher susceptibility was
   associated with younger larvae of both mosquito species. Significant
   slowdown of immature (larval and pupal) development and decrease in
   adult emergence rate after treatment with sub-lethal doses of the
   bacterium were also detected. This study reports for the first time the
   insecticidal activity of a root-associated biocontrol bacterium against
   aquatic mosquito larvae.},
