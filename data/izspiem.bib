@article{ WOS:000680335400004,
Author = {Falandysz, Jerzy and Meloni, Daniela and Fernandes, Alwyn R. and
   Saniewski, Michal},
Title = {Effect of drying, blanching, pickling and maceration on the fate of
   K-40, total K and Cs-137 in bolete mushrooms and dietary intake},
Journal = {ENVIRONMENTAL SCIENCE AND POLLUTION RESEARCH},
Year = {2022},
Volume = {29},
Number = {1, 1},
Pages = {742-754},
Month = {JAN},
Abstract = {The effects of blanching, blanching and pickling and maceration on the
   leaching of Cs-137 and K-40 from the flesh of three edible bolete
   mushroom species-Boletus edulis, Leccinum scabrum and Leccinum
   versipelle-were investigated. Significant (p < 0.05) decreases in
   activity were observed but varied depending on the treatment. Relative
   to fresh mushrooms, blanching decreased the activity concentration of
   Cs-137 by 15 +/- 13\%, and of K-40, by 16 +/- 7\%, but blanching and
   pickling (vinegar) reduced activity more effectively, by 55 +/- 8\% and
   40 +/- 20\% respectively. The corresponding losses of Cs-137 and K-40
   through maceration of dried, powdered mushrooms were 38 +/- 11\% and 35
   +/- 14\% ww, respectively. These results indicate that traditional
   domestic processing methods may not be as efficient at excluding Cs-137
   radioactivity as shown in some other studies. The activity concentration
   of Cs-137 in a typically sized (100 g) portion of a processed mushroom
   (sourced from nearshore regions of the southern Baltic Sea coast near
   Gdansk in 2015) meal was projected to be low, i.e. in the range of 0.51
   to 12 Bq kg(-1) ww. The corresponding effective dose of Cs-137 from
   blanched, blanched and pickled and macerated mushrooms per capita was
   also assessed to be low, from 0.001 to 0.010 mu Sv. Nutritionally, the
   median concentration of potassium (330 mg) in 100 g portions of blanched
   or pickled mushrooms would account for around 7\% of the adequate adult
   daily intake.},
