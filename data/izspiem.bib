@article{ WOS:000662380000001,
Author = {Zoccola, Roberto and Di Blasio, Alessia and Bossotto, Tiziana and
   Pontei, Angela and Angelillo, Maria and Dondo, Alessandro and Goria,
   Maria and Zoppi, Simona},
Title = {Validation of a Novel Diagnostic Approach Combining the VersaTREK (TM)
   System for Recovery and Real-Time PCR for the Identification of
   Mycobacterium chimaera in Water Samples},
Journal = {MICROORGANISMS},
Year = {2021},
Volume = {9},
Number = {5},
Month = {MAY},
Abstract = {Mycobacterium chimaera is an emerging pathogen associated with
   endocarditis and vasculitis following cardiac surgery. Although it can
   take up to 6-8 weeks to culture on selective solid media, culture-based
   detection remains the gold standard for diagnosis, so more rapid methods
   are urgently needed. For the present study, we processed environmental
   M. chimaera infected simulates at volumes defined in international
   guidelines. Each preparation underwent real-time PCR; inoculates were
   placed in a VersaTREK (TM) automated microbial detection system and onto
   selective Middlebrook 7H11 agar plates. The validation tests showed that
   real-time PCR detected DNA up to a concentration of 10 ng/mu L. A
   comparison of the isolation tests showed that the PCR method detected
   DNA in a dilution of x10(2) CFU/mL in the bacterial suspensions, whereas
   the limit of detection in the VersaTREK (TM) was <10 CFU/mL. Within less
   than 3 days, the VersaTREK (TM) detected an initial bacterial load of
   100 CFU. The detection limit did not seem to be influenced by NaOH
   decontamination or the initial water sample volume; analytical
   sensitivity was 1.5 x 10(2) CFU/mL; positivity was determined in under
   15 days. VersaTREK (TM) can expedite mycobacterial growth in a culture.
   When combined with PCR, it can increase the overall recovery of
   mycobacteria in environmental samples, making it potentially applicable
   for microbial control in the hospital setting and also in environments
   with low levels of contamination by viable mycobacteria.},
