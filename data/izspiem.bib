@article{ WOS:000437837700003,
Author = {Schiffer, Davide and Mellai, Marta and Bovio, Enrica and Bisogno, Ilaria
   and Casalone, Cristina and Annovazzi, Laura},
Title = {Glioblastoma niches: from the concept to the phenotypical reality},
Journal = {NEUROLOGICAL SCIENCES},
