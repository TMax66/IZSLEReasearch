@article{ WOS:000486665700020,
Author = {Candellone, Alessia and Peiretti, Pier Giorgio and Binello, Arianna and
   Bergagna, Stefania and Meineri, Giorgia},
Title = {Effect of linseed diet and hazelnut skin supplementation on oxidative
   status and blood serum metabolites in rabbits},
Journal = {PROGRESS IN NUTRITION},
