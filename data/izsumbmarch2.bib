@article{ ISI:A1994PW21300002,
Author = {OTTAVIANI, D and BACCHIOCCHI, I and ROCCHEGIANI, E},
Title = {{PSYCROPHILE AND THERMOPHILE MILK GERMS - IMPORTANCE IN THE ASSESSMENT OF
   MILK PRESERVATION AND WHOLESOMENESS}},
Journal = {{INDUSTRIE ALIMENTARI}},
Year = {{1994}},
Volume = {{33}},
Number = {{330}},
Pages = {{951-953}},
Month = {{OCT}},
Abstract = {{Sometimes it does not suffice to determine only the microbial and
   mesophile presence in the raw milk intended for alimentary use in order
   to guarantee the quality of the finished product.  In this work the
   Authors intended to evaluate with a predictive purpose the potential
   preservation and wholesomeness of cow milk intended for human use,
   either direct or for dairy products.  It is also intended to quantify: 
   1) the microbial psycrophile population responsible for many processes
   of degradation due to thermoresistant enzymes which may be in the
   processed products, even if the microbial population that generated them
   has been destroyed by thermal treatment; 2) the thermoresistant
   population which is not only able to survive processes of stabilization,
   but also to multiply in an often wide range of temperature, thus causing
   problems especially in case of regulatory microbial limits for food
   products on the market.}},
