@article{ ISI:000275628300005,
Author = {Colussi, S. and Vaccari, G. and Rasero, R. and Ponti, A. Maroni and Ru,
   G. and Sacchi, P. and Caramelli, M. and Agrimi, U. and Acutis, P. L.},
Title = {{Prospects for applying breeding for resistance to control scrapie in
   goats: The current situation in Italy}},
Journal = {{SMALL RUMINANT RESEARCH}},
Year = {{2010}},
Volume = {{88}},
Number = {{2-3, SI}},
