@article{ ISI:000078268200010,
Author = {Pellegrino, C and Guerci-Lena, P and Canova, A and Gennari, M},
Title = {{Impedometric evaluation of mesophilic bacteria in imported foods:
   cephalopods and crustaceans}},
Journal = {{ANNALI DI MICROBIOLOGIA ED ENZIMOLOGIA}},
Year = {{1998}},
Volume = {{48}},
Number = {{2}},
Pages = {{XXVII-XXXIII}},
Abstract = {{The impedometric detection has been used previously to estimate
   microbial contamination of different foods. The purpose of this study
   was to evaluate the reliable bacterial count in selected imported
   seafoods using conductance measurement in comparation with standard
   plate method. During 7 years (1991-1997) we have analyzed 352
   cephalopods and 261 crustaceans, coming from european and extraeuropean
   countries. A good correlation coefficent between impedometric and
   standard plate count was obtained for both seafoods. The reproducibility
   of the analisys allows the utilization of impedance method to obtain
   rapid and reliable estimates of bacterial contamination in cephalopods
   and crustaceans.}},
