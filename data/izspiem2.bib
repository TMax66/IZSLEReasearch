@article{ ISI:000186275500007,
Author = {Zini, E and Bovero, A and Nigrisoli, E and Ratto, A and Rampazzo, A and
   Zatelli, A},
Title = {{Sarcomatoid renal cell carcinoma with osteogenic differentiation and
   paraneoplastic hepatopathy in a dog, possibly related to Human
   Stauffer's syndrome}},
Journal = {{JOURNAL OF COMPARATIVE PATHOLOGY}},
Year = {{2003}},
Volume = {{129}},
Number = {{4}},
Pages = {{303-307}},
Month = {{NOV}},
Abstract = {{Sarcomatoid renal cell carcinoma is an uncommon tumour in human beings,
   and osteogenic differentiation is a rare feature. This report describes
   such a case in a male dog aged 8 years. The tumour, which showed
   extensive osseous metaplasia and a few necrotic areas, protruded into
   the renal pelvis, disrupting the renal capsule. Light microscopical and
   immunohistochemical examination revealed the epithelial nature of the
   tumour. Abnormal liver biochemistry, mild hepatocyte degeneration and
   the absence of histological evidence of metastasis suggested a
   paraneoplastic hepatopathy. (C) 2003 Elsevier Ltd. All rights reserved.}},
