@article{ ISI:000311211800059,
Author = {Ottaviani, Donatella and Leoni, Francesca and Serra, Roberto and
   Serracca, Laura and Decastelli, Lucia and Rocchegiani, Elena and Masini,
   Laura and Canonico, Cristina and Talevi, Giulia and Carraturo, Antonio},
Title = {{Nontoxigenic Vibrio parahaemolyticus Strains Causing Acute
   Gastroenteritis}},
Journal = {{JOURNAL OF CLINICAL MICROBIOLOGY}},
Year = {{2012}},
Volume = {{50}},
Number = {{12}},
Pages = {{4141-4143}},
Month = {{DEC}},
Abstract = {{We investigated the virulence properties of four Vibrio parahaemolyticus
   strains causing acute gastroenteritis following consumption of
   indigenous mussels in Italy. The isolated strains were cytotoxic and
   adhesive but, surprisingly, lacked tdh, trh, and type three secretion
   system 2 (T3SS2) genes. We emphasize that nontoxigenic V.
   parahaemolyticus can induce acute gastroenteritis, highlighting the need
   for more investigation of the pathogenicity of this microorganism.}},
