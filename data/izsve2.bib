@article{ WOS:000902329200001,
Author = {Carelli, Maria and Griggio, Francesca and Mingoia, Marina and Garofalo,
   Cristiana and Milanovic, Vesna and Pozzato, Nicola and Leoni, Francesca
   and Veschetti, Laura and Malerba, Giovanni and Sandri, Angela and
