@article{ WOS:000724599200005,
Author = {West, Johanna and Roeder, Juliane and Matrosovich, Tatyana and Beicht,
   Jana and Baumann, Jan and Mounogou Kouassi, Nancy and Doedt, Jennifer
   and Bovin, Nicolai and Zamperin, Gianpiero and Gastaldelli, Michele and
