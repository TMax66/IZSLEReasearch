@article{ WOS:000746594700007,
Author = {Bonardi, Silvia and Blagojevic, Bojan and Belluco, Simone and Roasto,
   Mati and Gomes-Neves, Eduarda and Vagsholm, Ivar},
Title = {Food chain information in the European pork industry: Where are we?},
