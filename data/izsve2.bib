@article{ WOS:000770919000007,
Author = {Pietropaoli, M. and Ribarits, A. and Moosbeckhofer, R. and Koeglberger,
   H. and Alber, O. and Gregorc, A. and Skerl, M. I. Smodis and Presern, J.
   and Bubnic, J. and Muz, M. Necati and Higes, M. and Tiozzo, B. and
   Jannoni-Sebastianini, F. and Lubroth, J. and Cazier, J. and Raizman, E.
   and Zilli, R. and Bagni, M. and Della Marta, U. and Formato, G.},
Title = {Biosecurity measures in European beekeeping},
Journal = {REVUE SCIENTIFIQUE ET TECHNIQUE-OFFICE INTERNATIONAL DES EPIZOOTIES},
Year = {2020},
Volume = {39},
Number = {3},
Pages = {725-735},
Month = {DEC},
Abstract = {Emerging pathogens of honey bees represent an important threat to the
   development of the beekeeping sector. The implementation of biosecurity
   measures in beekeeping (BMBs) plays an essential role in supporting
   honey bee health within the beekeeping sector. A group of experts, in
   collaboration with the BPRACTICES (Grant Agreement No. 696231, European
   Research Area on Sustainable Animal Production Systems {[}ERA-Net
   SusAn]) project partners, has provided the definition of BMBs. Thus,
   BMBs are all those operational activities implemented to control the
   risk of introduction and spread of specific honey bee disease agents. In
   this paper, the BMBs in the European beekeeping context are identified
   for the most relevant honey bee diseases in Europe: varroosis, American
   foulbrood (AFB), European foulbrood (EFB), nosemosis and aethinosis.
   Moreover, BMBs were classified in `categories' adapted to consider
   productivity and the `One Health' approach: human health, honey bee
   health and protection of the environment. The 84 BMBs described by the
   panel of experts were ranked according to the priority score attributed.
   The implementation of BMBs represents an essential step forwards to
   increase the resilience and sustainability of European beekeeping.},
