@article{ WOS:000939398500001,
Author = {Rota, Paola and Bonfante, Francesco and Anastasia, Luigi and La Rocca,
   Paolo and Pagliari, Matteo and Piccoli, Marco and Cirillo, Federica and
   Ghiroldi, Andrea and Franco, Valentina and Pappone, Carlo and Allevi,
   Pietro},
Title = {Design, Synthesis, and Antiviral Evaluation of Sialic Acid Derivatives
   as Inhibitors of Newcastle Disease Virus Hemagglutinin-Neuraminidase: A
   Translational Study on Human Parainfluenza Viruses},
Journal = {ACS INFECTIOUS DISEASES},
Year = {2023},
Month = {2023 FEB 27},
Abstract = {Global infections with viruses belonging to the Paramyxoviridae, such as
   Newcastle disease virus (NDV) or human parainfluenza viruses (hPIVs),
   pose a serious threat to animal and human health. NDV-HN and hPIVs-HN
   (HN hemagglutininneuraminidase) share a high degree of similarity in
   catalytic site structures; therefore, the development of an efficient
   experimental NDV host model (chicken) may be informative for evaluating
   the efficacy of hPIVs-HN inhibitors. As part of the broad research in
   pursuit of this goal and as an extension of our published work on
   antiviral drug development, we report here the biological results
   obtained with some newly synthesized C4- and C5-substituted
   2,3-unsaturated sialic acid derivatives against NDV. All developed
   compounds showed high neuraminidase inhibitory activity (IC50 0.03-13 mu
   M). Four molecules (9, 10, 23, 24) confirmed their high in vitro
   inhibitory activity, which caused a significant reduction of NDV
   infection in Vero cells, accompanied by very low toxicity.},
