@article{ WOS:000824446700004,
Author = {Jucker, C. and Belluco, S. and Oddon, S. Bellezza and Ricci, A. and
   Bonizzi, L. and Lupi, D. and Savoldelli, S. and Biasato, I and Caimi, C.
