@article{ ISI:000267454600007,
Author = {Zecconi, Alfonso and Piccinini, Renata and Fiorina, Silvia and Cabrini,
   Luca and Dapra, Valentina and Amadori, Massimo},
Title = {{Evaluation of interleukin-2 treatment for prevention of intramammary
   infections in cows after calving}},
Journal = {{COMPARATIVE IMMUNOLOGY MICROBIOLOGY AND INFECTIOUS DISEASES}},
Year = {{2009}},
Volume = {{32}},
Number = {{5}},
Pages = {{439-451}},
Month = {{SEP}},
Abstract = {{A low-dose treatment based on interleukin-2 (IL-2) was investigated for
   preventing mastitis in dairy cows. The treatment consisted of a single
   dose of IL-2 injected into the skin region drained by the supramammary
   lymph node 3-5 days after calving. The study included 45 cows (23
   treated and 22 controls) from three commercial dairy herds. The results
   showed that the treatment had no side effects. The treatment with IL-2
   induced the significant increase of several milk markers related to
   leukocyte and epithelial cell functions, i.e. SCC (somatic cell counts),
   serum amyloid A (SAA), lactoferrin and NAGase. The increased
   concentration of milk markers suggested also an activity of IL-2 on
   epithelial cells, resulting in a higher resistance to invading
   pathogens. Indeed, the increased efficiency of cells in the udder is
   supported by the higher frequency of healthy quarters observed in the
   treated group until day 17-19 after calving, in comparison with the
   control one. (C) 2008 Elsevier Ltd. All rights reserved.}},
