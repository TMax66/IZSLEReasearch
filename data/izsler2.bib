@article{ ISI:000330523200004,
Author = {Lelli, Davide and Papetti, Alice and Sabelli, Cristiano and Rosti,
   Enrica and Moreno, Ana and Boniotti, Maria B.},
Title = {{Detection of Coronaviruses in Bats of Various Species in Italy}},
Journal = {{VIRUSES-BASEL}},
