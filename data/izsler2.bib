@article{ ISI:000292798500026,
Author = {Di Francesco, Antonietta and Donati, Manuela and Morandi, Federico and
   Renzi, Maria and Masia, Marco Antonio and Ostanello, Fabio and
   Salvatore, Daniela and Cevenini, Roberto and Baldelli, Raffaella},
Title = {{Seroepidemiologic Survey for Chlamydia suis in Wild Boar (Sus scrofa)
   Populations in Italy}},
Journal = {{JOURNAL OF WILDLIFE DISEASES}},
Year = {{2011}},
Volume = {{47}},
Number = {{3}},
Pages = {{709-712}},
Month = {{JUL}},
Abstract = {{We used serology to estimate the prevalence of exposure to chlamydiae in
   Italian populations of wild boars (Sus scrota). Sera from 173
   hunter-killed wild boars harvested during the 2006-2009 hunting seasons
   in three Italian regions were tested for antibodies to Chlamydia suis,
   Chlamydophila pecorum, Chlamydophila abortus, and Chlamydophila psittaci
   by the microimmunofluorescence test. Antibody titers to chlamydiae >=
   1.:32 were detected in 110 of the 17:3 samples tested (63.6\%). Specific
   reactivity could be assessed only in 44 sera with antibody titers to C.
   suis that were two- to threefold higher than antibody titers against the
   other chlamydial species; the other 66 sera had similar reactivity
   against all the chlamydia species tested. Antibody to C. suis was
   detected in sera from wild boar populations with rare or no known
   contact with domestic pigs. These results suggest that the wild boar
   could be a chlamydia reservoir and may acquire chlamydiae independent of
   contacts with the domestic pig.}},
