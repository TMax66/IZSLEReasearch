@article{ ISI:000265924800001,
Author = {Harouna, A. and Zecchini, M. and Locatelli, C. and Scaccabarozzi, L. and
   Cattaneo, C. and Amadou, A. and Bronzo, V. and Marichatou, H. and
   Boettcher, P. J. and Zanoni, M. G. and Alborali, L. and Moroni, P.},
Title = {{Milk hygiene and udder health in the periurban area of Hamdallaye, Niger}},
Journal = {{TROPICAL ANIMAL HEALTH AND PRODUCTION}},
Year = {{2009}},
Volume = {{41}},
Number = {{5}},
Pages = {{705-710}},
Month = {{JUN}},
Abstract = {{The prevalence of intra-mammary infections in dairy herds was studied in
   Hamdallaye, Niger. A total of 956 milk samples were collected in 2007
   from 239 lactating cows of four local breeds in eight traditional herds;
   the first sampling was undertaken in the dry season at morning milking,
   and the second in the rainy season at evening milking. Staphylococcus
   aureus, Coagulase-Negative Staphylococci (CNS) and environmental
   microorganisms were detected in significantly (p < 0.05) more samples in
   the rainy season, 55.2\%, than in the dry season, 27.1\%. Statistically
   significant (P < 0.05) differences in prevalence were observed among
   herds and according to lactation number. Infections were assigned to
   four classes, according to the major pathogen, and the respective mean
   somatic cell counts during the dry season were: S. aureus, 775 x 10(3)
   cells/ml; CNS, 447 x 10(3) cells/ml; environmental microorganisms, 407 x
   10(3) cells/ml; and non-infected, 262 x 10(3) cells/ml. Most of the
   tested strains were sensitive to antibiotics, and selected strains of S.
   aureus (n = 15) were negative to the multiplex PCR tests for production
   of enterotoxins.}},
