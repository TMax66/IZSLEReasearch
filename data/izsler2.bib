@article{ ISI:000339602500025,
Author = {Delibato, Elisabetta and Rodriguez-Lazaro, David and Gianfranceschi,
   Monica and De Cesare, Alessandra and Comin, Damiano and Gattuso,
   Antonietta and Hernandez, Marta and Sonnessa, Michele and Pasquali,
   Frederique and Sreter-Lancz, Zuzsanna and Saiz-Abajo, Maria-Jose and
   Perez-De-Juan, Javier and Butron, Javier and Prukner-Radovcic, Estella
   and Tomic, Danijela Horvatek and Johannessen, Gro S. and Jakociune,
   Dziuginta and Olsen, John E. and Chemaly, Marianne and Le Gall,
   Francoise and Gonzalez-Garcia, Patricia and Lettini, Antonia Anna and
   Lukac, Maja and Quesne, Segolene and Zampieron, Claudia and De Santis,
   Paola and Lovari, Sarah and Bertasi, Barbara and Pavoni, Enrico and
   Proroga, Yolande T. R. and Capuano, Federico and Manfreda, Gerardo and
   De Medici, Dario},
Title = {{European validation of Real-Time PCR method for detection of Salmonella
   spp. in pork meat}},
Journal = {{INTERNATIONAL JOURNAL OF FOOD MICROBIOLOGY}},
Year = {{2014}},
Volume = {{184}},
Number = {{SI}},
Pages = {{134-138}},
Month = {{AUG 1}},
Abstract = {{The classical microbiological method for detection of Salmonella spp.
   requires more than five days for final confirmation, and consequently
   there is a need for an alternative methodology for detection of this
   pathogen particularly in those food categories with a short shelf-life.
   This study presents an international (at European level) ISO 16140-based
   validation study of a non-proprietary Real-Time PCR-based method that
   can generate final results the day following sample analysis. It is
   based on an ISO compatible enrichment coupled to an easy and inexpensive
   DNA extraction and a consolidated Real-Time PCR assay. Thirteen
   laboratories from seven European Countries participated to this trial,
   and pork meat was selected as food model. The limit of detection
   observed was down to 10 CFU per 25 g of sample, showing excellent
   concordance and accordance values between samples and laboratories
   (100\%). In addition, excellent values were obtained for relative
   accuracy, specificity and sensitivity (100\%) when the results obtained
   for the Real-Time PCR-based methods were compared to those of the ISO
   6579:2002 standard method. The results of this international trial
   demonstrate that the evaluated Real-Time PCR-based method represents an
   excellent alternative to the ISO standard. In fact, it shows an equal
   and solid performance as well as it reduces dramatically the extent of
   the analytical process, and can be easily implemented routinely by the
   Competent Authorities and Food Industry laboratories. (C) 2014 Elsevier
   B.V. All rights reserved.}},
