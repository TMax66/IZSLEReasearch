@article{ ISI:000350459100001,
Author = {Torre, Maria Luisa and Lucarelli, Enrico and Guidi, Simona and Ferrari,
   Maura and Alessandri, Giulio and De Girolamo, Laura and Pessina, Augusto
   and Ferrero, Ivana and GISM},
Title = {{Ex Vivo Expanded Mesenchymal Stromal Cell Minimal Quality Requirements
   for Clinical Application}},
Journal = {{STEM CELLS AND DEVELOPMENT}},
