@article{ WOS:000520912100014,
Author = {Bolzoni, Luca and Della Marca, Rossella and Groppi, Maria and Gragnani,
   Alessandra},
Title = {DYNAMICS OF A METAPOPULATION EPIDEMIC MODEL WITH LOCALIZED CULLING},
Journal = {DISCRETE AND CONTINUOUS DYNAMICAL SYSTEMS-SERIES B},
