@article{ WOS:000689939900001,
Author = {Lelli, Davide and Scanferla, Vittorio and Moreno, Ana and Sozzi, Enrica
   and Ravaioli, Valentina and Renzi, Maria and Tosi, Giovanni and Dottori,
   Michele and Lavazza, Antonio and Calzolari, Mattia},
Title = {Serological Evidence of Phleboviruses in Domestic Animals on the
   Pre-Apennine Hills (Northern Italy)},
Journal = {VIRUSES-BASEL},
