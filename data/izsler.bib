@article{ WOS:000664220100009,
Author = {Bertani, Valeria and Prioni, Simona and Di Lecce, Rosanna and Gazza,
   Ferdinando and Ragionieri, Luisa and Merialdi, Giuseppe and Bonilauri,
   Paolo and Jagannathan, Vidhya and Grassi, Sara and Cabitta, Livia and
   Paoli, Antonella and Morrone, Amelia and Sonnino, Sandro and
