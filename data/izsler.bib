@article{ WOS:000701586600001,
Author = {Trogu, Tiziana and Canziani, Sabrina and Salvato, Sara and Tolini, Clara
   and Grilli, Guido and Chiari, Mario and Farioli, Marco and Alborali,
   Loris and Gaffuri, Alessandra and Sala, Giovanni and Bianchi, Alessandro
