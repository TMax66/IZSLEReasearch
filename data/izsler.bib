@article{ WOS:000824099600001,
Author = {Righi, Francesco and Arnaboldi, Sara and Filipello, Virginia and Ianiro,
   Giovanni and Di Bartolo, Ilaria and Calo, Stefania and Bellini, Silvia
   and Trogu, Tiziana and Lelli, Davide and Bianchi, Alessandro and
   Bonardi, Silvia and Pavoni, Enrico and Bertasi, Barbara and Lavazza,
   Antonio},
Title = {<i>Torque Teno Sus Virus</i> (TTSuV) Prevalence in Wild Fauna of
   Northern Italy},
Journal = {MICROORGANISMS},
Year = {2022},
Volume = {10},
Number = {2},
Month = {FEB},
Abstract = {Torque teno sus virus (TTSuV) is a non-enveloped circular ssDNA virus
   which frequently infects swine and has been associated with hepatic,
   respiratory, and autoimmune disorders. TTSuV's pathogenic role is still
   uncertain, and clear data in the literature on virus reservoirs are
   lacking. The aims of this study were to investigate the presence of
   potentially zoonotic TTSuV in wild animals in Northern Italy and to
   evaluate their role as reservoirs. Liver samples were collected between
   2016 and 2020 during four hunting seasons from wild boars (Sus scrofa),
   red deer (Cervus elaphus), roe deer (Capreolus capreolus), and chamois
   (Rupicapra rupicapra). Samples originated from areas in Northern Italy
   characterized by different traits, i.e., mountains and flatland with,
   respectively low and high farm density and anthropization. Viral
   identification was carried out by end-point PCR with specific primers
   for TTSuV1a and TTSuVk2a species. TTSuV prevalence in wild boars was
   higher in the mountains than in the flatland (prevalence of 6.2\% and
   2.3\%, respectively). In wild ruminants only TTSuVk2a was detected (with
   a prevalence of 9.4\%). Our findings shed light on the occurrence and
   distribution of TTSuV in some wild animal species, investigating their
   possible role as reservoirs.},
