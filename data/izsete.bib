@article{ WOS:000594644800001,
Author = {Annunziata, Loredana and Visciano, Pierina and De Massis, Maria Rosaria
   and Schirone, Maria and Colagrande, Maria Novella and Campana, Guido and
   Migliorati, Giacomo},
Title = {Detection of ethinyl estradiol and resorcylic acid lactones in poultry
   meat according to the national residue plan},
Journal = {DRUG TESTING AND ANALYSIS},
Year = {2021},
Volume = {13},
Number = {4},
Pages = {862-866},
Month = {APR},
Abstract = {The aim of this study was the detection of ethinyl estradiol and
   resorcylic acid lactones (zeranol and taleranol) in poultry meat samples
   as unauthorized substances included in the Italian national residue
   plan. The samples were purified by a solid phase extraction using a C-18
   column combined with alumina, and the analytes were detected by two
   specific enzyme-linked immunosorbent assay (ELISA) kits. As they were
   collected in the frame of official control activities, the method was
   also validated according to the Commission Decision 2002/657/EC
   requirements for screening methods with qualitative purpose. Specificity
   and detection capability were the performance criteria considered for
   the validation study, and the latter parameter showed a value of 0.5 mu
   g/kg for both the investigated compounds. Such result was well
   comparable with the data reported by using chromatographic techniques as
   confirmation methods, and therefore, the ELISA kits tested in this study
   could be used for the screening of large numbers of samples.},
Publisher = {WILEY},
Address = {111 RIVER ST, HOBOKEN 07030-5774, NJ USA},
Type = {Article},
