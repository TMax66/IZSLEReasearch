   Ariful},
Title = {First isolation, identification and genetic characterization of
   <i>Brucella abortus</i> biovar 3 from dairy cattle in Bangladesh},
Journal = {VETERINARY MEDICINE AND SCIENCE},
Year = {2019},
Volume = {5},
