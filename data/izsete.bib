@inproceedings{ WOS:000678409203020,
Author = {Vincenzi, Stefano and Porrello, Angelo and Buzzega, Pietro and Cipriano,
   Marco and Fronte, Pietro and Cuccu, Roberto and Ippoliti, Carla and
   Conte, Annamaria and Calderara, Simone},
