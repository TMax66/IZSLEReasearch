@article{ WOS:000626576600002,
Author = {Cosseddu, Gian Mario and Doumbia, B. and Scacchia, M. and Pinoni, C. and
   Di Provvido, A. and Polci, A. and Isselmou, K. and Di Gennaro, A. and
   Spedicato, M. and Carmine, I. and Savini, G. and Dondona, A. Capobianco
   and Iapaolo, F. and Valleriani, F. and El Mamy, Ahmed Bezeid and Barry,
   Yaya and Monaco, F.},
Title = {Sero-surveillance of emerging viral diseases in camels and cattle in
   Nouakchott, Mauritania: an abattoir study},
Journal = {TROPICAL ANIMAL HEALTH AND PRODUCTION},
Year = {2021},
Volume = {53},
Number = {2},
Month = {MAR 5},
Abstract = {This study reports the monitoring of several emerging viral pathogens in
   Mauritania, which was carried out by the analysis of bovine and camel
   samples taken at the slaughterhouse of Nouakchott. Blood and serum were
   collected by random sampling from 159 camels and 118 cattle in March
   2013 at the large animals abattoir in Nouakchott. Serological tests for
   Rift Valley Fever (RVF), Peste des Petits Ruminants (PPR), West Nile
   disease (WND), epizootic haemorrhagic disease (EHD) and African horse
   sickness (AHS) were carried out using commercial ELISA kits. The
   samples, which resulted positives for PPR, WND and AHS, were tested with
   the confirmatory virus neutralization test (VNT). According to ELISA
   results, serological prevalence of RVF was 45\% (95\% CI 52.3-37.7) in
   camels and 16\% (95\% CI 22.6-9.4) in cattle. The difference between the
   observed prevalences in camels and in cattle was significant (p value <=
   0.01). PPR was absent in camels and had 12\% prevalence (95\% CI,
