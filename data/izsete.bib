@article{ WOS:000802731500001,
Author = {Pascucci, Ilaria and Paniccia, Marta and Giammarioli, Monica and
   Biagetti, Massimo and Duranti, Anna and Campomori, Pamela and Smilari,
   Valerio and Ancora, Massimo and Scialabba, Silvia and Secondini, Barbara
