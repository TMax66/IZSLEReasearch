@article{ WOS:000757038000001,
Author = {Santini, Marija and Haberle, Sara and Zidovec-Lepej, Snjezana and Savic,
   Vladimir and Kusulja, Marija and Papic, Neven and Viskovic, Klaudija and
   Zupetic, Ivana and Savini, Giovanni and Barbic, Ljubo and Tabain, Irena
