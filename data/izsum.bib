@article{ WOS:000593830200001,
Author = {Accoroni, Stefano and Giulietti, Sonia and Romagnoli, Tiziana and
   Siracusa, Melania and Bacchiocchi, Simone and Totti, Cecilia},
Title = {Morphological Variability of <i>Pseudo-nitzschia pungens</i> Clade I
   (Bacillariophyceae) in the Northwestern Adriatic Sea},
Journal = {PLANTS-BASEL},
Year = {2020},
Volume = {9},
Number = {11},
Month = {NOV},
Abstract = {Pseudo-nitzschia pungens is a common component of the phytoplankton
   community of the northern Adriatic Sea. In this study, an in-depth
   morphological analysis of P. pungens was carried out in both cultured
   strains isolated in different periods and field samples, revealing a
   surprisingly wide variability in a number of details, with both the
   gross morphology and ultrastructural levels deviating from the nominal
   P. pungens. Colonies showed an overlap (from one-third to one-sixth) and
   a transapical axis (rarely reaching 3 mu m), strongly differing from the
   original description of the species. Moreover, valves may be either
   symmetrical or slightly asymmetrical, with striae almost always
   biseriate but sometimes uniseriate or triseriate. Poroids' morphology in
   cingular bands was characterized by a wide variability (square,
   circular, or rectangular poroids without or with up to two hymen
   sectors), with several combination of them, even within the same
   cingular band. Phylogenetic analyses based on ITS rDNA showed that the
   P. pungens of the northern Adriatic Sea belonged to clade I. Domoic acid
   was not detected.},
