@article{ WOS:000811799700011,
Author = {Pratesi, Carlo and Esposito, Davide and Apostolou, Dimitrios and
   Attisani, Luca and Bellosta, Raffaello and Benedetto, Filippo and
   Blangetti, Ilaria and Bonardelli, Stefano and Casini, Andrea and
