@article{ WOS:000645049700009,
Author = {Roila, Russana and Branciari, Raffaella and Ranucci, David and Miraglia,
   Dino and Cristofani, Elise and Carloni, Cristiano and Pecorelli, Ivan
   and Fioroni, Laura},
Title = {Incidence of ionophore and non-ionophore anticoccidials residues in
   poultry meat and eggs and their risk characterization},
Journal = {ITALIAN JOURNAL OF FOOD SAFETY},
Year = {2021},
Volume = {10},
Number = {1},
Abstract = {A multi-residue method was applied to investigate the incidence and the
   concentration of ionophores and non-ionophore anticoccidials residues in
   poultry meat and hen eggs for the three-year period 2017-2019 in Italy.
   The risk related to the ingestion of such molecules was also
   characterized for the entire population. The average incidences of
   positive samples ranged from 1.35 to 9.45\% while the maximum average
   concentration was of 4.28 mu g/kg for non-ionophore molecules. No
   uncompliant sample was recorded. The overall risk characterization
   related to the intake of anticoccidials trought chicken meat and eggs
   reveal a minor concern for consumers of all age. However, the monitoring
   of coccidiostates residues through official control activity in poultry
   meat and egg is crucial and it should be continuously conducted to
   ensure safety of such products and safeguard consumers' health.},
