   Lestingi, Carmela and Romanelli, Sara and Rossi, Rosanna and Saluti,
   Giorgio and Moretti, Simone and Galarini, Roberta},
Title = {A GC-EI-MS/MS Method for the Determination of 15 Polybrominated Diphenyl
   Ethers (PBDEs) in Fish and Shellfish Tissues},
Journal = {FOOD ANALYTICAL METHODS},
