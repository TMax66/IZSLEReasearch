@article{ WOS:000536053400043,
Author = {Iammarino, Marco and Haouet, Naceur and Di Taranto, Aurelia and Berardi,
   Giovanna and Benedetti, Ferdinando and Di Bella, Sara and Chiaravalle,
   Antonio Eugenio},
Title = {The analytical determination of polyphosphates in food: A point-to-point
   comparison between direct ion chromatography and indirect photometry},
Journal = {FOOD CHEMISTRY},
Year = {2020},
Volume = {325},
Month = {SEP 30},
Abstract = {Being an indirect approach, the standard method for the determination of
   polyphosphates (PPs) in food maybe characterized by high variability
   which can lead to significant errors in the final measurement. In this
   study, this method was validated and then compared to the ion
   chromatography-based approach.
   Method trueness and measurement uncertainty resulted comparable, while
   the precision of ion chromatography approach was higher.
   No ``false positive{''} responses were obtained from ion chromatography
   analysis, while indirect photometry may cause ``false positive{''}
   results (5\% of analyzed samples). Regarding ``false negative{''}
   results, this possibility maybe verified for all food types tested and
