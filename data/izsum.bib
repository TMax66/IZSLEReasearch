@article{ WOS:000482994200040,
Author = {De Santis, Barbara and Debegnach, Francesca and Miano, Brunella and
   Moretti, Giorgio and Sonego, Elisa and Chiaretti, Antonio and Buonsenso,
   Danilo and Brera, Carlo},
Title = {Determination of Deoxynivalenol Biomarkers in Italian Urine Samples},
