@article{ ISI:000361902700012,
Author = {Moretti, Elena and Figura, Natale and Campagna, Maria Stella and
   Gonnelli, Stefano and Iacoponi, Francesca and Collodel, Giulia},
Title = {{Sperm Parameters and Semen Levels of Inflammatory Cytokines in
   Helicobacter pylori-infected Men}},
Journal = {{UROLOGY}},
Year = {{2015}},
Volume = {{86}},
Number = {{1}},
Pages = {{41-46}},
Month = {{JUL}},
Abstract = {{OBJECTIVE To explore the relationships between seminal interleukin 6
   (IL-6) and tumor necrosis factor alpha (TNF-alpha) with semen
   parameters, sperm apoptosis, and necrosis in subjects infected by
   Helicobacter pylori (HP) expressing and not expressing CagA.
   METHODS In 109 selected patients, HP infection and seminal levels of
   IL-6 and TNF-alpha were determined using enzyme-linked immunosorbent
   assays. Western blotting was used to detect antibodies to CagA. Semen
   parameters were determined following World Health Organization
   guidelines and sperm apoptosis and necrosis by annexin V and propidium
   iodide assay.
   RESULTS Twenty-eight subjects were infected by HP (HP+); among them, 12
   were CagA seropositive (CagA+) and 16 were negative (CagA-). Eighty-one
   men were HP seronegative (HP-). Semen TNF-alpha and IL-6 concentrations
   were increased in HP+ vs HP- groups (TNF-alpha: 41 pg/mL vs 27 pg/mL;
   IL-6: 11 pg/mL vs 5 pg/mL; P < .01). In comparison to the HP- group,
   CagA+ group showed reduced sperm motility (24\% vs 32\% motile sperm; P
   < .05), enhanced necrosis (33.5\% vs 21\% necrotic sperm; P < .05), and
   increased cytokines levels (TNF-alpha: 46 pg/mL vs 27 pg/mL; P < .01;
   IL-6: 17.5 pg/mL vs 5 pg/mL; P < .01). Sperm motility of CagA+ group was
   lower vs CagA- group (24\% vs 36.5\% motile sperm; P < .05). Both IL-6
   and TNF-alpha levels positively correlated with the percentage of
   necrotic sperm (P < .001).
   CONCLUSION CagA+ HP infection increases semen levels of inflammatory
   cytokines, which may reduce sperm motility and determine sperm damage
   and contribute to reduce the reproductive potential in men. (C) 2015
   Elsevier Inc.}},
