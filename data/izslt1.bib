Author = {Cinquina, AL and Longo, F and Anastasi, G and Giannetti, L and Cozzani,
   R},
Title = {{Validation of a high-performance liquid chromatography method for the
   determination of oxytetracycline, tetracycline, chlortetracycline and
   doxycycline in bovine milk and muscle}},
Journal = {{JOURNAL OF CHROMATOGRAPHY A}},
Year = {{2003}},
Volume = {{987}},
