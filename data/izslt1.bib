@article{ ISI:000374655200392,
Author = {Savini, Vincenzo and Franco, Alessia and Marrollo, Roberta and De
   Filippis, Roberta and Consilvio, Nicola Pietro and Donati, Valentina and
   Imperi, Monica and Pataracchia, Marco and Santarelli, Alessandro and
   Fazii, Paolo and Battisti, Antonio and Creti, Roberta},
Title = {{Sequence type 17 Streptococcus agalactiae with unusual `shell-like'
   phenotype was detected with Xpert (R) GBS assay}},
Journal = {{INTERNATIONAL JOURNAL OF CLINICAL AND EXPERIMENTAL MEDICINE}},
Year = {{2016}},
Volume = {{9}},
Number = {{2}},
Pages = {{3804-3807}},
Abstract = {{We report the isolation of a Streptococcus agalactiae (group B
   Streptococcus, GBS) strain showing a previously undescribed morphology.
   The isolate was collected during antenatal screening for GBS and formed
   colonies with irregular shapes and edges, along with a rough surface.
   Hence, we labeled it as `shell-like'. Although this strain was not
   recognized by eye as being GBS, given its unusual features, the Cepheid
   Xpert (R) GBS assay (Sunnyvale, US) allowed us to timely provide
   identification of it as S. agalactiae. The strain possessed the capsular
   serotype III and belonged to Sequence Type 17, which is the most
   represented in neonatal invasive infections. Unexpected GBS phenotypes
   may be responsible for falsely negative antenatal screening results,
   then for an increased risk for neonate disease and, consequently,
   newborn mortality.}},
