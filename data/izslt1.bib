@article{ ISI:A1997XV46600008,
Author = {Russo, MV and Neri, B},
Title = {{Fast analysis of clenbuterol and beta-agonist residues in bovine urine
   using GC-MS.}},
Journal = {{ANNALI DI CHIMICA}},
Year = {{1997}},
Volume = {{87}},
Number = {{7-8}},
Pages = {{505-512}},
Month = {{JUL-AUG}},
Abstract = {{A rapid and sensible analytical method for analysing clenbuterol and
   other beta-agonist residues in bovine urine is proposed. The method is
   based on solid phase extraction using porous silica derivatized with
   hydrofobic groups (C-18), derivatization of the extracted solutes with a
   BSTFA/TMCS mixture and analysis of the derivatives by gas chromatography
   - mass spectrometry using positive electron impact (EI) and selected ion
   monitoring (SIM). The recovery of clenbuterol, mabuterol and cimaterol
   is > 90\% with coefficient variation (CV) values between 3.0 and 3.5\%.
   The detection limit for clenbuterol is 0.1 ppb with a CV value of 8\%
   for SIM analysis.}},
