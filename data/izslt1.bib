Title = {{Non-toxigenic Corynebacterium ulcerans sequence types 325 and 339
   isolated from two dogs with ulcerative lesions in Italy}},
Journal = {{JOURNAL OF VETERINARY DIAGNOSTIC INVESTIGATION}},
Year = {{2018}},
Volume = {{30}},
