@article{ ISI:000459221900028,
Author = {Rau, Julietta V. and Curcio, Mariangela and Raucci, Maria Grazia and
   Barbaro, Katia and Fasolino, Ines and Teghil, Roberto and Ambrosio,
   Luigi and De Bonis, Angela and Boccaccini, Aldo R.},
Title = {{Cu-Releasing Bioactive Glass Coatings and Their in Vitro Properties}},
Journal = {{ACS APPLIED MATERIALS \& INTERFACES}},
Year = {{2019}},
