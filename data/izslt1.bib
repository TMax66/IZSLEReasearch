@article{ ISI:000248276300154,
Author = {Ballico, S. and Failla, S. and Iacurto, M. and Conto, M. and Signorelli,
   F. and Ficco, A.},
Title = {{Effect of packaging and aging time on shelf-life of beef meat}},
