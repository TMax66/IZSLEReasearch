@article{ ISI:000331980300203,
Author = {Alfieri, Lavinia and Fagiolo, Antonio and Roncoroni, Cristina and
   Zottola, Tiziana and Cavallina, Roberta and Lai, Olga},
Title = {{A Preliminary Study about Lymphocyte Subset of Water Buffalo Calves}},
Journal = {{BUFFALO BULLETIN}},
Year = {{2013}},
Volume = {{32}},
