
@article{ ISI:000648343800028,
Author = {Graziani, Gabriela and Barbaro, Katia and Fadeeva, V, Inna and Ghezzi,
   Daniele and Fosca, Marco and Sassoni, Enrico and Vadala, Gianluca and
   Cappelletti, Martina and Valle, Francesco and Baldini, Nicola and Rau,
