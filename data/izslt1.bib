@article{ ISI:000232015600007,
Author = {Ciabatti, I and Marchesi, U and Froiio, A and Paterno, A and Ruggeri, M
   and Amaddeo, D},
Title = {{Role of the ``National Reference Centre for Genetically Modified
   Organisms (GMO) detection{''} in the official control of food and feed}},
