@article{ ISI:000339602500025,
Author = {Delibato, Elisabetta and Rodriguez-Lazaro, David and Gianfranceschi,
   Monica and De Cesare, Alessandra and Comin, Damiano and Gattuso,
   Antonietta and Hernandez, Marta and Sonnessa, Michele and Pasquali,
