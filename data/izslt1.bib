@article{ ISI:000467667300011,
Author = {Eleni, Claudia and Neri, Bruno and Giannetti, Luigi and Grifoni,
   Goffredo and Meoli, Roberta and Stravino, Fiorentino and Friedrich,
   Klaus Gunther and Scholl, Francesco and Di Cerbo, Pilar and Battisti,
   Antonio},
Title = {{Death of captive-bred vultures caused by flunixin poisoning in Italy}},
Journal = {{ENVIRONMENTAL TOXICOLOGY AND PHARMACOLOGY}},
Year = {{2019}},
Volume = {{68}},
Pages = {{91-93}},
Month = {{MAY}},
Abstract = {{Among non steroidal anti-inflammatory drugs (NSAIDs) diclofenac is
   considered the main cause for the decline of vulture populations in the
   Indian subcontinent since the `90 s. Chemical analysis showed high
   levels of flunixin (31,350 mu g/kg) in beef which three captive Gyps
   vultures fed on, later dying with severe visceral gout. Levels in dead
   vultures' organs and tissues ranged from 4 to 38.5 mu g/kg. The typical
   lesions and the concentrations found in beef indicate flunixin as the
   cause of death. This is the first observational study which correlates
   the concentration of flunixin in the meat ingested with that found in
   tissues of vultures.}},
