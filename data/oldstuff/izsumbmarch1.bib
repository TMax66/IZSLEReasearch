@article{ ISI:000332192500003,
Author = {Moretti, A. and Agnetti, F. and Mancianti, F. and Nardoni, S. and Righi,
   C. and Moretta, I. and Morganti, G. and Papini, M.},
Title = {{Dermatophytosis in animals: epidemiological, clinical and zoonotic
   aspects}},
