@article{ ISI:000343961100003,
Author = {Galarini, Roberta and Saluti, Giorgio and Giusepponi, Danilo and Rossi,
   Rosanna and Moretti, Simone},
Title = {{Multiclass determination of 27 antibiotics in honey}},
