@article{ ISI:000386668900022,
Author = {Torricelli, Martina and Pierboni, Elisa and Tovo, Gloria Raquel and
   Curcio, Ludovica and Rondini, Cristina},
Title = {{In-house Validation of a DNA Extraction Protocol from Honey and Bee
   Pollen and Analysis in Fast Real-Time PCR of Commercial Honey Samples
   Using a Knowledge-Based Approach}},
