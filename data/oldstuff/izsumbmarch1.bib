@article{ ISI:000230355600005,
Author = {Bertozzini, E and Penna, A and Pierboni, E and Bruce, I and Magnani, M},
Title = {{Development of new procedures for the isolation of phytoplankton DNA
   from fixed samples}},
Journal = {{JOURNAL OF APPLIED PHYCOLOGY}},
