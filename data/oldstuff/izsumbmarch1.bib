@article{ ISI:000353316300031,
Author = {Di Guard, G. and Falconi, A. and Di Francesco, A. and Mazzariol, S. and
   Centelleghe, C. and Casalone, C. and Pautasso, A. and Cocumelli, C. and
   Eleni, C. and Petrella, A. and Di Francesco, C. E. and Sabatucci, A. and
