@article{ ISI:000220934300003,
Author = {Giammarini, C and Andreoni, F and Amagliani, G and Casiere, A and
   Barocci, S and Magnani, M},
Title = {{Purification and characterization of a recombinant listeriolysin O
   expressed in Escherichia coli and possible diagnostic applications}},
