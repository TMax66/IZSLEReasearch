@article{ ISI:000295986900003,
Author = {Moretta, I. and Veronesi, F. and Di Paola, R. and Battistacci, L. and
   Moretti, A.},
Title = {{Parasitological survey on wild boar (Sus scrofa) shut down in the
   hunting season 2009-2010 in Umbria (central Italy)}},
Journal = {{LARGE ANIMAL REVIEW}},
Year = {{2011}},
Volume = {{17}},
Number = {{5}},
Pages = {{187-192}},
Month = {{OCT}},
Abstract = {{Wild boar represents one of the most common and widespread wildlife and
   wild game species in Umbria region. Aim of work was to investigate the
   parasitofauna of wild boars living in Umbria analyzing the faecal
   samples of 123 animals shut down during the hunting season 2009/2010 by
   quali-quantitative coprological examinations. The results of the
   coprological analyses showed that gastro-intestinal strongyles (73.98\%)
   and coccidia (33.33\%) represented the predominant parasites and the
   most common parasitic association recovered. Moreover the statistical
   analysis revealed a significant correlation between the coccidia
   infection rate and the animal age (p<0.05). Copropositivity for further
   parasites including lungworms (8.94\%), acanthocephalans (6.55\%),
   Trichuris suis (4.88\%), Capillaria spp. (2.44\%), Spiruridae (1.63\%),
   Ascaris suum (0.81\%) and Strongyloides ransomi (0.81\%), was also
   detected. The examinations of the whole intestinal tracts and
   respiratory tracts of 16 wild boars allowed to confirm that
   gastro-intestinal strongyles were the most prevalent parasites of the
   wild boar and to identify three different species of gastro-intestinal
   strongyles (Oesophagostomum dentatum, Hyostrongylus rubidus,
   Globocephalus urosubulatus) and two of lungworms (Metastrongylus apri,
   Metastrongylus pudendotectus).}},
