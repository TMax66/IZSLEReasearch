@article{ ISI:000268119300005,
Author = {Lopedota, Angela and Trapani, Adriana and Cutrignelli, Annalisa and
   Chiarantini, Laura and Pantucci, Elena and Curci, Rosa and Manuali,
   Elisabetta and Trapani, Giuseppe},
Title = {{The use of Eudragit (R) RS 100/cyclodextrin nanoparticles for the
   transmucosal administration of glutathione}},
