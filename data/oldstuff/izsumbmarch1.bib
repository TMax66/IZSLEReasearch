@article{ ISI:000303595800004,
Author = {Amagliani, Giulia and Petruzzelli, Annalisa and Omiccioli, Enrica and
   Tonucci, Franco and Magnani, Mauro and Brandi, Giorgio},
Title = {{Microbiological Surveillance of a Bovine Raw Milk Farm Through Multiplex
   Real-Time PCR}},
Journal = {{FOODBORNE PATHOGENS AND DISEASE}},
