@inproceedings{ ISI:000240052100014,
Author = {Beghelli, D. and D'Alterio, G. L. and Severi, G. and Moscati, L. and
   Pezzotti, G. and Foglini, A. and Battistacci, L. and Cagiola, M. and
   Ayala Vargas, C.},
