@article{ WOS:000277974700005,
Author = {Sartore, S. and Bonfanti, L. and Lorenzetto, M. and Cecchinato, M. and
   Marangon, S.},
Title = {The effects of control measures on the economic burden associated with
   epidemics of avian influenza in Italy},
Journal = {POULTRY SCIENCE},
Year = {2010},
Volume = {89},
Number = {6},
Pages = {1115-1121},
Month = {JUN 1},
Abstract = {In 1999, Italy experienced a devastating epidemic of high-pathogenicity
   avian influenza (HPAI) caused by an H7N1 virus subtype. After this
   epidemic, a ministerial decree was passed to implement control measures
   for low-pathogenicity avian influenza (LPAI) due to H5 and H7 subtypes.
   We investigated whether these control measures have decreased the public
   expenditure associated with epidemics of LPAI and HPAI by comparing the
   direct and consequential losses of the 1999 epidemic to the losses
   associated with successive epidemics. The estimated total economic
   burden of the epidemics was about (sic)650 million ((sic)217 million in
   direct losses and (sic)433 million in consequential losses). The 1999
   epidemic accounted for most of these losses ((sic)507 million: (sic)112
   million in direct losses and (sic)395 million in consequential losses),
   whereas the total economic burden for the 5 successive LPAI was (sic)143
   million ((sic)105 million in direct losses and (sic)38 million in
   consequential losses). These results demonstrate that the implementation
   of a coordinated set of disease-control measures, which included both
   emergency and prophylactic vaccination, was able to reduce the overall
   costs associated with avian influenza epidemics. The results also show
   that the application of adequate LPAI control measures may limit the
   risk of emergence of an HPAI virus in an area with a high poultry
   density, allowing the complete disruption of the poultry market and its
   huge associated costs to be avoided.},
