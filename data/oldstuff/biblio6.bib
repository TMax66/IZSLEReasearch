@article{ WOS:000250775800007,
Author = {Scacchia, M. and Sacchini, F. and Filipponi, G. and Luciani, M. and
   Lelli, R. and Tjipura-Zaire, G. and Di Provvido, A. and Shiningwane, A.
   and Ndiipanda, F. and Pini, A. and Caporale, V. and Huebschle, O. J. B.},
Title = {Clinical, humoral and IFN gamma responses of cattle to infection with
   Mycoplasma mycoides var. mycoides small colony and attempts to condition
   the pathogenesis of the infection},
Journal = {ONDERSTEPOORT JOURNAL OF VETERINARY RESEARCH},
Year = {2007},
Volume = {74},
Number = {3},
Pages = {251-263},
Month = {SEP},
Abstract = {Contagious bovine pleuropneumonia (CBPP), caused by Mycoplasma mycoides
   var. mycoides small colony (MmmSC), is one of the most important
   diseases of cattle in Africa. The role of innate or acquired cell
   mediated and humoral immunity in conferring protection against MmmSC
   infection has not yet been elucidated. On the other hand, the
   pathological lesions caused by the aetiological agent have been
   considered indicative of an immunopathological process. In this study
   ten naive cattle were exposed to in-contact infection with animals
   infected by intubation with a strain of MmmSC. Clinical signs, antibody
   response, IFN gamma release and pathological changes at necropsy were
   analysed and compared with the events following in-contact infection of
   an equal number of animals kept under daily treatment with cyclosporine
   for the entire observation period of 84 days. Cyclosporine is a
   suppressor of the immune response related to the T-cell system. Under
   the conditions of the experiment, cyclosporine appeared to condition the
   pathogenesis of CBPP by delaying the events that follow infection,
   bringing further support to the possibility that the immune response may
   have an impact on the disease outcome.},
