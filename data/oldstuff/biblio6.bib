@article{ WOS:000260399500011,
Author = {Camarda, Antonio and Circella, Elena and Pennelli, Donato and Battista,
   Patrizia and Di Paola, Giancarlo and Madio, Anna and Tagliabue, Silvia},
Title = {Occurrence of pathogenic and faecal Escherichia coli in layer hens},
