@article{ WOS:000286226100007,
Author = {Vesely, T. and Cinkova, K. and Reschova, S. and Gobbo, F. and Ariel, E.
   and Vicenova, M. and Pokorova, D. and Kulich, P. and Bovo, G.},
Title = {Investigation of ornamental fish entering the EU for the presence of
   ranaviruses},
Journal = {JOURNAL OF FISH DISEASES},
Year = {2011},
Volume = {34},
Number = {2},
Pages = {159-166},
Month = {FEB},
Abstract = {A survey was performed on ornamental fish imported into the EU to detect
   viral agents belonging to the genus Ranavirus. The objective was to gain
   knowledge of the potential for these systemic iridoviruses to gain entry
   into the EU via international trade in ornamental fish. A total of 208
   pooled samples, representing 753 individual fish, were tested. The
   samples included 13 orders and 37 families, originating from different
   countries and continents. Tissues from fish that died during or just
   after transport were collected and examined by standard virological
   techniques in epithelioma papulosum cyprini cells, by transmission
   electron microscopy and by PCR for the detection of the major capsid
   protein and DNA polymerase gene sequences of ranaviruses. Virus was
   isolated from nine fish species but ranavirus was not identified in
   those samples. The results suggest that ranaviruses are not highly
   prevalent in ornamental fish imported into the EU.},
