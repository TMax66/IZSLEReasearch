@article{ WOS:000276455300107,
Author = {Pozza, Manuela Dalla and Valerii, Leila and Graziani, Manuel and
   Ianniello, Marco and Bagni, Marina and Damiani, Silvia and Ravarotto,
   Licia and Busani, Luca and Ceolin, Chiara and Terregino, Calogero and
