@article{ WOS:000277332700008,
Author = {Camma, Cesare and Ferri, Nicola and Zezza, Daniela and Marcacci,
   Maurilia and Paolini, Alessandra and Ricchiuti, Luciano and Lelli,
   Rossella},
Title = {Confirmation of crayfish plague in Italy: detection of Aphanomyces
   astaci in white clawed crayfish},
Journal = {DISEASES OF AQUATIC ORGANISMS},
Year = {2010},
Volume = {89},
Number = {3},
Pages = {265-268},
Month = {APR 9},
Abstract = {In the summer of 2009, high levels of mortality among white clawed
   crayfish Austropotamobius pallipes were observed in 3 watercourses of
   central Italy. PCR and culture methods were used to detect the causative
   agent of the disease. Two strains of Aphanomyces spp. were isolated and
   identified by PCR and DNA sequencing as Aphanomyces astaci and A.
