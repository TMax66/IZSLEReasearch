@article{ WOS:000289996800004,
Author = {Mariano, V. and Bagni, M.},
Title = {Italian research on animal health goes to Europe},
Journal = {LARGE ANIMAL REVIEW},
Year = {2010},
Volume = {16},
Number = {4},
Pages = {179-183},
Month = {AUG},
Abstract = {The prevention and control of disease outbreaks is essential to protect
   and raise the health status and condition of food-producing animals in
   the EU. An important result can rise through the strengthening of the
   research activities in the European Animal Health sector such as the
   development of new monitoring strategies, diagnostics and therapies. In
   order to build up a communal research agenda and mutual research
   activities on this sector, the EMIDA (Emerging and Major Infectious
   Disease of Livestock) ERAnet (European Research Area network) project
   collected information on the European Animal Health landscape through a
   questionnaire and carried out a study to identify perceived needs of the
   EU Animal Health Area. The information, collected from 19 countries, was
   useful for the organization of a pilot trans-national call opened the 7
   (th) September 2009. The call makes available about 20 million Euros for
   research projects on the following topics: vector borne disease;
   zoonosis and antibiotic resistance; major infectious disease of
   livestock; aquaculture. The participation of Italian Research Institutes
   to the call has been copious and represents a great opportunity to
   develop a durable focused network with others European research
   organizations.},
