@article{ WOS:000270246800020,
Author = {Moreno, Ana and Barbieri, Ilaria and Sozzi, Enrica and Luppi, Andrea and
   Lelli, Davide and Lombardi, Guerino and Zanoni, Maria Grazia and
   Cordioli, Paolo},
Title = {Novel swine influenza virus subtype H3N1 in Italy},
Journal = {VETERINARY MICROBIOLOGY},
Year = {2009},
Volume = {138},
Number = {3-4},
Pages = {361-367},
Month = {SEP 18},
Abstract = {To date, three subtypes of swine influenza viruses, H1N1, H1N2, and H3N2
   have been isolated in Italy. In 2006, a novel swine influenza virus
   subtype (H3N1) was isolated from coughing pigs. RT-PCR performed on lung
   tissues, experimental infection in pigs with the novel isolate, and
   cloning the virus by plaque assay confirmed this unique H and N
   combination. The novel isolate was also antigenically and genetically
   characterized. Genetic and phylogenetic analysis showed that the
   complete HA gene of the H3N1 strain has the highest nucleotide identity
   to three Italian H3N2 strains, one isolated in 2001 and two in 2004,
   whereas the full length NA sequence is closely related to three H1N1
   subtype viruses isolated in Italy in 2004. The remaining genes are also
   closely related to respective genes found in H1N1 and H3N2 SIVs
   currently circulating in Italy. This suggests that the novel SIV could
   be a reassortant between the H3N2 and H1N1 SIVs circulating in Italy.
   (C) 2009 Elsevier B.V. All rights reserved.},
