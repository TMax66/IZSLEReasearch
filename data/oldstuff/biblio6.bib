@article{ WOS:000208233300004,
Author = {Dorigatti, I. and Mulatti, P. and Rosa, R. and Pugliese, A. and Busani,
   L.},
Title = {Modelling the spatial spread of H7N1 avian influenza virus among poultry
   farms in Italy},
Journal = {EPIDEMICS},
Year = {2010},
Volume = {2},
Number = {1},
Pages = {29-35},
Month = {MAR},
Abstract = {We analysed the between-farm transmission of the H7N1 highly pathogenic
   avian influenza virus that disrupted the Italian poultry production in
   the 1999-2000 epidemic with a SEIR model with a spatial transmission
   kernel, accounting for the containment measures actually undertaken. We
   found significant differences in susceptibility between species and a
   reduction in transmissibility after the first phase. We performed
   simulations to assess the effectiveness of the implemented and new
   control measures. The most effective measure was the ban on restocking.
   An earlier start of pre-emptive culling promotes eradication; restricted
   pre-emptive culling delays eradication but causes lower losses. (C) 2010
   Elsevier B. V. All rights reserved.},
