@article{ WOS:000251479400021,
Author = {Caccio, Simone M. and Rinaldi, Laura and Cringoli, Giuseppe and
   Condoleo, Renato and Pozio, Edoardo},
Title = {Molecular identification of Cryptosporidium parvum and Giardia
   duodenalis in the Italian water buffalo (Bubalus bubalis)},
Journal = {VETERINARY PARASITOLOGY},
Year = {2007},
Volume = {150},
Number = {1-2},
Pages = {146-149},
Month = {NOV 30},
Abstract = {Livestock are commonly infected with protozoan parasites of the genera
   Cryptosporidium and Giardia, and some of the species and genotypes found
   in these animals have zoonotic significance. We characterized isolates
   of both parasites recovered from the Italian water buffalo (Bubalus
   bubalis), an economically important species whose milk is used for the
   production of ``buffalo mozzarella{''} fresh cheese. Molecular analysis
   of the Cryptosporidium small subunit ribosomal DNA gene and of the
   Giardia beta-giardin gene shows the presence of both zoonotic parasites
   (Cryptosporidium parvum and Giardia duodenalis assemblage A) and
   host-specific parasites (G. duodenalis assemblage E), suggesting that
   water buffaloes can contribute to environmental contamination with
   oocysts and cysts potentially infectious to humans if their faeces are
   improperly disposed of. On the other hand, mozzarella cheese is probably
   a safe product, given that its production involves the treatment of
   cheese curd at 85-95 degrees C, which is likely to kill or inactivate
   the parasites. (c) 2007 Elsevier B.V. All rights reserved.},
