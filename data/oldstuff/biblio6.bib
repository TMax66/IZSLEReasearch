@article{ WOS:000289405500002,
Author = {Grattarola, C. and Iulini, B. and Zoppi, S. and Varello, K. and Dondo,
   A. and Rumello, G. and Baglivo, T. and Rondoletti, M. and Savini, G. and
   Amorisco, F. and Casalone, C.},
Title = {Investigation on three outbreaks of malignant catarrhal fever in cattle
   in Piedmont},
Journal = {LARGE ANIMAL REVIEW},
Year = {2011},
Volume = {17},
Number = {2},
Pages = {49-55},
Month = {APR},
Abstract = {Malignant catarrhal fever (MCF) is a severe, fatal disease of cattle and
   other domestic and wild ruminants and exhibited a long incubation
   period. Aetiological agents of MCF belong to subfamily
   gamma-Herpesvirinae, gen. Rhadinovirus and are represented by AIHV-1
   (Alcelaphine herpes virus type 1) e OvHV-2 (Ovine herpesvirus type 2);
   in Europe the disease is frequently associated with the transmission of
   a gamma-herpesvirus, the ovine herpesvirus type 2 (OvHV-2).
   MCF usually appears sporadically and affects few animals; infection
   occur through via oronasal and it is characterized by accumulation of
   lymphocytes in various tissues. However, the pathogenic basis of the
   disease is still an enigma.
   The present study describes three outbreaks of MCF in cattle reared in
   Piedmont.
   During Spring - Summer 2010, post-mortem examination on five animals
   with clinical signs ascribed to MCF was conducted. A full necropsy was
   performed and brain and tissue samples from all organs were
   systematically collected for histology. Bacteriological and virological
   examinations were also carried out.
   Results obtained by anatomo-histopathological investigation and
