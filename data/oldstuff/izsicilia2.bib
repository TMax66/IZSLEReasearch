@article{ ISI:000181139000004,
Author = {Ziino, G and Giuffrida, A and Stancanelli, A and Panebianco, A},
Title = {{Shell disease in Nephrops norvegicus from the Mediterranean sea.
   Morphological and hygienic remarks}},
Journal = {{ARCHIV FUR LEBENSMITTELHYGIENE}},
Year = {{2002}},
Volume = {{53}},
Number = {{6}},
Pages = {{134-136}},
Month = {{NOV-DEC}},
Abstract = {{Six hundred specimens of Nephrops norvegicus from the Mediterranean Sea
   were examined for shell disease. Six specimens (1\%) were found positive
   for erosive exoskeleton lesions. The lesions were only found on the
   chelae and ranged from 1 mm to 15 mm in diameter. The smaller lesions
   were clearly defined circular, whitish areas, affecting the superficial
   exoskeleton; the wider lesions had irregular borders and contained
   necrotic material. The histo-pathological findings were similar to those
   described for shell disease of other crustaceans. Pseudomonas spp. were
   isolated from all lesions; in one case they were associated with
   Enterobacter agglomerans. Finally, the potential hygienic-sanitary
   implications related to the described pathology were discussed.}},
