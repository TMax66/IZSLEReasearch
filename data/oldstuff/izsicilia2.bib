@article{ ISI:000248276300231,
Author = {Reale, S. and Vitale, F. and Scatassa, M. L. and Caracappa, S. and
   Curro, V. and Todaro, M.},
Title = {{Molecular characterization of dominant bacterial population in
   ``Vastedda della Valle del Belice{''} cheese: preliminary investigation}},
Journal = {{ITALIAN JOURNAL OF ANIMAL SCIENCE}},
Year = {{2007}},
Volume = {{6}},
