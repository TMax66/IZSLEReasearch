@article{ ISI:000349269800009,
Author = {Barnard, Shanis and Matthews, Lindsay R. and Messori, Stefano and
   Vulpiani, Michele Podaliri and Ferri, Nicola},
Title = {{Behavioural reactivity of ewes and lambs during partial and total social
   isolation}},
Journal = {{APPLIED ANIMAL BEHAVIOUR SCIENCE}},
Year = {{2015}},
Volume = {{163}},
Pages = {{89-97}},
Month = {{FEB}},
Abstract = {{This work investigated the differences in the reactivity of Sarda
   (primiparous n=18, multiparous n=17) and Dorset (multiparous n=8) breeds
   of sheep and their singleton lambs to two challenging test situations
   involving a mother-lamb partial separation test and an isolation test.
   Non-parametric analysis used single behavioural variables and fear
   scores to evaluate the effect of parity, sex of lambs, and the
   association between mother-lamb behaviour. Amongst ewes, Dorset were
   characterised by a more calm temperament while Sarda (especially
   primiparous ewes) were more active in their response to challenge (i.e.
   more attempts to escape). As with their dams, lambs reflected to a
   certain extent this divergence and overall during isolation lamb fear
   score was on average significantly higher than dams. Correlations
   between measures of behavioural reactivity across tests were carried out
   to search for predictive measures of fear. A very strong correlation
   emerged linking vocalisation to locomotor activity. Vocalisation could
   be a good candidate as predictor factor of an active reaction of sheep
   to a fearful.situation. (C) 2014 Elsevier B.V. All rights reserved.}},
