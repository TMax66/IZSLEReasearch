@article{ ISI:A1996VN48300007,
Author = {Savini, G and Dunsmore, JD and Robertson, ID},
Title = {{Studies on pathogenesis, tissue infection and congenital transmission in
   cows experimentally infected with Sarcocystis cruzi by various routes}},
Journal = {{VETERINARY PARASITOLOGY}},
Year = {{1996}},
Volume = {{64}},
Number = {{4}},
Pages = {{319-327}},
Month = {{SEP 16}},
Abstract = {{A group of nine cows, naturally infected with Sarcocystis, were
   challenged with Sarcocystis cruzi: three intrarumenally with sporocysts,
   two intrarumenally with water (controls), two intravenously with
   merozoites grown in vitro and two intravenously with saline solution
   (controls). The animals intrarumenally challenged with sporocysts
   developed acute sarcocystiosis and produced stillborn calves, whereas
   those intravenously challenged with merozoites suffered from subclinical
   sarcocystiosis with premature births. No parasites were found in calves
   from cows challenged with sporocysts; however, a meront of Sarcocystis
   was found within a macrophage in the cerebrospinal fluid of a calf from
   a cow intravenously inoculated with merozoites of S. cruzi. This is the
