@inproceedings{ ISI:000234682200050,
Author = {Savini, G and Goffredo, M and Monaco, F and Di Gennaro, A and de Santis,
   P and Meiswinkel, R and Caporale, V},
Editor = {{MacLachlan, NJ and Pearson, JE}},
Title = {{The isolation of bluetongue virus from field populations of the
   Obsoletus Complex in central Italy}},
Booktitle = {{BLUETONGUE, PT 1, PROCEEDINGS}},
Series = {{VETERINARIA ITALIANA (SERIES)}},
Year = {{2004}},
Volume = {{40}},
