@inproceedings{ ISI:000234682200043,
Author = {Giovannini, A and Cahstri, P and Nannini, D and Paladini, C and
   Santucci, U and Patta, C and Caporale, V},
Editor = {{MacLachlan, NJ and Pearson, JE}},
Title = {{Bluetongue in Italy: Part II}},
