@inproceedings{ ISI:000234682200044,
Author = {Goffredo, M and Meiswinkel, R},
Editor = {{MacLachlan, NJ and Pearson, JE}},
Title = {{Entomological surveillance of bluetongue in Italy: methods of capture,
   catch analysis and identification of Culicoides biting midges}},
Booktitle = {{Bluetongue, Pt 1, Proceedings}},
