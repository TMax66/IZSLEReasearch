@article{ ISI:000328378400005,
Author = {Giangaspero, Massimo and Savini, Giovanni and Orusa, Riccardo and Osawa,
   Takeshi and Harasawa, Ryo},
Title = {{Prevalence of antibodies against Parainfluenza virus type 3, Respiratory
   syncitial virus and bovine Herpesvirus type 1 in sheep from Northern
   Prefectures of Japan}},
Journal = {{VETERINARIA ITALIANA}},
Year = {{2013}},
Volume = {{49}},
Number = {{3}},
Pages = {{285-289}},
Month = {{JUL-SEP}},
Abstract = {{Ovine sera collected in the Prefectures of Hokkaido, Aomori and Iwate in
   the Northern Japan were examined for the presence of antibodies against
   Respiratory syncytial virus (RSV), bovine Herpesvirus type 1 (infectious
   bovine rhinotracheitis: IBR) and Parainfluenza virus type 3 (PIV3) using
   serum neutralisation (SN) and enzyme-linked immunosorbent assay (ELISA)
   tests. Twenty-three animals (11.73\%) out of the 196 tested were
   sero-positive to PIV3. Sixteen animals (8.69\%) out of the 184 tested
   reacted to RSV. No animals were positive to IBR antigen.
   Sero-conversions to PIV3 were detected in Hokkaido and Iwate (14.92\%
   and 8.82\%, respectively). Antibodies against RSV were detected in
   Hokkaido (9.23\%) and Aomori (14.28\%). Although no diagnostic measures
   were in place, the infections did not appear to be related to any
   reduction in sheep productivity.}},
