@article{ ISI:A1994PF32100002,
Author = {CAPUA, I and GOUGH, RE and MANCINI, M and CASACCIA, C and WEISS, C},
Title = {{A NOVEL INFECTIOUS-BRONCHITIS STRAIN INFECTING BROILER-CHICKENS IN ITALY}},
