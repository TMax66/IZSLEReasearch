Title = {{High antibody titres against predicted Mycoplasma surface proteins do
   not prevent sequestration in infected lung tissue in the course of
   experimental contagious bovine pleuropneumonia}},
Journal = {{VETERINARY MICROBIOLOGY}},
Year = {{2014}},
Volume = {{172}},
