@article{ ISI:000267588000001,
Author = {Hugas, Marta and Tsigarida, Eirini and Robinson, Tobin and Calistri,
   Paolo},
Title = {{The EFSA Scientific Panel on Biological Hazards first mandate: May
   2003-May 2006. Insight into foodborne zoonoses}},
Journal = {{TRENDS IN FOOD SCIENCE \& TECHNOLOGY}},
