@article{ ISI:000314464600040,
Author = {Varela, Mariana and Schnettler, Esther and Caporale, Marco and Murgia,
   Claudio and Barry, Gerald and McFarlane, Melanie and McGregor, Eva and
   Piras, Ilaria M. and Shaw, Andrew and Lamm, Catherine and Janowicz, Anna
