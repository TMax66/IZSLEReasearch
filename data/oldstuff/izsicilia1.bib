@article{ ISI:000277211600004,
Author = {Alloggio, Ingrid and De Ruvo, Giuseppe and Torina, Alessandra and
   Caroli, Anna and Petazzi, Ferruccio and Pieragostini, Elisa},
Title = {{Reversible switch from hemoglobin A to C in sheep and recovery from
   anemia following experimental infection with Anaplasma ovis}},
Journal = {{ITALIAN JOURNAL OF ANIMAL SCIENCE}},
Year = {{2009}},
Volume = {{8}},
