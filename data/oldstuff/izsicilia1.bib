@article{ ISI:000287529000003,
Author = {Loria, G. R. and Puleio, R. and Agnello, S. and Chetta, M. and Tola, S.
   and Nicholas, R. A. J.},
Title = {{Infectious kerato-conjunctivitis (IKC) caused by Mycoplasma
   conjunctivae: a new risk for Italian sheep farming}},
Journal = {{LARGE ANIMAL REVIEW}},
Year = {{2011}},
Volume = {{17}},
Number = {{1}},
Pages = {{13-17}},
Month = {{FEB}},
Abstract = {{In small ruminant livestock, ocular diseases cause severe economical
   losses due to costs of therapies and difficulties to manage blind
   animals to pasture. In Italy, in the last years, among ocular pathogens,
   a recently classified mycoplasma species, Mycoplasma conjunctivae, is
   responsible of several outbreaks of infectious keratoconjunctivitis
   (IKC). The disease infects only ocular tissues causing very severe
   symptoms ( and death for starvation) in wild ruminants; in domestic
   livestock it occurs in lactating and weakening animals and is normally
   characterised by benign course and complete recovering of the vision in
   few weeks. Laboratory confirmation is mainly based on bio-molecular
   methods (PCR, DGGE) which reduce the time needed for diagnosis and
   overcome all difficulties relate to isolation procedures. Prophylaxis
   concerns a quarantine period for all new introduced animals and it seems
   to be the only measure applicable against the spreading of the disease.
   Control is normally performed by utilization of eye drops with
   antibiotic or ointments are helpful to resolve the clinical
