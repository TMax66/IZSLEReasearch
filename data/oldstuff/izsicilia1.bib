@article{ ISI:000316039300019,
Author = {Ressel, Lorenzo and Puleio, Roberto and Loria, Guido Ruggero and
   Vannozzi, Iacopo and Millanta, Francesca and Caracappa, Santo and Poli,
   Alessandro},
Title = {{HER-2 expression in canine morphologically normal, hyperplastic and
   neoplastic mammary tissues and its correlation with the clinical outcome}},
Journal = {{RESEARCH IN VETERINARY SCIENCE}},
