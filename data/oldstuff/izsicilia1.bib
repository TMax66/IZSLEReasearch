@article{ ISI:000346128700011,
Author = {Giammanco, Giovanni M. and Di Bartolo, Ilaria and Purpari, Giuseppa and
   Costantino, Claudio and Rotolo, Valentina and Spoto, Vittorio and
   Geraci, Gaetano and Bosco, Girolama and Petralia, Agata and Guercio,
