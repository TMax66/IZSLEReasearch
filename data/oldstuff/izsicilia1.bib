@article{ ISI:000355776500003,
Author = {Mazzei, Veronica and Longo, Sabrina and Conte, Francesca and Pecoraro,
   Roberta and Sal-Vaggio, Antonio and Tibullo, Daniele and Tiralongo,
   Francesco and Lombardo, Bianca Maria and Brundo, Maria Violetta},
Title = {{EFFECTS OF TRIBUTYLTIN AND DIBUTYLTIN ON SPERM MOTILITY OF Mytilus
   galloprovincialis (Mollusca: Mytilidae)}},
Journal = {{THALASSAS}},
Year = {{2015}},
Volume = {{31}},
Number = {{2}},
Pages = {{31-37}},
Month = {{JUL}},
Abstract = {{The organotin compounds, in particular tributyltin (TBT) and dibutyltin
   (DBT), are classified among the most harmful xenobiotics introduced by
   man in the aquatic environment. Despite the several studies conducted,
   the mechanism of their toxic action is not still clarified. Being
   lipophilic substances, they interact with the phospholipid component of
   cell membranes, determining changes in physico-chemical properties and
   in the activity of membrane proteins, with serious repercussions on
   important cellular functions.
   Our research aims to investigate, through a spermiotoxicity test, the
   influence in vivo of TBT and DBT on the bivalve mollusc Mytilus
   galloprovincialis. Different concentrations of two contaminants were
   tested varying exposure times. The results show that exposure to
   contaminants causes, even at the lowest concentrations tested and after
   only 1 h of exposure, a total block of sperm motility that could affect
   reproductive success in the natural habitat of the species. This block
   is due to a structural alteration of the beta-tubulin, highlighted by
   western blotting, which undergoes proteolytic cleavage. Therefore,
   beta-tubulin could be used as a biomarker of effect because there are
