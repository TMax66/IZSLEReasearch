@article{ ISI:000264952300005,
Author = {Amadori, Massimo and Farinacci, Maura and Begni, Barbara and Faita,
   Roberto and Podavini, Damiano and Colitti, Monica},
Title = {{Effects of Interferon-alpha on the Inflammatory Response of Swine
   Peripheral Blood Mononuclear Cells}},
Journal = {{JOURNAL OF INTERFERON AND CYTOKINE RESEARCH}},
Year = {{2009}},
Volume = {{29}},
Number = {{4}},
Pages = {{241-247}},
Month = {{APR}},
Abstract = {{Interferon-alpha (IFN-alpha) at low concentrations had been previously
   shown to control the expression of inflammatory cytokine genes in swine
   pulmonary alveolar macrophages. In the first part of this study,
   cultured swine peripheral blood mononuclear cells (PBMCs) were
   supplemented with IFN-alpha at low/moderate concentrations, and then
   stimulated with lipopolysaccharide (LPS). The expression of IFN-alpha,
   IFN-gamma, IL-1 beta, TNF-alpha, and IL-6 genes was determined by
   real-time PCR. IFN-alpha at low/moderate concentrations did not
   significantly reduce the expression of any cytokine gene under study,
   with clear trends though to a concentration-dependent reduction of IL-1
   beta gene expression and to a concentration-dependent increase of
   IFN-gamma gene expression. In vivo, orally administered IFN-alpha was
   shown instead to modulate the inflammatory response to early weaning in
   uncultured PBMCs of specific pathogen-free piglets. As opposed to the in
   vitro model, the oral IFN-alpha treatment reduced after weaning the
   expression of the IFN-gamma gene (P < 0.08) and increased that of the
   IL-1 beta gene (P < 0.05). There was also a trend to a reduced
   expression of both IL-6 and TNF-alpha. The above modulation of cytokine
   genes expression and the greater daily mean weight gain of treated
   piglets highlight important regulatory properties of oral IFN-alpha in
   the response to the weaning stress.}},
