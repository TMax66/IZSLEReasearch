@article{ ISI:000390568500026,
Author = {Latrofa, Maria Stefania and Giannelli, Alessio and Persichetti, Maria
   Flaminia and Pennisi, Maria Grazia and Solano-Gallego, Laia and Brianti,
   Emanuele and Parisi, Antonio and Wall, Richard and Dantas-Torres, Filipe
