@article{ WOS:A1997XM00400030,
Author = {Zanin, E and Capua, I and Casaccia, C and Zuin, A and Moresco, A},
Title = {Isolation and characterization of Aujeszky's disease virus in captive
   brown bears from Italy},
Journal = {JOURNAL OF WILDLIFE DISEASES},
Year = {1997},
Volume = {33},
Number = {3},
Pages = {632-634},
Month = {JUL},
Abstract = {An epizootic of Aujeszky's disease (pseudorabies) in four captive
   European brown bears (Ursus arctos) in November 1994, in the Val di Non,
   Trentino Region, Italy, was linked to consumption of raw pork. Affected
   animals had severe pruritus resulting in self-mutilation, and all four
   died within 24 hr after onset of clinical signs. Aujeszky's disease
   virus was isolated on first passage from the brain and was characterized
   by means of restriction endonuclease analysis. Based on these data, we
   believe that bears are extremely susceptible to the disease, and that
   wildlife managers should consider pseudorabies as a potential risk for
   the captive and wild bear populations.},
