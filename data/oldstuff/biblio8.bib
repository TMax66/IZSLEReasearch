@article{ WOS:000071699100008,
Author = {Dusi, G and Fierro, A and Tognoli, N},
Title = {HPLC determination of ivermectin residues in milk for human consumption},
Journal = {ITALIAN JOURNAL OF FOOD SCIENCE},
Year = {1997},
Volume = {9},
Number = {4},
Pages = {337-342},
Abstract = {A rapid and very effective analytical procedure for the routine HPLC
   determination of ivermectin (IVM) residues in milk for human consumption
   or for the dairy industry was developed and tested. The drug was
   extracted from the sample with acetonitrile and then the extract, mixed
   with water and triethylamine, was rapidly cleaned up on an SPE C-18
   column. The IVM eluate was derivatized and the derivatization mixture
   was cleaned up on an SPE silica column. The IVM derivate was then
   analysed by HPLC with fluorescence detection. The mean findings from IVM
   fortified samples were 73\% with SD 4\% for cow milk, 70\% with SD 5\%
   for sheep milk, 80\% with SD 4\% for goat milk and 78\% with SD 5\% for
   buffalo milk in a concentration range of 2.8-55.6 ng/mL. The limit of
   determination was 0.5 ng/mL (ppb) and the absolute detection limit was
   250 pg injected IVM.},
