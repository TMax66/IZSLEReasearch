@article{ WOS:A1994MT64500015,
Author = {CASTRUCCI, MR and CAMPITELLI, L and RUGGIERI, A and BARIGAZZI, G and
   SIDOLI, L and DANIELS, R and OXFORD, JS and DONATELLI, I},
Title = {ANTIGENIC AND SEQUENCE-ANALYSIS OF H3 INFLUENZA-VIRUS HEMAGGLUTININS
   FROM PIGS IN ITALY},
Journal = {JOURNAL OF GENERAL VIROLOGY},
Year = {1994},
Volume = {75},
Number = {2},
Pages = {371-379},
Month = {FEB},
Abstract = {To investigate the possible mechanism of maintenance of old human
   influenza A (H3N2) viruses in pigs, the haemagglutinins (HAs) of seven
   isolates from swine were studied by analysis of nucleotide and deduced
   primary amino acid sequences, as well as reactivity of the HA molecule
   to chicken antisera and monoclonal antibodies. The swine HAs were
   closely similar to the HA of the A/Victoria/3/75 human variant as
   regards antigenic and molecular characteristics. These findings are
   consistent with the hypothesis that the swine HA genes were transmitted
   from an early human H3 virus to pigs, where they survived with limited
   mutations over a period of 5 years. The sequence data were also compared
   with swine H3 sequences to investigate genetic relationships between the
   H3 genes from swine viruses isolated in different geographical areas. An
   evolutionary tree, constructed from the nucleotide sequences of viruses
   isolated from pigs in China and in Italy, illustrated that, depending on
   the country of their isolation, the HA genes of swine influenza A (H3N2)
   viruses have different origins, e.g. human and avian, and evolved
   independently in different lineages. The study provides direct support
   for the hypothesis that pigs might serve as a `mixing vessel' for the
   generation of pandemic strains of human influenza viruses.},
Publisher = {MICROBIOLOGY SOC},
Address = {14-16 MEREDITH ST, LONDON, ENGLAND},
Type = {Article},
Language = {English},
Affiliation = {IST SUPER SANITA, DIV VIROL, I-00161 ROME, ITALY.
   IST ZOOPROFILATT SPERIMENTALE, PARMA, ITALY.
   NATL INST MED RES, LONDON NW7 1AA, ENGLAND.
   LONDON HOSP, COLL MED, DEPT VIROL, LONDON E1 2AD, ENGLAND.},
DOI = {10.1099/0022-1317-75-2-371},
ISSN = {0022-1317},
EISSN = {1465-2099},
Keywords-Plus = {HEMAGGLUTININ GENE; A VIRUSES; ORIGIN; DRIFT; SUBTYPE; NUCLEOPROTEIN;
