@article{ WOS:000072435800008,
Author = {Serpe, L and Gallo, P and Fidanza, N and Scaramuzzo, A and Fenizia, D},
Title = {Detection of Brucella spp in milk by PCR},
Journal = {INDUSTRIE ALIMENTARI},
Year = {1998},
Volume = {37},
Number = {367},
Pages = {191-194},
Month = {FEB},
Abstract = {A rapid polymerase chain reaction (PCR) method was developed for the
   detection of Brucella spp in milk. This method involves a single step
   procedure based on freeze and thaw steps which allow the release of
   bacterial DNA directly into the Food matrix, so that extraction and
   purification of DNA from bacteria is not required.
   The template DNA collected in the medium was amplified by using two
   oligonucleotides homologous to the regions of the gene coding for a
   31kDa outer membrane protein characteristic of the genus Brucella.
   The amplification products were detected by agarose gel electrophoresis.
   This method seems to be a suitable diagnostic test thanks to the
   specificity and speed features showed.},
