@article{ WOS:000078529600005,
Author = {Lorenzini, R and Mattioli, S and Fico, R},
Title = {Allozyme variation in native red deer Cervus elaphus of Mesola Wood,
   northern Italy: implications for conservation},
Journal = {ACTA THERIOLOGICA},
Year = {1998},
