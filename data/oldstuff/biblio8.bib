@article{ WOS:A1997YD64000008,
Author = {Mioni, R and Grimaldi, M and Bordin, P and DallaPozza, MC and Zorzi, A
   and Bacelle, C},
Title = {Comparison between selective media for Escherichia coli and coliforms
   simultaneous determination in food of animal origin},
Journal = {INDUSTRIE ALIMENTARI},
Year = {1997},
Volume = {36},
Number = {362},
Pages = {1014-1020},
Month = {SEP},
Abstract = {The performance of three new coltural media called Coli ID (Bio
   Merieux), Chromocult Coliform Agar (Merck) e Colicrom Agar (DID), which
   base their specific power in the beta-glucuronidase and
   beta-galactosidase enzymatic reactions and allow the Escherichia coli
   and coliforms determination at the same time, has been evaluated in
   comparison to Rapid' E. coli and V.R.B.A., respectively used for the
   research of the above mentioned micro-organisms.
   The comparative study showed that Coli ID is more selective and specific
   than the other new media and than V.R.B.A. Regarding Escherichia coli
   determination, Coli ID demonstrated to be as specific as Rapid' E. coli
