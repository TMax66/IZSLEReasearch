@article{ WOS:A1991FG61900004,
Author = {CASTRUCCI, G and FRIGERI, F and FERRARI, M and TRALDI, V},
Title = {THE VACCINATION AND CHALLENGE WITH BOVINE VIRAL DIARRHEA VIRUS (BVDV) OF
   CALVES PREVIOUSLY INFECTED WITH A NONCYTOPATHIC BVDV},
Journal = {COMPARATIVE IMMUNOLOGY MICROBIOLOGY AND INFECTIOUS DISEASES},
Year = {1991},
Volume = {14},
Number = {1},
Pages = {31-38},
Abstract = {Four calves were infected with noncytopathic (NCP) New York-1 strain of
   bovine viral diarrhea virus (BVDV).  During the observation period of
   one month the calves remained clinically normal but the virus was
   repeatedly recovered from their pharyngeal swabbings and blood.  Thirty
   days following infection the four calves were vaccinated, together with
   two uninfected calves, with a modified-live vaccine containing
   cytopathic (CP) BVDV, infectious bovine rhinotracheitis virus and
   parainfluenza-3 virus.  No detrimental effects were observed after
   vaccination.  Forty-three days after vaccination the calves were
   challenged by exposure either with the CP TVM-2 strain or the NCP New
   York-1 strain of BVDV.  The vaccinated calves remained healthy
   throughout the 60-day observation period.},
