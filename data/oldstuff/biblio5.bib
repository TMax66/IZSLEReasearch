@article{ WOS:000303197100012,
Author = {Cassano, Tommaso and Serviddio, Gaetano and Gaetani, Silvana and Romano,
   Adele and Dipasquale, Pasqua and Cianci, Silvia and Bellanti, Francesco
   and Laconca, Leonardo and Romano, Antonino Davide and Padalino, Iolanda
   and LaFerla, Frank M. and Nicoletti, Ferdinando and Cuomo, Vincenzo and
