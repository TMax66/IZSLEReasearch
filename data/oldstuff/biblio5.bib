@article{ WOS:000320595800068,
Author = {Decaro, Nicola and Larocca, Vittorio and Parisi, Antonio and Losurdo,
   Michele and Lia, Riccardo Paolo and Greco, Maria Fiorella and Miccolis,
   Antonio and Ventrella, Gianpiero and Otranto, Domenico and Buonavoglia,
   Canio},
Title = {Clinical Bovine Piroplasmosis Caused by Babesia occultans in Italy},
Journal = {JOURNAL OF CLINICAL MICROBIOLOGY},
Year = {2013},
Volume = {51},
Number = {7},
Pages = {2432-2434},
Month = {JUL},
Abstract = {A clinical outbreak of bovine piroplasmosis was reported in Italy. The
