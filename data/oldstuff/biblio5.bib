@article{ WOS:000306864900052,
Author = {Marruchella, Giuseppe and Ligios, Ciriaco and Di Guardo, Giovanni},
Title = {Age, scrapie status, PrP genotype and follicular dendritic cells in
   ovine ileal Peyer's patches},
Journal = {RESEARCH IN VETERINARY SCIENCE},
Year = {2012},
Volume = {93},
Number = {2},
Pages = {853-856},
Month = {OCT},
Abstract = {Follicular dendritic cells (FDCs) residing within ileal Peyer's patches
   (PPs) are of crucial relevance for sheep scrapie early pathogenesis and
   subsequent scrapie prion neuroinvasion. In this study, ileal PP
   follicles were significantly more numerous in lambs than in adult Sarda
   breed sheep, with significant differences being also found in lymphoid
   follicle area, perimeter and FDC density. Furthermore, PrPd deposition
   within ileal PPs and host's PrP genotype did not significantly influence
   these parameters. We conclude that age significantly affects FDC density
   in Heal PPs from Sarda breed ovines, independently from host's scrapie
   status and PrP genotype. (C) 2011 Elsevier Ltd. All rights reserved.},
