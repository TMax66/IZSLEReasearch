@article{ WOS:000326306600022,
Author = {Prevete, N. and Staiano, R. I. and Granata, F. and Detoraki, A. and
   Necchi, V. and Ricci, V. and Triggiani, M. and De Paulis, A. and Marone,
   G. and Genovese, A.},
Title = {EXPRESSION AND FUNCTION OF ANGIOPOIETINS AND THEIR TIE RECEPTORS IN
   HUMAN BASOPHILS AND MAST CELLS},
Journal = {JOURNAL OF BIOLOGICAL REGULATORS AND HOMEOSTATIC AGENTS},
Year = {2013},
Volume = {27},
Number = {3},
Pages = {827-839},
Month = {JUL-SEP},
Abstract = {The Angiopoietin/Tie system is a key regulator of vascular remodeling,
   maturation, angiogenesis and lymphangiogenesis. In humans there are
   three angiopoietins: Angiopoietin-1 (Angl), Angiopoietin-2 (Ang2), and
   Angiopoietin-4 (Ang4). Angl and Ang2 are the best characterized
   angiopoietins. The `` angiopoietin receptor system consists of two type
   I tyrosine kinase receptors (Tie1 and Tie2). Tie2 binds all known
   angiopoietins. We sought to characterize Angl, Ang2, Tiel and T1e2
   expression and functions in human basophils and mast cells. Basophils,
   LAD-2 cells and Human Lung Mast Cells (HLMCs) constitutively express
   Angl and Ang2 mRNA. Intracellular staining for Angl and Ang2 was
   stronger in basophils than in mast cells. Immunoelectron microscopy
   demonstrated Angl in cytoplasmic vesicles of basophils. The protein
   kinase C activators phorbol diester (PMA) and bryostatin 1 (Bryol)
   stimulated basophils to rapidly release a large amount of Angl.
   PMA-induced Angl release was inhibited by brefeldin A. Tiel and Tie2
   mRNAs were expressed in basophils, LAD-2 and HLMCs. Basophils, LAD-2 and
   HLMCs expressed Tiel on the cell surface. HLMCs and LAD-2 expressed Tie2
   on the cell surface, whereas basophils did not. Angl, but not Ang2,
   induced migration of mast cells through the engagement of Tie2. Neither
   Angl nor Ang2 induced basophil chemotaxis. We have identified a novel
   mechanism of cross-talk between human basophils and mast cells mediated
   by the Angl/Tie2 system that might be relevant in the orchestration of
   inflammatory and neoplastic angiogenesis.},
