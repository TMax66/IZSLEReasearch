@article{ WOS:000302339200003,
Author = {La Rosa, Giuseppina and Fratini, Marta and Vennarucci, Valentina Spuri
   and Guercio, Annalisa and Purpari, Giuseppa and Muscillo, Michele},
Title = {GIV noroviruses and other enteric viruses in bivalves: a preliminary
   study},
Journal = {NEW MICROBIOLOGICA},
Year = {2012},
Volume = {35},
Number = {1},
Pages = {27-34},
Month = {JAN},
Abstract = {We evaluated the presence of the enteric viruses: norovirus, adenovirus,
   enterovirus, astrovirus, hepatitis A virus, and hepatitis E virus in
   bivalves using nested PCR methods and cell culture assays. Noroviruses
