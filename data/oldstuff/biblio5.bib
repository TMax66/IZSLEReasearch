@article{ WOS:000299139100015,
Author = {Giangaspero, Massimo and Ibata, Georgina and Savini, Giovanni and Osawa,
   Takeshi and Tatami, Shingo and Takagi, Eishu and Moriya, Hiroaki and
   Okura, Norimoto and Kimura, Atsushi and Harasawa, Ryo},
