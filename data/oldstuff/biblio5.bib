@article{ WOS:000331980300180,
Author = {Amoroso, Maria Grazia and Corrado, Federica and De Carlo, Esterina and
   Lucibelli, Maria Gabriella and Martucciello, Alessandra and Galiero,
   Giorgio},
Title = {Bubaline Herpesvirus 1 Associated with Abortion in a Mediterranean Water
