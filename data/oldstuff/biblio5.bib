Type = {Review},
Language = {English},
Affiliation = {Romero-Tejeda, A (Corresponding Author), Ist Zooprofilatt Sperimentale Venezie, Div Comparat Biomed Sci, Viale Univ 10, I-35020 Legnaro, PD, Italy.
   Romero-Tejeda, Aurora; Capua, Ilaria, Ist Zooprofilatt Sperimentale Venezie, Div Comparat Biomed Sci, I-35020 Legnaro, PD, Italy.},
DOI = {10.1111/irv.12075},
ISSN = {1750-2640},
EISSN = {1750-2659},
Keywords = {Host range; influenza; interspecies transmission; pandemic; virus
   adaptation},
