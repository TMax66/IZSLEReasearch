@article{ WOS:000335887900069,
Author = {Di Giannatale, Elisabetta and Di Serafino, Gabriella and Zilli,
   Katiuscia and Alessiani, Alessandra and Sacchini, Lorena and Garofolo,
   Giuliano and Aprea, Giuseppe and Marotta, Francesca},
Title = {Characterization of Antimicrobial Resistance Patterns and Detection of
   Virulence Genes in Campylobacter Isolates in Italy},
Journal = {SENSORS},
