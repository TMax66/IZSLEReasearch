@article{ WOS:000304977500004,
Author = {Peletto, Simone and Lo Presti, Alessandra and Modesto, Paola and Cella,
   Eleonora and Acutis, Pier Luigi and Farchi, Francesca and Ciotti, Marco
   and Zehender, Gianguglielmo and Ciccozzi, Massimo},
Title = {Genetic diversity of Usutu virus},
Journal = {NEW MICROBIOLOGICA},
Year = {2012},
Volume = {35},
Number = {2},
Pages = {167-174},
Month = {APR},
Abstract = {Usutu virus is a mosquito-borne virus first isolated from Culex naevei
   in South Africa in 1959. The first emergence of Usutu virus outside
   Africa was recorded in Austria. Here, a phylogenetic analysis targeting
   the E5 and NS5 genes was carried out on the viral strains circulating in
   Europe. The NS5 gene tree showed two main clades, one of which included
   the Italian sequences. In the E gene tree all sequences grouped into the
   same main clade, with sequences from Austria divided into two separate
   clusters. Only sites under negative selective pressure were found in E
   and NS5 proteins. The results suggest that Usutu virus circulating in
   Europe has a degree of genetic diversity higher than expected and that
   infection may arise from different sources.},
