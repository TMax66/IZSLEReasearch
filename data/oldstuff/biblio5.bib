@article{ WOS:000323634600006,
Author = {Santini, Antonello and Raiola, Assunta and Ferrantelli, Vincenzo and
   Giangrosso, Giuseppe and Macaluso, Andrea and Bognanno, Matteo and
   Galvano, Fabio and Ritieni, Alberto},
Title = {Aflatoxin M-1 in raw, UHT milk and dairy products in Sicily (Italy)},
Journal = {FOOD ADDITIVES \& CONTAMINANTS PART B-SURVEILLANCE},
Year = {2013},
Volume = {6},
Number = {3},
Pages = {181-186},
Month = {SEP 1},
Abstract = {A survey on 73 milk samples from different animal breeds and 24 dairy
   products samples from Sicily, Italy, was carried out for the presence of
   aflatoxin M-1 (AFM1) by LC-fluorescence detection after immunoaffinity
   cleanup. AFM1 was detected in 48\% and 42\% of the milk and dairy
   samples at concentration ranges between<5.0-16.0 and<5.0-18.0ngL(-1),
   respectively. Within the raw milk samples, 92\% had an AFM1 content
   below 5.0ngL(-1), in 7\% of the cases it was in the range
   5.0-10.0ngL(-1) and 1\% was contaminated between 10.0 and 20.0ngL(-1).
   For the dairy products, ultra-high-temperature treated (UHT) milk, milk
   cream and cheese, the incidence was 42\%, of which 83\% contained less
   than 5.0ngL(-1) and 17\% contained 10.0-20.0ngL(-1) AFM1. The levels of
   contamination found justify continuous monitoring for public health and
   to reduce consumer exposure.},
