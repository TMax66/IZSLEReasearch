@article{ WOS:000441877400008,
Author = {Lanave, Gianvito and Dowgier, Giulia and Decaro, Nicola and Albanese,
   Francesco and Brogi, Elisa and Parisi, Antonio and Losurdo, Michele and
   Lavazza, Antonio and Martella, Vito and Buonavoglia, Canio and Elia,
   Gabriella},
Title = {Novel Orthopoxvirus and Lethal Disease in Cat, Italy},
Journal = {EMERGING INFECTIOUS DISEASES},
Year = {2018},
Volume = {24},
Number = {9},
Pages = {1665-1673},
Month = {SEP},
Abstract = {We report detection and full-genome characterization of a novel
   orthopoxvirus (OPXV) responsible for a fatal infection in a cat. The
   virus induced skin lesions histologically characterized by leukocyte
   infiltration and eosinophilic cytoplasmic inclusions. Different PCR
   approaches were unable to assign the virus to a defined OPXV species.
   Large amounts of typical brick-shaped virions, morphologically related
   to OPXV, were observed by electron microscopy. This OPXV strain
   (Italy\_09/17) was isolated on cell cultures and embryonated eggs.
   Phylogenetic analysis of 9 concatenated genes showed that this virus was
   distantly related to cowpox virus, more closely related to to ectromelia
   virus, and belonged to the same cluster of an OPXV recently isolated
   from captive macaques in Italy. Extensive epidemiologic surveillance in
   cats and rodents will assess whether cats are incidental hosts and
   rodents are the main reservoir of the virus. The zoonotic potential of
   this novel virus also deserves further investigation.},
