@article{ WOS:000376163100045,
Author = {Iammarino, Marco and dell'Oro, Daniela and Bortone, Nicola and
   Mangiacotti, Michele and Damiano, Rita and Chiaravalle, Antonio Eugenio},
Title = {Radiostrontium levels in foodstuffs: 4-Years control activity by Italian
   reference centre, as a contribution to risk assessment},
Journal = {FOOD CHEMISTRY},
Year = {2016},
Volume = {210},
Pages = {344-354},
Month = {NOV 1},
Abstract = {Sr-90 is considered an important contaminant relating to food supply
   chains. In this study, 176 liquid and 260 solid foods, were analysed in
   order to quantify Sr-90.
   Through ruggedness tests, the application field of radiochemical methods
   used was extended successfully to all most important types of
   foodstuffs.
   Regarding liquid matrices, milk samples resulted the most important
   indicator about Sr-90 contamination, with mean Sr-90 activity
   concentration equal to 0.058 Bq L-1. Among other liquid foods,
   wine/spirits and livestock watering resulted the most contaminated, with
   mean contamination levels equal to 0.022 and 0.035 Bq L-1, respectively.
   Concerning solid matrices, cheeses produced from sheep's milk and animal
   feeds resulted the most contaminated (mean levels: 1.237 and 1.557 Bq
   kg(-1), respectively). Meat products and seafood showed contamination
   levels not significant within this survey; while, among vegetables,
   cacao/chocolate and spices resulted in contamination levels comparable
   with those of cheese obtained from milk of cows origin. (C) 2016
   Elsevier Ltd. All rights reserved.},
