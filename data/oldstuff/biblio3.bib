@article{ WOS:000427557500007,
Author = {Paoloni, Angela and Solfrizzo, Michele and Bibi, Rita and Pecorelli,
   Ivan},
Title = {Development and validation of LC-MS/MS method for the determination of
   Ochratoxin A and its metabolite Ochratoxin in poultry tissues and eggs},
Journal = {JOURNAL OF ENVIRONMENTAL SCIENCE AND HEALTH PART B-PESTICIDES FOOD
   CONTAMINANTS AND AGRICULTURAL WASTES},
Year = {2018},
Volume = {53},
Number = {5},
Pages = {327-333},
Abstract = {The objective of this study was to develop a liquid chromatography
   tandem mass spectrometry (LC-MS/MS) method for the determination of
   Ochratoxin A (OTA) and Ochratoxin (OT) in poultry tissues and eggs. The
   two toxins were extracted by a mixture of acetonitrile/water, purified
   with a reversed phase C18 solid phase extraction column (SPE) and
   determined by LC-MS/MS. The LC-MS/MS method performances were evaluated
   in terms of linearity in solvent and in matrix (ranged from 0.5 to 15.10
   mu g L-1 for OTA and from 0.60 to 17.85 mu g L-1 for OT), limit of
   detection (LOD), limit of quantitation (LOQ), specificity, accuracy and
   precision in repeatability conditions. Recovery experiments were
   performed by spiking poultry liver, kidney, muscle and eggs around 1 mu
   g kg(-1) and 10 mu g kg(-1). LODs were 0.27 and 0.26 mu g kg(-1) while
   LOQs were fixed at 1.0 and 1.2 mu g kg(-1) for OTA and OT, respectively.
   Main recoveries for OTA ranged from 82 to 109\% and for OT ranged from
   55 to 89\%. The values of within-laboratory relative standard deviation
   (RSDr) were equal to or below 20\%. Considering the results obtained and
   that all analytical performance criteria were fulfilled, the new
   extraction and purification method developed for OTA and OT
   determination in animal tissues and eggs was found appropriate for
   control laboratories and research activities designed to ensure food
   safety.},
