@article{ WOS:000388562100001,
Author = {Alba, Patricia and Caprioli, Andrea and Cocumelli, Cristiano and
   Ianzano, Angela and Donati, Valentina and Scholl, Francesco and Sorbara,
   Luigi and Terracciano, Giuliana and Fichi, Gianluca and Di Nocera, Fabio
   and Franco, Alessia and Battisti, Antonio},
Title = {A New Multilocus Sequence Typing Scheme and Its Application for the
   Characterization of Photobacterium damselae subsp damselae Associated
   with Mortalityin Cetaceans},
Journal = {FRONTIERS IN MICROBIOLOGY},
Year = {2016},
Volume = {7},
Month = {OCT 21},
Abstract = {Photobacterium damselae subsp. damselae (PDD) is a known pathogen of
   fish, humans and marine mammals. In this study, a Multilocus Sequence
   Typing (MLST) scheme based on six housekeeping genes (glp, gyrB, metG,
   pnt, pyrC, and toxR) was developed to better understand the PDD
   population structure and used to type 73 PDD isolates from cetaceans,
   mainly striped dolphins (Stenella coeruleoalba) involved in mortality
   episodes, and from a few marine chelonians. Five reference ATCC strains
   were also included in the study. Typing allowed the discrimination of
   groups of PDD strains isolated from different host species, at different
   times and from different geographic areas, suggesting that a clonal PDD
   group may have spread in the Tyrrhenian sea at the time of an Unusual
   Mortality Event (UME) among cetaceans, mainly striped dolphins, occurred
   in early 2013 along the Italian western coasts.},
Publisher = {FRONTIERS MEDIA SA},
Address = {AVENUE DU TRIBUNAL FEDERAL 34, LAUSANNE, CH-1015, SWITZERLAND},
Type = {Article},
Language = {English},
Affiliation = {Battisti, A (Corresponding Author), Ist Zooprofilatt Sperimentale Lazio \& Toscana, Sede Cent, Gen Diagnost Dept, Rome, Italy.
   Alba, Patricia; Caprioli, Andrea; Ianzano, Angela; Donati, Valentina; Sorbara, Luigi; Franco, Alessia; Battisti, Antonio, Ist Zooprofilatt Sperimentale Lazio \& Toscana, Sede Cent, Gen Diagnost Dept, Rome, Italy.
