@article{ WOS:000390965800064,
Author = {Bassanetti, Irene and Carcelli, Mauro and Buschini, Annamaria and
   Montalbano, Serena and Leonardi, Giuliano and Pelagatti, Paolo and Tosi,
   Giovanni and Massi, Paola and Fiorentini, Laura and Rogolino, Dominga},
Title = {Investigation of antibacterial activity of new classes of essential oils
   derivatives},
Journal = {FOOD CONTROL},
Year = {2017},
Volume = {73},
Number = {B},
Pages = {606-612},
Month = {MAR},
Abstract = {Essential oils (EOs) have deserved much attention in the past decades
   for their antimicrobial activity, since many of them have demonstrated
   efficacy against food-borne pathogenic and spoilage microorganisms.
   Moreover, they have potential application in animal nutrition as
   multifunctional feed supplements, avoiding or diminishing the use of
   antibiotics in livestock. However, low solubility and bioavailability as
   well as volatility and marked aromatic note are important limitations in
   food and feed applications. In this study we present the synthesis,
   characterization and evaluation of the antibacterial activity of new
   thymol, carvacrol and menthol derivatives. The new compounds have been
   designed to overcome the limitations of the precursors, such as poor
   water solubility and volatility, still maintaining a good antimicrobial
   profile. We evaluated the activity of the synthetized compounds against
   pathogens causing important foodborne diseases, i.e. Clostridium
   perfringens, Salmonella typhimurium, Salmonella enteritidis and
   Escherichia coli. The low Ms and MBCs values for some of the studied
   compounds, combined with water solubility and negligible cytotoxicity
   towards HT-29 human cells, confirmed the potential use for EOs
   derivatives in the food industry. (C) 2016 Elsevier Ltd. All rights
   reserved.},
