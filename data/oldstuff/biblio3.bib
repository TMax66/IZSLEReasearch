@article{ WOS:000445370300004,
Author = {Torresi, Marina and Sperandii, Annafranca and Ricci, Lucilla and
   Prencipe, Vincenza and Migliorati, Giacomo and Pomilio, Francesco},
Title = {Detection and characterisation of potentially pathogenic species of
   Vibrio in the Vibrato river, Abruzzo Region, Italy},
Journal = {VETERINARIA ITALIANA},
Year = {2018},
Volume = {54},
Number = {2},
Pages = {125-135},
Month = {APR-JUN},
Abstract = {This study aimed to isolate, define the genetic profile, assess
   potential pathogenicity and PFGE, evaluate the seasonal distribution of
   Vibrio cholerae, Vibrio parahaemolyticus, and Vibrio River, vulnificus
   strains isolated from the Vibrata river (Abruzzo Region, Italy) during a
   monitoring Vibrio spp., period of one year. Detection was performed
   according to ISO/TS 21872-1-2:2007. Species Virulence genes.
   identification and characterisation were achieved using molecular
   methods. Vibrio spp. were detected in 50\% (23) of the water samples. In
   particular, V. cholerae, V. parahaemolyticus, and V. vulnificus were
   isolated in 18 (39.1\%), 4 (8.7\%), and 2 (4.3\%) samples, respectively.
   All V. parahaemolyticus strains were tdh gene negative, 75\% were
   positive for trh gene. In 30 V. cholerae isolates, the polymerase chain
   reaction (PCR) assay for detecting virulence and regulatory genes (ctxA,
   toxR, tcpA, ompU, hlyA, tcpl, zot, and stn/sto) revealed 6 genotypes
   associated to different levels of pathogenicity. Pulsed-field gel
   electrophoresis (PFGE) characterisation of the V. cholerae strains
   identified 13 different pulsotypes. A greater degree of similarity was
   shown for strains isolated in the same period of the year. Results of
   our study reveal a potential health risk associated with the waters of
   the Vibrata river, which are used for irrigation and next to the
   swimming areas of Abruzzo coastline.},
