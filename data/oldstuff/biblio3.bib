@article{ WOS:000404647100021,
Author = {El Romeh, Ali and Zecchin, Bianca and Fusaro, Alice and Ibrahim, Elias
   and El Bazzal, Bassel and El Hage, Jeanne and Milani, Adelaide and
   Zamperin, Gianpiero and Monne, Isabella},
Title = {Highly Pathogenic Avian Influenza H5N1 Clade 2.3.2.1c Virus in Lebanon,
   2016},
Journal = {AVIAN DISEASES},
Year = {2017},
Volume = {61},
Number = {2},
Pages = {271-273},
Month = {JUN},
Abstract = {We report the phylogenetic analysis of the first outbreak of H5N1 highly
