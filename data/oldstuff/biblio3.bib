@article{ WOS:000414392000004,
Author = {Di Martino, G. and Zamprogna, S. and Garbo, A. and Caucci, C. and
   Marchesan, M. and Bonfanti, L. and Dalmau, A.},
Title = {Welfare assessment in Italian pig slaughterhouses},
Journal = {LARGE ANIMAL REVIEW},
Year = {2017},
Volume = {23},
Number = {4},
Pages = {149-153},
Month = {AUG},
Abstract = {Welfare Quality (R) Protocol is a flexible, standardized and
   scientifically proven tool for welfare assessment of pigs at the time of
   slaughtering. The sampling scheme is based on defined sample sizes and
   is developed across the major slaughtering phases (unloading, lairage,
   stunning, jugulation). The protocol has been applied in 9 Italian
   abattoirs, to highlight possible margins of improvement and to define
   reference values for some important animal based indicators. In the
   unloading area, the levels of fear, slipping, lameness, space, thermal
   comfort, sick and dead animals were assessed. At lairage, space
   allowance, drinking points and level of thermal comfort were considered.
   Vocalisations were counted by means of two measurements (continuous and
   instantaneous) when pigs were moved from lairage to the stunning system.
   Stunning effectiveness by means of 4 parameters, level of skin lesions
   and signs of diseases (pneumonia, pericarditis, liver parasites) were
   evaluated in the stunning area and after slaughtering the animals,
   respectively. All abattoirs assured an overall good level of welfare; in
   particular, the prevalence of slipping ranged between 1 and 13\%,
   lameness did not exceed 2\% and carcasses with more than 10 lesions
   ranged between 2 and 10\%. Collected data are part of a larger study on
   the application of Welfare Quality protocol in 5 countries (Italy,
   Spain, Portugal, Finland and Brazil) with the ultimate aim to provide
   risk thresholds taking into account the high variability of production
   systems worldwide.},
