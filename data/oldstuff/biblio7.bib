@inproceedings{ WOS:000083860700030,
Author = {Coni, V and Alongi, C and Zintu, L and Ligios, C and Liciardi, M and
   Pulina, G},
Editor = {Barillet, F and Deligeorgis, S and Zervas, NP and Boyazoglu, JG and Vallerand, F},
Title = {Experimental intramammary infection with Staphylococcus haemolyticus in
   dairy ewes},
Booktitle = {MILKING AND MILK PRODUCTION OF DAIRY SHEEP AND GOATS},
Series = {EAAP EUROPEAN ASSOCIATION FOR ANIMAL PRODUCTION PUBLICATION},
Year = {1999},
Number = {95},
Pages = {175-177},
Note = {6th International Symposium on the Milking of Small Ruminants, ATHENS,
   GREECE, SEP 26-OCT 01, 1998},
Organization = {Int Sci Comm Hellenic Soc Anim Product; European Assoc Anim Product;
   European Cooperat Res Network Agr; Ctr Int Hautes Etudes Agronom; Int
   Comm Anim Recording; Greek Minist Agr \& Culture \& Sci},
Abstract = {To investigate its pathogenicity, a strain of S. haemolyticus from field
   cases was inoculated into the mammary gland of 10 lactating ewes. The
   experiment lasted from the day of infection to the third month after the
   subsequent lambing. Milk samples were periodically taken from all the
   ewes to ascertain the somatic cell count, the rate of fat, proteins and
   lactose. The same samples were cultured. The ewes were clinically
   examined and the quantity of milk produced was measured. Tissue samples
   were obtained to study the sequential histopathological features of the
   infection. All the infected mammary glands showed clear evidence of
   clinical mastitis 16 hours after inoculation and there were significant
   changes in the rates of fat, proteins and lactose when compared with the
   controls. The inoculated mammary glands produced less milk than did the
   controls. S. haemolyticus in pure culture was isolated from the mammary
   glands of all inoculated animals. Bacteria was not found in the milk
   samples taken from 2 ewes during pre-lambing lactation and in 2 others
   after lambing, although 3 ewes continued to discharge S. haemolyticus
   throughout the entire experiment. Histologically, in the tissue samples
   obtained from the ewes slaughtered at 3 and 9 days after inoculation,
   there was evidence of intra-alveolar neutrophilic infiltration. In the
   mammary gland of the ewe slaughtered 16 days after inoculation, a mild
   proliferation of fibrous tissue and focal lymphocitic aggregation were
   observed. Chronic inflammatory features dominated in the mammary glands
   of the S. haemolyticus infected ewes. No lesions were found in the S.
   haemolyticus negative ewes.},
