@article{ WOS:000231570300012,
Author = {Grossi, P and Giudice, C and Bertoletti, I and Cioccarelli, G and
   Brocchi, E and Cammarata, G and Gelmetti, D},
Title = {Immunohistochemical detection of the p27 capsid protein of caprine
   arthritis-encephalitis virus (CAEV) in bone-marrow cells of seropositive
   goats},
Journal = {JOURNAL OF COMPARATIVE PATHOLOGY},
Year = {2005},
Volume = {133},
Number = {2-3},
Pages = {197-200},
Month = {AUG-OCT},
Abstract = {Bone-marrow samples were collected from 48 CAEV-seropositive,
   symptomless goats (30 kids, IS adults). The samples were formalin-fixed
   and processed for histological examination. In addition, all samples
   were examined immunohistochemically with a monoclonal antibody (1A7)
   against the p27 capsid protein of maedi-visna virus, an antibody which
   cross-reacts with the Ca-p27 of CAEV. Samples from 16 goats (10/30 kids,
   6/18 adults) showed positive immunolabelling of bone-marrow stromal
   cells (fibrocytes, endothelial cells and adipocytes) and of scattered
   macrophages, whereas haematopoietic cells were negative. The detection
   of viral Ca-p27 protein in bone-marrow fibrocytes was consistent with
   previous in-vitro studies which indicated that such cells are
   semi-permissive for CAEV infection. It is speculated that bone-marrow
   stromal cells represent a viral reservoir in symptomless animals. (c)
   2005 Elsevier Ltd. All rights reserved.},
