@article{ WOS:000224393600030,
Author = {Parisi, A and Normanno, G and Addante, N and Dambrosio, A and Montagna,
   CO and Quaglia, NC and Celano, GV and Chiocco, D},
Title = {Market survey of Vibrio spp. and other microrganisms in Italian
   shellfish},
Journal = {JOURNAL OF FOOD PROTECTION},
Year = {2004},
Volume = {67},
Number = {10},
Pages = {2284-2287},
Month = {OCT},
Abstract = {A survey was conducted of Vibrio spp., Escherichia coli, fecal
   coliforms, and Salmonella in 644 molluscan shellfish samples marketed in
   the Apulia region of southern Italy. Vibrios were found in 278 samples
   (43\%), and levels of E. coli and fecal coliforms were above the Italian
   legal limit in 27 and 34 samples (4 and 5\%), respectively. Salmonella
   was not detected in any of the samples. Because the majority of the
   vibrio isolates were found in samples that were compliant with Italian
   regulations, there appears to be no relationship between the presence of
   microorganisms of fecal origin and the presence of vibrios potentially
   harmful to human health.},
