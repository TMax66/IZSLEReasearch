Address = {12 RUE DE PRONY, 75017 PARIS, FRANCE},
Type = {Article},
Language = {English},
Affiliation = {Giovannini, A (Corresponding Author), Ist Zooprofilattico Sperimentale Abruzzo \& Molise, Via Campo Boario, I-64100 Teramo, Italy.
