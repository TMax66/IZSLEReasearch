@article{ WOS:000232435300031,
Author = {Torina, A and Caracappa, S and Barera, A and Dieli, F and Sireci, G and
   Genchi, C and Deplazes, P and Salerno, A},
Title = {Toxocara canis infection induces antigen-specific IL-10 and IFN gamma
   production in pregnant dogs and their puppies},
Journal = {VETERINARY IMMUNOLOGY AND IMMUNOPATHOLOGY},
Year = {2005},
Volume = {108},
Number = {1-2, SI},
Pages = {247-251},
Month = {OCT 18},
Note = {7th International Veterinary Immunology Symposium (IVIS 7), Quebec City,
   CANADA, JUL 25-30, 2004},
Organization = {Pfizer Anim Hlth; Fort Dodge \& Bioniche; Exhibitors Serotec; Pierce
   Biotechnol; Cedarlane Lab Ltd; VMRD Inc; Stencell Technol Inc},
Abstract = {Toxocara canis (T canis) is originally a parasite of canine bitches and
   their pups. The pathogenicity of T canis infection is enhanced during
   pregnancy and puppyhood. The aim of this study was to investigate if
   modification of IFN gamma and IL-10 secretion occurs during infection in
   pregnant dogs and puppies. Analysis of cytokines secreted could let us
   hypothesize a role for IL-10 and/or IFN gamma in T. canis infection. We
   tested T. canis-specific production of IFN gamma and IL-10 by
   lymphocytes of pregnant dogs and their puppies after in vitro
   re-exposure to purified excretory/secretory antigen (ESAg) from T canis.
   Blood mononuclear cells (BMC) isolated from pregnant dogs and their
   puppies were cultured in the presence of ESAg. Cultures' supernatants
   were tested for cytokine levels by ELISA. Results obtained showed that
   IL-10 concentrations increased during pregnancy in infected animals and
   in the meantime IFN gamma production decreased. In puppyhood, we
   observed that, IL-10 concentration decreased with the age of puppies
   mainly in infected animals while IFN gamma increased. In conclusion, our
   data suggests that BMC of infected dogs have a particular modification
   of IL-10 and IFN gamma synthesis. These data could be the basis to
   design immunotherapeutic approaches. (c) 2005 Elsevier B.V. All rights
   reserved.},
