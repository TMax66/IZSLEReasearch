@article{ WOS:000244953300042,
Author = {Riggio, V. and Finocchiaro, R. and van Kaam, J. B. C. H. M. and
   Portolano, B. and Bovenhuis, H.},
Title = {Genetic parameters for milk somatic cell score and relationships with
   production traits in primiparous dairy sheep},
Journal = {JOURNAL OF DAIRY SCIENCE},
Year = {2007},
Volume = {90},
Number = {4},
Pages = {1998-2003},
Month = {APR},
Abstract = {A total of 13,066 first-lactation test-day records of 2,277 Valle del
   Belice ewes from 17 flocks were used to estimate genetic parameters for
   somatic cell scores (SCS) and milk production traits, using a
   repeatability test-day animal model. Heritability estimates were low and
   ranged from 0.09 to 0.14 for milk, fat, and protein yields, and
   contents. For SCS, the heritability of 0.14 was relatively high. The
   repeatabilities were moderate and ranged from 0.29 to 0.47 for milk
   production traits. The repeatability for SCS was 0.36. Flock-test-day
   explained a large proportion of the variation for milk production
   traits, but it did not have a big effect on SCS. The genetic
   correlations of fat and protein yields with fat and protein percentages
   were positive and high, indicating a strong association between these
   traits. The genetic correlations of milk production traits with SCS were
   positive and ranged from 0.16 to 0.31. The results showed that SCS is a
   heritable trait in Valle del Belice sheep and that single-trait
   selection for increased milk production will also increase SCS.},
