@article{ WOS:000176096100013,
Author = {Russo, MV and Neri, B},
Title = {Fluvalinate residues in honey by capillary gas chromatography - Electron
   capture detection - Mass spectrometry},
Journal = {CHROMATOGRAPHIA},
Year = {2002},
Volume = {55},
Number = {9-10},
Pages = {607-610},
Month = {MAY},
Abstract = {A fast, simple and accurate analytical method for determination of
   residual Fluvalinate in honey is described. Honey sample was dissolved
   in methanol-water and Fluvalinate extracted with ethyl acetate. The
   organic phase was evaporated and the residue dissolved in ethyl acetate.
   After clean-up on a C-18 Sep-Pak cartridge Fluvalinate was eluted with
   n-hexane, concentrated under a stream of nitrogen and analysed by
   capillary GC-ECD-MS in selected-ion monitoring mode. Recoveries from
   honey samples spiked at 7 fortification levels (0.01 - 50 ng g(-1))
   range from 93.8 - 100.2\%, RSD less than or equal to 5.0\%. The limit of
   detection was 8 ng g(-1) in GC-ECD and 18 ng g(-1) in GC-MS (SIM) with
   RSDs of 72 and 6.8\% respectively The method was used to analyse several
   types of honey (60 samples) from different Italian regions. Fluvalinote
   residues were detected in these samples and concentrations ranged from
   LOD to 0.09 ng g(-1).},
