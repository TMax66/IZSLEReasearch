@article{ WOS:000234776900024,
Author = {Martella, V and Banyai, K and Ciarlet, M and Iturriza-Gomara, M and
   Lorusso, E and De Grazia, S and Arista, S and Decaro, N and Elia, G and
   Cavalli, A and Corrente, M and Lavazza, A and Baselga, R and
   Buonavoglia, C},
Title = {Relationships among porcine and human P{[}6] rotaviruses: Evidence that
   the different human P{[}6] lineages have originated from multiple
   interspecies transmission events},
Journal = {VIROLOGY},
Year = {2006},
Volume = {344},
Number = {2},
Pages = {509-519},
Month = {JAN 20},
Abstract = {Porcine rotavinis strains (PoRVs) bearing human-like VP4 P{[}6] gene
   alleles were identified. Genetic characterization with either PCR
   genotyping or sequence analysis allowed to determine the VP7 specificity
   of the PoRVs as G3, G4, G5 and G9, and the VP6 as genogroup I, that is
   predictive of a subgroup I specificity. Sequence analysis of the VP8{*}
   trypsin-cleavage product of VP4 allowed PoRVs to be characterized
   further into genetic lineages within the P{[}6] genotype. Unexpectedly,
   the strains displayed significantly higher similarity (up to 94.6\% and
   92.5\% at an and nt level, respectively) to human M37-like P{[}6]
   strains (lineage 1), serologically classifiable as P2A, or to the
   atypical Hungarian P{[}61 human strains (HRVs), designated as lineage V
   (up to 97.0\% an and 96.1\% nt), than to the porcine P{[}6] strain
   Gottfried, lineage II (<85.1\% aa and 82.2 nt), which is serologically
   classified as P2B. Interestingly, no P{[}6] PoRV resembling the original
   prototype porcine strain, Gottfried, was detected, while Japanase P{[}6]
   PoRV clustered with the atypical Japanase G I human strain AU19. By
   analysis of the 10th and 11th genome segments, all the strains revealed
   a NSP4B genogroup (Wa-like) and a NSP5/6 gene of porcine origin. These
   findings strongly suggest interspecies transmission of rotavirus strains
   and/or genes, and may indicate the occurrence of at least 3 separate
   rotavirus transmission events between pigs and humans, providing
   convincing evidence that evolution of human rotaviruses is tightly
   intermingled with the evolution of animal rotaviruses. (C) 2005 Elsevier
   Inc. All rights reserved.},
