@article{ WOS:000168102300003,
Author = {Dusi, G and Faggionato, E and Bertocchi, L and Facchetti, S and
   Baiguera, M},
Title = {Determination of Avermectin and Milbemycin residues in milk for human
   consumption using liquid chromatography with fluorescence detection},
Journal = {INDUSTRIE ALIMENTARI},
Year = {2001},
Volume = {40},
Number = {399},
Pages = {11-14},
Month = {JAN},
Abstract = {Avermectins and Milbemycin belong to a parasiticidal family of drugs
   highly efficacious against external and internal parasites. his family
   consists of some molecules Ivermectin, Doramectin, Abamectin and
   Moxidectin - that can be used only in meat-producing animals and of
   Eprinomectin that is also available for the treatment of lactating dairy
   animals. A multiresidual analytical procedure was needed in order to
   analyze by only one method all these antiparasitic drugs. In this way it
   could be possible to discover treatments with forbidden drugs for
   lactating dairy animals and, at the same time, to verify that the
   residues of approved drugs are lower than the maximun residue limit
   (MRL). A multiresidual LC method with fluorimetric detection for the
   simultaneous determination in milk of the five above-mentioned molecules
   was developed and tested. The drugs were extracted with an organic
   solvent and the extract was cleaned up, before the derivatization step,
   on an SPE C18 column. After the derivatization the extract was cleaned
   up on an SPE Silica and then analyzed using HPLC with fluorescence
   :detector. The limit of detection (LOD) was below 1 mug/kg and
   recoveries above 70\%, with CV below 13\%, were achieved for all five
   compounds in a concentration range of 5-60 mug/kg.},
