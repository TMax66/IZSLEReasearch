@article{ WOS:000168423600031,
Author = {Brambilla, G and Buiarelli, F and Cartoni, GP and Coccioli, F and
   Colamonici, C and Fagiolo, A and Giannini, C and Neri, B},
Title = {Determination of flumethasone in calf urine and serum by liquid
