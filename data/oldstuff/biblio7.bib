@article{ WOS:000179929800031,
Author = {Gambinossi, F and Mecheri, B and Caminati, G and Nocentini, M and
   Puggelli, M and Gabrielli, G},
Title = {Antibiotic interaction with phospholipid monolayers},
Journal = {MATERIALS SCIENCE \& ENGINEERING C-BIOMIMETIC AND SUPRAMOLECULAR SYSTEMS},
Year = {2002},
Volume = {22},
Number = {2},
