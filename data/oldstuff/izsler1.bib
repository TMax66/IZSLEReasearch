@article{ ISI:000506894800066,
Author = {Chiapponi, Chiara and Faccini, Silvia and Fusaro, Alice and Moreno, Ana
   and Prosperi, Alice and Merenda, Marianna and Baioni, Laura and Gabbi,
   Valentina and Rosignoli, Carlo and Alborali, Giovanni L. and Cavicchio,
