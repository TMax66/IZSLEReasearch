Author = {Crotta, Matteo and Lavazza, Antonio and Mateus, Ana and Guitian, Javier},
Title = {{Quantitative risk assessment of hepatitis E virus: Modelling the
   occurrence of viraemic pigs and the presence of the virus in organs of
   food safety interest}},
