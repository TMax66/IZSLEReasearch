@article{ ISI:000504824000084,
Author = {Calonzi, Dario and Romano, Alicia and Monistero, Valentina and Moroni,
   Paolo and Luini, Mario Vittorio and Biscarini, Filippo and Castiglioni,
   Bianca and Cremonesi, Paola},
Title = {{Technical note: Development of multiplex PCR assays for the molecular
   characterization of Streptococcus uberis strains isolated from bovine
   mastitis}},
