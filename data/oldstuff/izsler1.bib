@article{ ISI:000408740200019,
Author = {Cirlini, Martina and Demuth, Teresa M. and Biancardi, Alberto and
   Rychlik, Michael and Dall'Asta, Chiara and Bruni, Renato},
Title = {{Are tropane alkaloids present in organic foods? Detection of scopolamine
   and atropine in organic buckwheat (Fagopyron esculentum L.) products by
   UHPLC-MS/MS}},
Journal = {{FOOD CHEMISTRY}},
Year = {{2018}},
Volume = {{239}},
Pages = {{141-147}},
Month = {{JAN 12}},
Abstract = {{A closer monitoring of tropane alkaloids (TA) in foods is now
   recommended by the European Commission, following a series of alerts
   related to the contamination of buckwheat with weeds of the genus
   Datura. A novel, accurate UHPLC-MS/MS method was developed and validated
   for the rapid detection of scopolamine and atropine in buckwheat foods.
   A suitable extraction protocol was set up to maximize recoveries and
   detection limits in different raw, processed and baked foods. The method
   offers good performances in terms of sensitivity, accuracy and
   precision, with LOQs at 0.04 and 0.10 mu g/kg. The established method is
   suitable for routine determination of trace levels of TA and was applied
   to 26 different buckwheat-derived organic foods, detecting TA in 3
   samples (13.9-83.9 mu g/kg for atropine and 5.7-10.4 mu g/kg for
   scopolamine). Only in one case the level of contamination was relevant
   in terms of food safety. (C) 2017 Elsevier Ltd. All rights reserved.}},
