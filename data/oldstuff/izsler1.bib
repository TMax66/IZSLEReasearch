@article{ ISI:000408560600006,
Author = {Percivalle, Elena and Sassera, Davide and Rovida, Francesca and Isernia,
   Paola and Fabbi, Massimo and Baldanti, Fausto and Marone, Piero},
Title = {{Usutu Virus Antibodies in Blood Donors and Healthy Forestry Workers in
   the Lombardy Region, Northern Italy}},
Journal = {{VECTOR-BORNE AND ZOONOTIC DISEASES}},
