@article{ ISI:000608109900003,
Author = {Cornacchia, Alessandra and Saletti, Maria Antonietta and Di Marzio,
   Violeta and Salini, Romolo and Marfoglia, Cristina and Tieri, Elga and
   D'Alterio, Nicola and Marri, Nicla and Losito, Francesca and Arienzo,
   Alyexandra and Murgia, Lorenza and Antonini, Giovanni and Amatiste,
   Simonetta and Leboffe, Loris and Pomilio, Francesco},
Title = {{Application of the Micro Biological Survey analytical method for the
   determination of bacterial load in cow raw milk}},
Journal = {{ITALIAN JOURNAL OF FOOD SAFETY}},
Year = {{2020}},
Volume = {{9}},
Number = {{3}},
Pages = {{146-149}},
Abstract = {{The aim of this study was to evaluate the performance of ``Micro
   Biological Survey - MBS Test{''} in the enumeration of bacterial load in
   cow raw milk. The MBS test is based on a colorimetric method recently
   developed and patented by ``Roma Tre{''} University, Italy. The
   evaluation of the performance of the MBS method was carried out by
   comparison with plate count at 30 degrees C (gold standard) and flow
   cytometry. Thirteen independent set of experiments were performed
   analyzing a total of 104 samples of cow raw milk with the selected
   methods. Results obtained using the MBS method are comparable with those
   obtained with the plate count method at 30 degrees C (CFU/mL) and flow
   cytometry technology; in particular, the results obtained with the MBS
   method are very close to plate count's at 30 degrees C. On the other
   hand, there are statistically significant differences between these two
   methods' and flow cytometry technology's results that could be due to
   the different experimental conditions.}},
