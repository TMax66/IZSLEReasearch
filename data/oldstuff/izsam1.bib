@article{ ISI:000426976800010,
Author = {Luciani, Mirella and Schirone, Maria and Portanti, Ottavio and Visciano,
   Pierina and Armillotta, Gisella and Tofalo, Rosanna and Suzzi, Giovanna
   and Sonsini, Luigia and Di Febo, Tiziana},
Title = {{Development of a rapid method for the detection of Yersinia
   enterocolitica serotype O:8 from food}},
Journal = {{FOOD MICROBIOLOGY}},
Year = {{2018}},
Volume = {{73}},
Pages = {{85-92}},
Month = {{AUG}},
Abstract = {{In this study, a new and alternative method based on monoclonal
   antibodies (MAbs) for the rapid detection of Yersinia enterocolitica O:8
   was developed. This microorganism is an emerging foodborne pathogen
   causing gastrointestinal disease in humans. The transmission can occur
   through contaminated food such as raw or undercooked meat, milk and
   dairy products, water and fresh vegetables. Nine MAbs (46F7, 54B11,
   54C11, 62D10, 64C7, 64C10, 72E8, 72E10, 72G6) were characterized and
   selected versus Y. enterocolitica O:8, and only 2 of them showed also a
   weak cross-reaction with Campylobacter jejuni. The MAb 54B11 was used
   for the development of Y. enterocolitica capture-ELISA in food matrices,
   i.e. meat and dairy products (n = 132). The method was validated by ISO
   16140:2003 and compared with the official method for the detection of
   presumptive pathogenic Y. enterocolitica (ISO 10273:2003). Relative
   accuracy, sensitivity and specificity corresponded to 100\%. The
   selectivity was evaluated on other food samples (n = 126) showing a
   lower confidence limit of 90.3\% and an upper confidence limit of 100\%.
   The results from this study demonstrated that the developed method was
   rapid and cheap, specific and sensitive for the screening of the
