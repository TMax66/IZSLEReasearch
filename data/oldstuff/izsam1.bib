@article{ ISI:000417724800014,
Author = {Caron, A. and Chiweshe, N. and Mundava, J. and Abolnik, C. and Dondona,
   A. Capobianco and Scacchia, M. and Gaidet, N.},
Title = {{Avian Viral Pathogens in Swallows, Zimbabwe Infectious Diseases in
   Hirundinidae: A Risk to Swallow?}},
Journal = {{ECOHEALTH}},
Year = {{2017}},
Volume = {{14}},
Number = {{4}},
Pages = {{805-809}},
Month = {{DEC}},
Abstract = {{We sampled 417 swallows in a wetland ecosystem of Zimbabwe in February
   2010 and October 2011. RT-PCR tests revealed circulation of avian
   paramyxovirus type I, avian influenza and West Nile disease viruses in
   these populations. We discuss the relevance of these findings in
   relation to what is known on the epidemiology of these viruses in these
   hosts and in relation to the host ecology. We conclude with
   recommendations to focus more research on Passeriformes in disease
   ecology and in particular on the hirundinidae family.}},
