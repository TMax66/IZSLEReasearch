@article{ WOS:000487930300027,
Author = {Dimitrov, Kiril M. and Abolnik, Celia and Afonso, Claudio L. and Albina,
   Emmanuel and Bahl, Justin and Berg, Mikael and Briand, Francois-Xavier
   and Brown, Ian H. and Choi, Kang-Seuk and Chvala, Ilya and Diel, Diego
