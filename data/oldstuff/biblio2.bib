@article{ WOS:000456688300033,
Author = {Di Francesco, Cristina E. and Profeta, Francesca and Romanucci,
   Mariarita and Zuccarini, Roberto and Altea, Tiziana and Malatesta,
   Daniela and Della Salda, Leonardo and Marsilio, Fulvio},
Title = {Evidence of avian poxvirus and papillomavirus infection in Gyps fulvus
   in Italy},
Journal = {ARCHIVES OF VIROLOGY},
Year = {2019},
Volume = {164},
Number = {1},
Pages = {291-295},
Month = {JAN},
Abstract = {The identification of avian poxvirus and avian papillomavirus associated
   with cutaneous lesions in griffon vultures (Gyps fulvus) by
   histopathology, electron microscopy and PCR analysis is reported.
   Sequence analysis of the fpv140 gene revealed 99\% identity to two
   poxviruses obtained from a white-tailed sea eagle (Haliaeetus albicilla)
   and a common buzzard (Buteo buteo). Partial sequence of the
   papillomavirus L1 gene showed sequence similarity to papillomavirus LI
   genes from African grey parrot (Psittacus erithacus) (69\% identity),
   duck (Anas platyrhynchos) (68\% identity), and yellow-necked francolin
   (Francolinus leucoscepus) (66\% identity). To date, this is the first
   identification of avian poxvirus and papillomavirus in griffon vultures
   and the first evidence of infection of both viruses in live wild birds.},
