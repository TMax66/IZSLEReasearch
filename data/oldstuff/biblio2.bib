@article{ WOS:000514374600010,
Author = {Gallo, Pasquale and Di Marco Pisciottano, Ilaria and Fattore, Margherita
   and Rimoli, Maria Grazia and Seccia, Serenella and Albrizio, Stefania},
Title = {A method to determine BPA, BPB, and BPF levels in fruit juices by liquid
   chromatography coupled to tandem mass spectrometry},
Journal = {FOOD ADDITIVES AND CONTAMINANTS PART A-CHEMISTRY ANALYSIS CONTROL
   EXPOSURE \& RISK ASSESSMENT},
Year = {2019},
Volume = {36},
Number = {12},
Pages = {1871-1881},
Abstract = {The monitoring of the contamination levels of bisphenol A (BPA) and its
   congeners bisphenol B (BPB) and bisphenol F (BPF) in foodstuffs is a
   necessary process for assessment of consumers' risk. After development
   and validation of a method using Strata (R) C18-E cartridge cleanup with
   detection by liquid chromatography coupled to tandem mass spectrometry,
   forty-six samples of fruit juices, sold on Italian markets, have been
   analysed to assess the concentration of BPA, BPB and BPF. BPB and BPF
   were not detected in any samples, BPA was found in 33 \% of the samples.
   The observed levels ranged from 0.50 ng mL(-1) to 2.85 ng mL(-1).
   Potential Daily Intakes (PDI) of BPA for Italian populations were
   calculated by the budget method model. PDIs ranged from 0.012 to 0.285
   mu g kg(-1) bw day(-1). None of the calculated values exceeded the
   current temporary TDI of 4 mu g kg(-1) bw day(-1).},
