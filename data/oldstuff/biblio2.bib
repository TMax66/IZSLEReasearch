@article{ WOS:000442518200005,
Author = {Rutigliano, Mariacinzia and Gagliardi, Rosa and Santillo, Antonella and
   Diurno, Valentina and Di Luccia, Aldo and la Gatta, Barbara},
Title = {Assessment of decreases in beta-casein during frozen storage of buffalo
   milk},
Journal = {INTERNATIONAL DAIRY JOURNAL},
Year = {2018},
Volume = {86},
Pages = {36-38},
Month = {NOV},
Abstract = {The use of fresh milk, delivered and processed within 60 h from the
   first milking is mandatory for production of Protected Designation of
   Origin buffalo mozzarella. However, the use of frozen milk is a common
   practice during the summer to cope with a greater market demand. The aim
   of this study was to detect qualitative and quantitative differences
   between fresh and frozen milk at the protein level. The fresh buffalo
   milk was aliquoted in eight batches, keeping them for up to 104 weeks at
   -20 degrees C. Fresh and frozen buffalo milk batches were analysed by
   the application of a sensitive ELISA test. The results showed a decrease
   in beta-casein levels during frozen storage, revealing enzymatic
   activity at -20 degrees C. Therefore, the ELISA assay could be a
   promising tool to safeguard the production of the typical local fresh
   cheeses through the control of raw milk before cheese manufacturing. (C)
   2018 Elsevier Ltd. All rights reserved.},
