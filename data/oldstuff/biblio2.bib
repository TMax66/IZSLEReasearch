@article{ WOS:000551257800029,
Author = {Carrieri, Raffaele and Borriello, Giorgia and Piccirillo, Giulio and
   Lahoz, Ernesto and Sorrentino, Roberto and Cermola, Michele and Censi,
   Sergio Bolletti and Grauso, Laura and Mangoni, Alfonso and Vinale,
