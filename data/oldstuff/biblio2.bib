@article{ WOS:000535184600003,
Author = {Tripaldi, C. and Rinaldi, S. and Palocci, G. and Di Giovanni, S. and
   Campagna, M. C. and Di Russo, C. and Zottola, T.},
Title = {CHEMICAL AND MICROBIOLOGICAL CHARACTERISTICS OF HOMOGENISED RICOTTA
   CHEESE PRODUCED FROM BUFFALO WHEY},
Journal = {ITALIAN JOURNAL OF FOOD SCIENCE},
Year = {2020},
Volume = {32},
Number = {2},
Pages = {292-309},
Abstract = {To extend the shelf life of buffalo Ricotta cheese, a process was
   assessed that included a second heat treatment followed by
   homogenisation and hot packaging. The microbiological and chemical
   characteristics as well as the oxidation degree of the product were
   determined over storage for 21 days using total antioxidant activity,
   redox potential, malondialdehyde content, and protein-bound carbonyl
   content. Homogenised buffalo Ricotta cheese has a longer shelf life than
   traditional Ricotta cheese, although the process could be optimised to
   reduce the total bacterial load during storage. No significant oxidative
   damage occurred during storage. This innovative process could promote
   the market expansion of Ricotta cheese.},
