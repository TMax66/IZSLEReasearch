@article{ WOS:000556807500001,
Author = {Gazzi, Arianna and Fusco, Laura and Orecchioni, Marco and Ferrari,
   Silvia and Franzoni, Giulia and Yan, J. Stephen and Rieckher, Matthias
   and Peng, Guotao and Lucherelli, Matteo Andrea and Anna Vacchi, Isabella
