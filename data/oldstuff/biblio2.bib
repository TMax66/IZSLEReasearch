@article{ WOS:000475474000007,
Author = {Pecoraro, Roberta and Salvaggio, Antonio and Scalisi, Elena M. and
   Iaria, Carmelo and Lanteri, Giovanni and Copat, Chiara and Ferrante,
   Margherita and Fragala, Giuseppe and Zimbone, Massimo and Impellizzeri,
   Giuliana and Brundo, Maria V.},
Title = {Evaluation of the effects of silver nanoparticles on Danio rerio cornea:
   Morphological and ultrastructural analysis},
Journal = {MICROSCOPY RESEARCH AND TECHNIQUE},
Year = {2019},
Volume = {82},
Number = {8},
Pages = {1297-1301},
Month = {AUG},
Abstract = {Although in recent years there has been intensification in the use of
   nanoparticles (NPs) for the production of many commercial products, few
   studies have been carried out to assess the risks associated with its
   use. Among the most used NPs, silver nanoparticles (AgNPs) have a
