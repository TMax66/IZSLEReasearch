@article{ ISI:000319166100002,
Author = {Marini, Francesca and Mutinelli, Franco and Montarsi, Fabrizio and
   Cline, Andrew Richard and Gatti, Enzo and Audisio, Paolo},
Title = {{First report in Italy of the dusky sap beetle, Carpophilus lugubris, a
   new potential pest for Europe}},
Journal = {{JOURNAL OF PEST SCIENCE}},
Year = {{2013}},
Volume = {{86}},
Number = {{2}},
Pages = {{157-160}},
Month = {{JUN}},
Abstract = {{Following a report submitted by a beekeeper to Istituto Zooprofilattico
   Sperimentale delle Venezie, in Padua province (Borgoricco, Veneto, NE
   Italy), the authors discovered in April 2011 several specimens of the
   dusky sap beetle Carpophilus lugubris Murray 1864 (Coleoptera,
   Nitidulidae) on the bottom board of some beehives. At a later time (May
   2012), an isolated specimen was also collected within a Natural Reserve
   in Belluno province (Feltre, Veneto, NE Italy). These specimens
   represent the first known records of this species in Italy or Europe as
   a whole. This is also the first record of the species associated with
   beehives. This discovery led us to review the available bionomical and
   faunistic data for this widespread Nearctic and Neotropical species. C.
   lugubris is a recognized pest of corn, therefore is very important
   monitoring its possible dispersal into cornfields, beehives, and other
   agricultural regions of NE Italy and southern Europe over the next few
   years.}},
