@article{ ISI:000336524100021,
Author = {Skarin, Hanna and Aberg, Annica Tevell and Woudstra, Cedric and Hansen,
   Trine and Lofstrom, Charlotta and Koene, Miriam and Bano, Luca and
   Hedeland, Mikael and Anniballi, Fabrizio and De Medici, Dario and
   Engvall, Eva Olsson},
Title = {{THE WORKSHOP ON ANIMAL BOTULISM IN EUROPE}},
