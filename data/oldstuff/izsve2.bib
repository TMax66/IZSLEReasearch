@article{ ISI:000350558400003,
Author = {Mongillo, Paolo and Pitteri, Elisa and Adamelli, Serena and Bonichini,
   Sabrina and Farina, Luca and Marinelli, Lieta},
Title = {{Validation of a selection protocol of dogs involved in animal-assisted
   intervention}},
Journal = {{JOURNAL OF VETERINARY BEHAVIOR-CLINICAL APPLICATIONS AND RESEARCH}},
