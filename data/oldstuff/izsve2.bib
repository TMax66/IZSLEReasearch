@article{ ISI:000340074000003,
Author = {Pretto, T. and Zambon, M. and Civettini, M. and Caburlotto, G. and
   Boffo, L. and Rossetti, E. and Arcangeli, G.},
Title = {{Massive mortality in Manila clams (Ruditapes philippinarum) farmed in
   the Lagoon of Venice, caused by Perkinsus olseni}},
Journal = {{BULLETIN OF THE EUROPEAN ASSOCIATION OF FISH PATHOLOGISTS}},
Year = {{2014}},
Volume = {{34}},
Number = {{2}},
Pages = {{43-53}},
Abstract = {{Perkinsosis is a category of mollusc diseases caused by protozoan
   parasites belonging to the genus Perkinsus. Heavy infection with
   Perkinsus olseni often results in tissue inflammation, reduced growth
   and impaired reproduction. The lagoons of the Veneto Region, Italian
   leading producer of Manila clams, to date have never experienced events
   of mass mortality related to P. olseni, since the first detection of the
   pathogen in 1991. Our study describes a severe mortality in Manila clam
   in the autumn of 2011 in the southern part of the Lagoon of Venice,
   where extensive lesions caused by P. olseni were observed.}},
