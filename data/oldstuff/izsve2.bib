@article{ ISI:000358734000003,
Author = {Mancin, Marzia and Toson, Marica and Grimaldi, Maria and Barco, Lisa and
   Trevisan, Romina and Carnieletto, Paola and Ricci, Antonia},
Title = {{Application of bootstrap method to evaluate bimodal data: an example of
   food microbiology proficiency test for sulfite-reducing anaerobes}},
Journal = {{ACCREDITATION AND QUALITY ASSURANCE}},
Year = {{2015}},
Volume = {{20}},
Number = {{4}},
