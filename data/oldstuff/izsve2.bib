@inproceedings{ ISI:000366835107060,
Author = {Mantovani, C. and Mascarello, G. and Pinto, A. and D'Este, F. and
   Gallina, A. and Ravarotto, L.},
Editor = {{Chova, LG and Martinez, AL and Torres, IC}},
Title = {{BRINGING SCIENTIFIC RESEARCH INTO THE CLASSROOM}},
