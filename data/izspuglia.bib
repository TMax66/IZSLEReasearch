@article{ ISI:000294795600024,
Author = {Cassano, Tommaso and Romano, Adele and Macheda, Teresa and Colangeli,
   Roberto and Cimmino, Concetta Stefania and Petrella, Antonio and
   LaFerla, Frank M. and Cuomo, Vincenzo and Gaetani, Silvana},
Title = {{Olfactory memory is impaired in a triple transgenic model of Alzheimer
   disease}},
Journal = {{BEHAVIOURAL BRAIN RESEARCH}},
Year = {{2011}},
Volume = {{224}},
Number = {{2}},
Pages = {{408-412}},
Month = {{OCT 31}},
Abstract = {{Olfactory memory dysfunctions were investigated in the triple-transgenic
   murine model of Alzheimer's disease (3x Tg-AD). In the social
   transmission of food preference test, 3x Tg-AD mice presented severe
   deficits in odor-based memory, without gross changes in general
   odor-ability. A beta and tau immunoreactivity was not observed in the
   primary processing regions for odor, the olfactory bulbs (OBs), whereas
   marked immunostaining was present in the piriform, entorhinal, and
   orbitofrontal cortex, as well as in the hippocampus. Our results suggest
   that the impairment in olfactory-based information processing might
   arise from degenerative mechanisms mostly affecting higher cortical
   regions and limbic areas, such as the hippocampus. (C) 2011 Elsevier
   B.V. All rights reserved.}},
