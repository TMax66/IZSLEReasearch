@article{ ISI:000471270000003,
Author = {Nicastri, Emanuele and Vairo, Francesco and Mencarini, Paola and
   Battisti, Antonio and Agrati, Chlara and Cimini, Eleonora and Carrara,
   Stefania and D'Arezzo, Silvia and Adone, Rosanna and Vulcano, Antonella
   and Iannetta, Marco and Capone, Alessandro and Petrosillo, Nicola and
   Fasanella, Antonio and Ippolito, Giuseppe and Di Caro, Antonino and
