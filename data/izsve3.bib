@article{ ISI:000207458500015,
Author = {Genovesi, Sacha and Leita, Liviana and Sequi, Paolo and Andrighetto,
   Igino and Sorgato, M. Catia and Bertoli, Alessandro},
Title = {{Direct Detection of Soil-Bound Prions}},
