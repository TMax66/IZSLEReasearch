@article{ ISI:000271573400009,
Author = {Cafarchia, Claudia and Otranto, Domenico and Weigl, Stefania and
   Campbell, Bronwyn E. and Parisi, Antonio and Cantacessi, Cinzia and
   Mancianti, Francesca and Danesis, Patrizia and Gasser, Robin B.},
Title = {{Molecular characterization of selected dermatophytes and their
   identification by electrophoretic mutation scanning}},
