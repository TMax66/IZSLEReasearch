@article{ ISI:000284426300004,
Author = {Soglia, Dominga and Rossi, Luca and Cauvin, Elsa and Citterio, Carlo and
   Ferroglio, Ezio and Maione, Sandra and Meneguz, Pier Giuseppe and
   Spalenza, Veronica and Rasero, Roberto and Sacchi, Paola},
Title = {{Population genetic structure of Alpine chamois (Rupicapra r. rupicapra)
   in the Italian Alps}},
Journal = {{EUROPEAN JOURNAL OF WILDLIFE RESEARCH}},
Year = {{2010}},
Volume = {{56}},
Number = {{6}},
Pages = {{845-854}},
Month = {{DEC}},
Abstract = {{Analysis of the genetic diversity of the Alpine chamois in Italy was
   conducted using a pool of 26 microsatellite loci. A total of 209 animals
   were analyzed, representing six geographical populations from different
   location of the Southern slope of the Alps. Clear genetic differences
   have emerged between the sampled chamois groups. Some were consistent
   with an isolation-by-distance model. However, in parallel, other
   mechanisms intervened in areas that, in addition to being peripheral to
   the main alpine ridge, had suffered from recent bottlenecks. In such
   areas, genetic drift and a low rate of gene flow are likely explanations
   for the current genetic structure.}},
