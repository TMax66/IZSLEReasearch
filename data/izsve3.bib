@article{ ISI:000276455300003,
Author = {Capua, Ilaria and Alexander, Dennis},
Title = {{Perspectives on the Global Threat: The Challenge of Avian Influenza
   Viruses for the World's Veterinary Community}},
Journal = {{AVIAN DISEASES}},
Year = {{2010}},
Volume = {{54}},
Number = {{1, S}},
