@article{ ISI:000289031600033,
Author = {Monini, M. and Biasin, A. and Valentini, S. and Cattoli, G. and Ruggeri,
   F. M.},
Title = {{Recurrent rotavirus diarrhoea outbreaks in a stud farm, in Italy}},
Journal = {{VETERINARY MICROBIOLOGY}},
Year = {{2011}},
Volume = {{149}},
Number = {{1-2}},
Pages = {{248-253}},
Month = {{APR 21}},
Abstract = {{A total of 47 stool samples were collected at the same stud farm from
   young foals with rotavirus diarrhoea and from their stud mares. Illness
   involved foals during three consecutive winter seasons. Infection in the
   farm appeared firstly in January-February 2008. After vanishing in the
   warm seasons, cases reappeared in March 2009 and 2010. Determination of
   the rotavirus G- and P-types was carried out using nested RT-PCR in
   samples collected in 2009 and 2010. A total of 19 of 47 samples resulted
   positive for rotavirus. The G type was determined in 19/47 samples,
   whereas the P genotype was determined in 17/47 samples. All equine
   strains presented a G14 VP7 in combination with a P{[}12]VP4, suggesting
   persistence of the same viral strain in the stud farm, during at least
   two consecutive winter periods. Sequence analysis of the genes encoding
   the outer capsid rotavirus proteins VP7 and VP4 revealed that the virus
   had a close relationship between strains recently isolated in the rest
   of Europe. (C) 2010 Elsevier B.V. All rights reserved.}},
