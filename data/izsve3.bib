@article{ ISI:000295986900001,
Author = {Bano, L. and Bonci, M. and Drigo, I. and Tonon, E. and Mazzolini, E. and
   Carminato, A. and Granato, A. and Ceglie, L. and Natale, A. and Zanette,
   G. and Agnoletti, F.},
Title = {{Recurrent detection of Histophilus somni in the genital tract of dairy
   cattle with reproductive failures in Italy}},
Journal = {{LARGE ANIMAL REVIEW}},
Year = {{2011}},
Volume = {{17}},
Number = {{5}},
Pages = {{171-176}},
Month = {{OCT}},
Abstract = {{Histophilus somni is able to infect the genital tract of dairy cattle
   and to cause vaginitis, embryonic death and abortion. This paper
   describes two outbreaks characterized by reproductive disorders in dairy
   cattle associated with genital infections caused by H. somni. In the
   first case-report the presence of infectious agents responsible for
   abortion, reproductive failure or vaginal discharge has been
   investigated by means of bacteriological, serological and biomolecular
   techniques. Among the infectious agents investigated a significant
   association (P<0.001) between the positivity for H. somni and reduced
   fertility was observed, with higher prevalence of vaginal discharge and
   poor conception rate among infected animals.
   The second case-report describes the effectiveness of the therapeutic
   approach adopted by the veterinary, based on the execution of vaginal
   washings with an antibiotic solution containing gentamicin, lincomycin
   and spectinomycin, in cows with a muco-purulent vaginal discharge and
   the worsening of the reproductive performance due to H. somni
   infections.
   These results suggest to consider H. somni as a possible cause of
   reproductive failures in dairy cattle.}},
