@article{ ISI:000188168700015,
Author = {Montesissa, C and Capolongo, F and Santi, A and Biancotto, G and
   Dacasto, M},
Title = {{Metabolism of tilmicosin by rabbit liver microsomes and hepatocytes}},
Journal = {{VETERINARY JOURNAL}},
