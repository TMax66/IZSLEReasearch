@article{ ISI:000271613600019,
Author = {Brutti, Andrea and Rovere, Pierpaolo and Cavallero, Serena and D'Amelio,
   Stefano and Danesi, Patrizia and Arcangeli, Giuseppe},
Title = {{Inactivation of Anisakis simplex larvae in raw fish using high
   hydrostatic pressure treatments}},
Journal = {{FOOD CONTROL}},
Year = {{2010}},
Volume = {{21}},
Number = {{3}},
Pages = {{331-333}},
Month = {{MAR}},
Abstract = {{The nematode larvae of the Anisakis genus are one of the main health
   hazards for consumers of raw fish or similar, such as ``carpaccio{''},
   and cold smoked or marinated products.
   This study evaluated the efficacy of high hydrostatic pressure
   treatments on Anisakis larvae in mackerel (Scomber scombrus) from the
   North Sea. From the treatment parameters studied, a combination of 300
   MPa for 5 min resulted as being capable of inactivating 100\% of the
   larvae present in the tissues of the fish treated. Biomolecular
   examination confirmed their main belonging to the species Anisakis
   simplex sensu stricto.
   The use of this technology is for the treatment of other fatty fish such
   as sardines and anchovies prior to marinating. (C) 2009 Elsevier Ltd.
   All rights reserved.}},
