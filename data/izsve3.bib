Editor = {{Chova, LG and Belenguer, DM and Torres, IC}},
Title = {{``ALIMENTINSALUTE{''}: A SOCIAL MARKETING PROJECT FOR PUBLIC HEALTH
   COMMUNICATION AND EDUCATION ON FOOD SAFETY AND NUTRITION IN VENETO
   REGION}},
Booktitle = {{3RD INTERNATIONAL CONFERENCE OF EDUCATION, RESEARCH AND INNOVATION
   (ICERI2010)}},
Year = {{2010}},
