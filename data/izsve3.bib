@article{ ISI:000283782000007,
Author = {Cerutti, M. C. and Citterio, C. V. and Bazzocchi, C. and Epis, S. and
   D'Amelio, S. and Ferrari, N. and Lanfranchi, P.},
Title = {{Genetic variability of Haemonchus contortus (Nematoda:
   Trichostrongyloidea) in alpine ruminant host species}},
Journal = {{JOURNAL OF HELMINTHOLOGY}},
Year = {{2010}},
Volume = {{84}},
Number = {{3}},
Pages = {{276-283}},
Month = {{SEP}},
Abstract = {{Genetic variability of the ovine parasite Haemonchus contortus from the
   Alpine area was investigated using mitochondrial DNA (nd4 gene),
   internal transcribed spacers 1 and 2 and microsatellites, in order to
   assess whether cross-transmission between domestic and wild ruminants
   occurs. The dataset was composed of 78 individual adult male H.
   contortus collected from chamois (Rupicapra r. rupicapra), roe deer
   (Capreolus capreolus), alpine ibex (Capra ibex ibex), domestic goat
   (Capra hircus) and sheep (Ovis aries) from different alpine areas. The
   data obtained show low host specificity and high genetic variation
   within H. contortus populations. The analyses indicate the presence of
   two mitochondrial haplotype clusters among host species and the absence
   of cryptic parasite species, confirming H. contortus as a generalist
   nematode and suggesting that parasite transmission between populations
   of domestic and wild ruminants normally occurs.}},
