@article{ ISI:000234681100015,
Author = {De Nardi, R and Terregino, C and Cattoli, G and Toffan, A and Beato, MS
   and Guberti, V and Scremin, M},
Title = {{Preliminary results of an influenza surveillance in wild birds, game
   birds, domestic ducks and geese in North Eastern Italy}},
Journal = {{ITALIAN JOURNAL OF ANIMAL SCIENCE}},
Year = {{2005}},
Volume = {{4}},
Number = {{3}},
Pages = {{292-295}},
Month = {{JUL-SEP}},
Note = {{43rd SIPA Congress, Forli, ITALY, OCT 07-08, 2004}},
Organization = {{SIPA}},
Abstract = {{Following the avian influenza (AI) epidemics which occurred in Italy
   between 1997 and 2003, a surveillance program, funded by the Italian
   Ministry of Health was implemented. Among the tasks of this surveillance
   program was an investigation on wild and domestic birds to asses
   circulation of avian influenza viruses in their natural reservoirs. In
   this study we collected samples from migratory wild birds (Anseriformes
   and Charadriiformes), from national and importated game fowls, and from
   7 backyard farms of geese and ducks. Cloacal swabs were screened by
   means of real-time RT-PCR (RRT-PCR) and/or directly processed for
   attempted virus isolation in embryonated fowl's SPF eggs and blood
   samples for presence of antibodies against avian influenza viruses.
   Avian influenza viruses were only obtained from migratory waterfowls
   belonging to the family Anseriformes, and not from domestic waterfowls
   or game birds. This study confirms that the risk of introduction of
   novel influenza viruses in densely populated areas of poultry farms in
   Veneto is linked to migratory wild birds and in particular from birds
   belonging to the family Anseriformes.}},
