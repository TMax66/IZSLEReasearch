@article{ ISI:000273005300012,
Author = {Terregino, Calogero and Beato, Maria Serena and Bertoli, Elena and
   Mancin, Marzia and Capua, Ilaria},
Title = {{Unexpected heat resistance of Italian low-pathogenicity and
