@article{ WOS:000734876300001,
Author = {Cuomo, Paola and Medaglia, Chiara and Allocca, Ivana and Montone, Angela
   Michela Immacolata and Guerra, Fabrizia and Cabaro, Serena and Mollo,
   Ernesto and Eletto, Daniela and Papaianni, Marina and Capparelli,
   Rosanna},
Title = {Caulerpin Mitigates Helicobacter pylori-Induced Inflammation via Formyl
   Peptide Receptors},
Journal = {INTERNATIONAL JOURNAL OF MOLECULAR SCIENCES},
Year = {2021},
Volume = {22},
Number = {23},
Month = {DEC},
Abstract = {The identification of novel strategies to control Helicobacter pylori
   (Hp)-associated chronic inflammation is, at present, a considerable
   challenge. Here, we attempt to combat this issue by modulating the
   innate immune response, targeting formyl peptide receptors (FPRs),
   G-protein coupled receptors that play key roles in both the regulation
   and the resolution of the innate inflammatory response. Specifically, we
   investigated, in vitro, whether Caulerpin-a bis-indole alkaloid isolated
   from algae of the genus Caulerpa-could act as a molecular antagonist
   scaffold of FPRs. We showed that Caulerpin significantly reduces the
   immune response against Hp culture filtrate, by reverting the
   FPR2-related signaling cascade and thus counteracting the inflammatory
   reaction triggered by Hp peptide Hp(2-20). Our study suggests Caulerpin
   to be a promising therapeutic or adjuvant agent for the attenuation of
   inflammation triggered by Hp infection, as well as its related adverse
   clinical outcomes.},
