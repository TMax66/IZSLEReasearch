@article{ WOS:000446367300002,
Author = {Santoro, M. and Di Nocera, F. and Iaccarino, D. and Cipriani, P. and
   Procesi, I. Guadano and Maffucci, F. and Hochscheid, S. and Blanco, C.
   and Cerrone, A. and Galiero, G. and Nascetti, G. and Mattiucci, S.},
Title = {Helminth parasites of the dwarf sperm whale Kogia sima (Cetacea:
   Kogiidae) from the Mediterranean Sea, with implications on host ecology},
Journal = {DISEASES OF AQUATIC ORGANISMS},
