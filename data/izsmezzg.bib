@article{ WOS:000674370700001,
Author = {Matteo, Ferro and Lucarelli, Giuseppe and de Cobelli, Ottavio and Dolce,
   Pasquale and Terracciano, Daniela and Musi, Gennaro and Porreca, Angelo
   and Busetto, Gian Maria and Del Giudice, Francesco and Soria, Francesco
