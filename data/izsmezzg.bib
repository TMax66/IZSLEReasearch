@article{ WOS:000701107700001,
Author = {Paradiso, Rubina and Riccardi, Marita Georgia and Cecere, Bianca and
   Riccone, Nunzia and Scialla, Roberto and Anzalone, Antonietta and
   Cerrone, Anna and De Carlo, Esterina and Borriello, Giorgia and Fusco,
